	<select id="getHeadMessageOfCompany" resultMap="AllResultMap" parameterType="integer">
		select
			c.id,
			c.company_name,
			c.phone,
			p.pic_path,
			c.pid
		from base_company c
		left join res_pic p on c.company_logo = p.id
		where c.id = #{companyId}
	</select>

    <select id="getHeadMessageOfShop" resultType="com.nork.product.model.CompanyShop" parameterType="integer">
        select
			s.id            as id,
			s.shop_name     as shopName,
			p.pic_path      as shopLogoPath
		from company_shop s
		left join res_pic p on s.logo_pic_id = p.id
		where s.id = #{shopId}
    </select>

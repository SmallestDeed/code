@
-ms-viewport {
	width: device-width;
}

body, div, ul, h1, h2, h3, h4, h5, p, dl, dd, dt, form, input, option,
	br, option {
	margin: 0;
	padding: 0;
}

body, div, button, input, select, textarea {
	font-family: "寰蒋闆呴粦", arial;
}

html {
	overflow: hidden;
}

html {
	height: 100%;
}

body {
	height: 100%;
	overflow: hidden;
	margin: 0;
	padding: 0;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
	color: #FFFFFF;
	background-color: #000000;
}

ul, li {
	margin: 0;
	padding: 0;
	list-style: none;
}

.clear {
	clear: both;
}


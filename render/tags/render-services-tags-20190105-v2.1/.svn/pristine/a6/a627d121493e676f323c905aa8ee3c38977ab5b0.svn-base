package com.nork.pano.model.scene;


import java.io.Serializable;

/**
 * 同城联盟720单点分享场景信息
 * Created by chenm on 2018/1/30.
 */
public class UnionStoreSingleSenceInfoVo implements Serializable{

  private static final long serialVersionUID = 1L;
	
    /** 场景图片id **/
	private Integer picId;
	
	/** 原方案名称 **/
	private String designPlanName;

	/** 方案缩略图地址**/
	private String planThumbnailUrl;

    public Integer getPicId() {
      return picId;
    }
  
    public void setPicId(Integer picId) {
      this.picId = picId;
    }
  
    public String getDesignPlanName() {
      return designPlanName;
    }
  
    public void setDesignPlanName(String designPlanName) {
      this.designPlanName = designPlanName;
    }
  
    public String getPlanThumbnailUrl() {
      return planThumbnailUrl;
    }
  
    public void setPlanThumbnailUrl(String planThumbnailUrl) {
      this.planThumbnailUrl = planThumbnailUrl;
    }
	
	
	
}

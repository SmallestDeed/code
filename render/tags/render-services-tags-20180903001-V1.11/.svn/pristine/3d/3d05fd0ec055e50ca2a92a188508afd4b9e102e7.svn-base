	<!--根据条件获取样板房产品列表 -->
	<select id="byTempletIdProductList" resultMap="AllResultMap2"
			parameterType="com.nork.design.model.DesignTempletProduct">

		SELECT dtp.product_id,dtp.is_main_product,bp.product_code,dtp.pos_name,dtp.product_group_id,sp.structure_code,
		a.valuekey as productTypeCode,b.valuekey as productSmallTypeCode,dtp.region_mark,dtp.center,dtp.style_id,
		dtp.measure_code,dtp.is_standard
		FROM design_templet_product dtp,base_product bp,structure_product sp,sys_dictionary a ,sys_dictionary b
		where dtp.product_id = bp.id AND dtp.product_group_id = sp.id AND dtp.is_deleted = 0
		AND  a.type ='productType'
		AND a.value = bp.product_type_value
		AND b.type = a.valuekey
		AND b.value = bp.product_small_type_value
		<if test="designTempletId !=  null  "> and dtp.design_templet_id  = #{designTempletId,jdbcType=INTEGER} </if>
		<if test="groupType !=  null  "> and dtp.group_type  = #{groupType,jdbcType=INTEGER} </if>
		<if test="groupType == 1 "> and sp.structure_type != 'BJQ' </if>
	</select>



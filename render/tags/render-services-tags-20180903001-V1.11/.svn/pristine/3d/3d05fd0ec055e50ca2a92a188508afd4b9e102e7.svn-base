				and is_deleted = 0
	</update>
	
	<!-- getListByTempletId -->
	<select id = "getListByTempletId" resultMap="AllResultMap">
		select
		dtp.product_id,
		bp.product_code,
		bp.product_length,
		bp.product_height,
		bp.product_width,
		bp.full_pave_length,
		dtp.wall_orientation,
		dtp.pos_name,
		dtp.region_mark,
		dtp.measure_code,
		dtp.wall_type,
		dtp.is_main_product,
		sdbig.valuekey as big_type_valuekey,
		sdsmall.valuekey as small_type_valuekey,
		dtp.center,
		dtp.product_group_id,
		dtp.plan_group_id,
		dtp.group_type,
		dtp.id,
		dtp.bind_parent_productId,
		dtp.center,
		dtp.is_main_structure_product,
		dtp.is_group_replace_way,
		dtp.product_index,
		bp.product_smallpox_identify_str
		from design_templet_product dtp
		left join base_product bp on bp.id = dtp.product_id
		left join sys_dictionary sdbig on sdbig.type = "productType" and sdbig.value = bp.product_type_value
		left join sys_dictionary sdsmall on sdsmall.type = sdbig.valuekey and sdsmall.value = bp.product_small_type_value
		where dtp.design_templet_id = #{designTempletId} and dtp.is_deleted = 0
	</select>
	

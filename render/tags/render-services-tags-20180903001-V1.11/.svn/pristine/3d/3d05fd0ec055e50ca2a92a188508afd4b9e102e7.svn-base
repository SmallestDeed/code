	<!--根据条件获取样板房结构产品列表 -->
	<select id="getStructureProductByDesignId" resultMap="AllResultMap">
		SELECT dtp.*,sp.structure_type,sp.structure_code FROM design_templet_product dtp
		RIGHT JOIN structure_product sp ON sp.id = dtp.product_group_id AND sp.is_deleted = 0
		WHERE dtp.design_templet_id = #{designTempletId,jdbcType=INTEGER} AND dtp.is_deleted = 0
		AND dtp.group_type  = 1 and sp.structure_type != 'BJQ' GROUP BY product_group_id
	</select>


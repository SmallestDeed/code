<view class="type-box">
  <view class="title">选择发布类别</view>
  <view class="type-item" wx:for="{{typeList}}" wx:key="{{index}}" data-id="{{item.id}}" bindtap='toDetail'>
      <view class="type-item-img">
        <image src="{{item.imgUrl}}"></image>
      </view>
      <view class="item-info">
        <view class="item-title">{{item.title}}</view>
        <view class="item-type">{{item.content}}</view>
      </view>
  </view>
</view>


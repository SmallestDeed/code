/* pages/house-type/house-type.wxss */
page {
    display: block;
    min-height: 100%;
    background-color: #f5f5f5;
}
.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    background: rgb(255, 255, 255)
}
.type-header .left,
.type-header .right {
    font-weight: 600;
    text-align: center;
    color: #313131;
    font-size: 36rpx;
    line-height: 88rpx;
}
.cut-off {
    color: #ddd;
    float: right;
    width: 1px;
    background: #ddd;
    margin-top: 28rpx;
    height: 36rpx;
}
.type-search {
    overflow: hidden;
    margin-top: 20rpx;
}
.list {
    width: 100%;
    margin-top: 549rpx;
    font-size: 28rpx;
    color: "#333333";
    background: #fff;
}
.list.spaceIteam{
    margin-top: 0rpx;
}
.list-iteam,
.list-iteam-active {
    padding: 31rpx 0;
    width: 100%;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1px solid #ddd;
}
.list-iteam:last-child {
    border-bottom: none;
}
.iteam_right {
    margin-right: 31rpx;
    text-align: right;
    color: #8e8e8e;
    position: relative;
}
.flex_iteam,
.iteam_left {
    margin-left: 31rpx;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
/*三级联动样式*/
/**app.wxss**/
.label {
    padding-left: 31rpx;
    padding-right: 26rpx;
    width: 120rpx;
}
.container {
    position: absolute;
    height: 100%;
    width: 100%;
    min-height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 0;
    box-sizing: border-box;
}
.selectBody {
    width: 100%;
    font-size: 28rpx;
    color: #999999;
    overflow: hidden;
}
.selectContext{
background: #fff;
}
input {
    color: #333333;
}
.citypickers {
    position: fixed;
    height: 100%;
    width: 100%;
    min-height: 100%;
    background-color: red;
}
.citypicker {
    font-size: 34rpx;
}
.citybody {
    background:#fff;
    position: fixed;
    bottom: 0px;
    z-index: 2;
}
.maskLayer{
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1;
    background: rgba(0,0,0,.5);
}
.cityheader {
    position: absolute;
    top: 0px;
    width: 100%;
    z-index: 4;
    font-size: 34rpx;
}
.city-cancel {
    float: left;
    margin: 20rpx;
    color: #818181;
    font-size: 34rpx;
}
.city-true {
    float: right;
    margin: 20rpx;
    color: #ff6419;
    font-size: 34rpx;
}
.section .picker {
    background-color: #fff;
    border-bottom: 1px #d9d9d9 solid;
    border-top: 1px #d9d9d9 solid;
    padding: 20rpx;
}
.cell_select {
    padding: 30rpx 0;
    width: 100%;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.cell_select.frist {
    border-bottom: 1px solid #ddd;
}
.section {
    padding-top: 20rpx;
}
.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}
.userinfo-nickname {
    color: #aaa;
}
.usermotto {
    margin-top: 200px;
}
.type-button {
    margin-top:80rpx;
    padding: 0 35rpx;
    height: 80rpx;
}
.btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    width: 680rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    text-align: center;
    text-decoration: none;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 39rpx;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000000;
    background-color: #F8F8F8;
}
button[type=primary] {
    color: #FFFFFF;
    background-color: #ff6419;
}
.flex_iteam.active {
    color: #ff6419;
}
.type-search-space {
    width: 100%;
    background: #fff;
}
.space-type {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    height: 89rpx;
}
.space-gird{
    width: 100%;
}
.space-gird-scroll{
    max-height: 320rpx;
}
.square .space-gird-scroll{
    max-height: 530rpx;
}
.space-type-iteam {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    font-size:28rpx;
    text-align: center;
    height: 89rpx;
    line-height: 89rpx;
}
.gird-view-iteam{
    float: left;
    margin-left:27rpx;
    margin-bottom: 20rpx;
    width: 216rpx;
    height: 80rpx;
    border-radius: 5rpx;
    background: #f5f5f5;
    text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;
}
.space-type-iteam.active{
    color: #ff6419;
}


.main .con .product-list .product-iteam{
    width:710rpx;
    margin:21rpx 20rpx;
    height: 560rpx;
    border-radius:10rpx;
}
.product-image{
    width: 100%;
    height: 420rpx;
    overflow: hidden;
    position: relative;
}
.product-image .productImage{
    width: 100%;
}
.iconIMG0,.iconIMG1,.iconIMG2{
    width: 70rpx;
    height: 70rpx;
    position: absolute;
    /* z-index: 1; */
    bottom: 35rpx;
}
.iconIMG0 image,.iconIMG1 image,.iconIMG2 image {
    width: 70rpx;
    height: 70rpx;
}
.iconIMG0,.iconIMG1,.iconIMG2{
    left: 30rpx;
}
/* .iconIMG1{
    left: 130rpx;
}
.iconIMG2{
    left: 230rpx;
} */
.product-info{
    padding:34rpx 29rpx 5rpx 29rpx;
    background:#ffffff;
    overflow: hidden;
}
.product-info-left{
    line-height: 32rpx;
    float: left;
    font-size:32rpx;
    color: #333333
}
.product-info-right{
    position: relative;
    line-height: 32rpx;
    float: right;
    font-size: 24rpx;
    color: #999
}
.product-info-bottom{
    font-size: 26rpx;
    color: #999;
    margin-top:10rpx
}
.iconIMG3,.iconIMG4{
    margin-left:2px;
    width: 32rpx;
    height: 32rpx;
    vertical-align: -3rpx;

}
.good{
    margin-left:7rpx;
}
.ellipsis{
  height: 48rpx;
  width: 48rpx;
}
.black{
  height: 50rpx;
  width: 140rpx;
  background-color: #4d4d4d;
  border-radius: 5rpx;
  margin-right: 5rpx;
  display: flex;
  flex-direction: row
}
.none{
  display: none
}
.deleteIcon{
  height: 28rpx;
  width: 28rpx;
  padding: 11rpx 11rpx 11rpx 20rpx
}
.deletetxt{
  line-height: 50rpx;
  font-size: 28rpx;
  color: #ffffff;
  margin-left: 5rpx;
}
.img{
  width: 220rpx;
  height: 183rpx;
}


.empty_tip{
  position: fixed;
  height: 100%;
  width: 100%;
  display:flex;
  flex-direction:column;
  align-items:center;/*垂直居中*/
  justify-content: center;/*水平居中*/
  margin-top:-100rpx;
  top:0;

}
.empty_tip .image{
  width: 403rpx;
	height: 270rpx;
}
.empty_tip .txt{
  font-size: 28rpx;
  color: #999999;
  margin-top:40rpx;
}

/* 渲染模块增加 */
button {
  background-color: transparent !important;
  border: none;
  border-radius: 0rpx !important;
	color: #ff6419 !important;
  font-size: 32rpx;
  outline: none;
  padding:0 !important;
}
button:after{
  border: none;
  outline: none;
}
<view class="touch-item {{item.isTouchMove ? 'touch-move-active' : ''}}" data-index="{{index}}" bindtouchstart="touchstart" bindtouchmove="touchmove" wx:for="{{contentlist}}" wx:if="{{contentlist.length>0}}">
    <view class="content">
    	<view class="title clearfix">
    		<view class="fl"><text class="point" wx:if="{{!item.status==1}}">●</text>{{item.title}}</view>
    		<view class="fr">{{item.gmtCreate}}</view>
    	</view>
    	<view class="info">{{item.content}}</view>
    </view>
    <view class="del" catchtap="del" data-index="{{index}}" data-id="{{item.id}}">删除</view>
</view>




<block wx:if="{{contentlist==undefind || contentlist.length<1}}">
<view class="empty_tip ">
  <image class="image" src="../static/image/me_pic_no_02.png"></image>
  <view class="txt">{{message}}</view>
</view>
</block>
  <!-- 快速导航 -->
  <import src="../../component/quick-navigation/quick-navigation.wxml"/>
  <template is="quickNavigation" data="{{ ...quickNavigation }}"/>

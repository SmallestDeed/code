/* 方案列表 */

.tag {
    height: 68rpx;
    width: 40rpx;
    position: absolute;
    top: 0;
    left: 20rpx;
}

.copyRightPrice {
    font-size: 40rpx;
    color: #ff6419;
    padding: 30rpx;
    padding-top: 9rpx;
    background: #fff;
    height:40rpx;
    line-height:40rpx;
}
.copyRightPrice.galys{
    color: #999;
    text-decoration: line-through;
}
.copyRightPrice_tit {
    display: inline-block;
    width: 80rpx;
    height: 32rpx;
    font-size: 22rpx;
    color: #ff6419;
    background: #ffeee5;
    text-decoration: none;
    line-height: 32rpx;
    vertical-align: middle;
    text-align: center;
}
.copyRightPrice.galys .copyRightPrice_tit{
    color: #999;
    background: #e8e8e8;
}
.case-list .case-item {
    margin-bottom: 20rpx;
    /* background-color:#ededed; */
    border-radius: 10rpx;
    overflow: hidden;
}
.case-list .case-item:last-child{
    margin-bottom: 0;
}
.case-list .case-item.case-item-border {
    border: #ededed 1rpx solid;
}

.case-list .case-item .case-image {
    height: 420rpx;
}

.case-list .case-item .case-image .bigPth {
    width: 100%;
    height: 100%;
    background: #fff;
}

.case-list .case-item .case-text {
    height: 127rpx;
    background-color: white;
    /* border-bottom-left-radius: 10rpx; 
  border-bottom-right-radius: 10rpx; */
}

.case-list .case-item .case-image {
    position: relative;
}

.case-list .case-item .case-image .Renovation-house {
    width: 171rpx;
    height: 60rpx;
    background-color: #ff6419;
    border-radius: 30px 0px 0px 30px;
    position: absolute;
    bottom: 35rpx;
    right: 0;
    color: #fff;
    font-size: 28rpx;
    line-height: 60rpx;
    padding-left: 39rpx;
    box-sizing: content-box;
}

.case-image .Renovation-house .arrow {
    width: 11rpx;
    height: 24rpx;
    position: absolute;
    right: 30rpx;
    top: 17rpx;
}

.case-image .seven-two-ico {
    position: absolute;
    bottom: 28rpx;
    left: 30rpx;
}

.case-image .seven-two-ico .identical-class {
    width: 70rpx;
    height: 70rpx;
    margin-right: 39rpx;
    float: left;
}

.seven-two-ico .identical-class image {
    width: 70rpx;
    height: 70rpx;
}

.case-list .case-item .case-text {
    padding-left: 31rpx;
}

.case-text .case-mes {
    padding-top: 30rpx;
    height: 32rpx;
    position: relative;
    padding-bottom: 9rpx;
}

.case-mes .case-mes-userphoto {
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    width: 68rpx;
    height: 68rpx;
}

.case-mes-userphoto image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
}

.case-mes .case-mes-name {
    font-size: 32rpx;
    max-width: 70%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #333;
    line-height: 32rpx;
}

.case-mes .case-mes-function .collet, .case-mes .case-mes-function .thumbs-up {
    margin-right: 26rpx;
    vertical-align: middle;
}

.case-mes .case-mes-function image {
    width: 47rpx;
    height: 47rpx;
    float: left;
}

.case-mes .case-mes-function text {
    color: #999;
    font-size: 24rpx;
    line-height: 47rpx;
    float: left;
}

.case-text .case-style {
    font-size: 26rpx;
    line-height: 57rpx;
    color: #999;
}

.case-style .case-style-image {
    width: 56rpx;
    height: 56rpx;
}

.case-style image {
    width: 56rpx;
    height: 56rpx;
}

.case-condition {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 90rpx;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 10;
}

.condition-box {
    width: 100%;
    max-height: 740rpx;
    position: absolute;
    left: 0;
    top: 0;
    background-color: #fff;
    padding: 31rpx 0rpx 8rpx 31rpx;
    box-sizing: border-box;
    overflow: auto;
}

.condition-box .single-box {
    width: 216rpx;
    height: 80rpx;
    background-color: #f5f5f5;
    border-radius: 5rpx;
    margin: 0 20rpx 20rpx 0;
    text-align: center;
    font-size: 28rpx;
    line-height: 80rpx;
    color: #333;
}

.condition-box .single-box.condition-box-active {
    background-color: #fff1eb;
    color: #ff6419;
}

.clearfix:after {
    content: '';
    height: 0;
    line-height: 0;
    clear: both;
    visibility: hidden;
    display: block;
}

.clearfix {
    zoom: 1;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

/* 装修报价说明弹框 */

.decorate-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 100000;
}

.decorate-price-box {
    width: 540rpx;
    /* height: 680rpx; */
    border-radius: 10rpx;
    background-color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 0 60rpx 28rpx;
    box-sizing: border-box;
}

.decorate-price-box .price-box-title {
    margin-top: 38rpx;
    line-height: 53rpx;
    text-align: center;
    color: #333;
    font-size: 32rpx;
}

.decorate-price-box .price-box-list .price-box-item {
    margin-top: 32rpx;
}

.price-box-list .price-box-item .box-item-main {
    color: #333;
    line-height: 48rpx;
    font-size: 28rpx;
}

.price-box-list .price-box-item .box-item-secondary {
    color: #999;
    font-size: 24rpx;
    line-height: 28rpx;
}

.decorate-price-box .price-box-confirm {
    margin-top: 50rpx;
    width: 420rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    background-color: #ff6419;
    font-size: 32rpx;
    border-radius: 40rpx;
}

/* 方案模块修改 */

.case-pageview {
    background-color: #fff;
    padding: 0 30rpx;
}

.case-pageview .case-pageview-box {
    border-top: solid 1rpx #ededed;
    line-height: 89rpx;
}

.case-pageview .pageview-box-left {
    color: #999;
    font-size: 24rpx;
}

.case-pageview .pageview-box-right {
    margin-top: 20rpx;
}

.pageview-box-right .collet {
    margin-right: 26rpx;
}

.pageview-box-right .thumbs-up {
    vertical-align: middle;
}

.pageview-box-right image {
    width: 47rpx;
    height: 47rpx;
    float: left;
}

.pageview-box-right text {
    color: #999;
    font-size: 24rpx;
    line-height: 47rpx;
    float: left;
}
.zzzcs{
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.7);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 1000;
}
.zzzcs .showView{
    width: 540rpx;
    position: fixed;
    background: #fff;
    border-radius: 10rpx;
    left: 50%;
    margin-left: -270rpx;
    padding: 176rpx 30rpx 50rpx;
    box-sizing: border-box;
    z-index: 1001;
    margin-top: -273rpx;
    top: 45%;
}
.zzzcs .showView>image.bgImg{
    display: block;
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    border-radius: 10rpx 10rpx 0 0;
    height: 176rpx;
}
.zzzcs .showView .items{
    width: 100%;
    box-sizing: border-box;
    height: 78rpx;
    line-height: 78rpx;
    font-size: 26rpx;
    color: #333;
    border-radius: 100rpx;
    padding: 0 30rpx;
    background-color: #f5f5f5;
    margin-top: 20rpx;
    position: relative;
}
.zzzcs .showView .items.code{
    padding-right: 195rpx;
}
.zzzcs .showView .items.code .pubCode{
    width: 195rpx;
    position: absolute;
    height: 28rpx;
    top: 25rpx;
    line-height: 28rpx;
    right: 0;
    text-align: center;
    color: #ff6419;
    font-size: 28rpx;
    border-left: 2rpx solid #dedede;
}
.zzzcs .showView .items input{
    width: 100%;
    height: 100%;
    line-height: 78rpx;
}
.zzzcs .btns.disableds{
    background: #ccc !important;
}
.zzzcs .pubCode.disableds{
    color: #999 !important;
}
.zzzcs .showView .items.btns{
    background-color: #ff6419;
    text-align: center;
    font-size: 32rpx;
    color: #fff;
}
.zzzcs .showView>image.close{
    width: 56rpx;
	height: 56rpx;
    position: absolute;
    bottom: -100rpx;
    left: 50%;
    margin-left: -28rpx;
}
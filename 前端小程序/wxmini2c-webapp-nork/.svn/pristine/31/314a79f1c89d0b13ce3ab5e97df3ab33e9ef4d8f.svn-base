<!-- <wxs module="filter" src="../../utils/toHref.wxs"></wxs> -->
<view class='case-list'>
  <form bindsubmit='formToThreeD' report-submit='true'>
  <!-- 方案列表 -->
    <view class='case-item {{border?"case-item-border":""}}' wx:for='{{recommendCaseList}}' 
      wx:key='{{index}}' 
      data-id='{{item.designPlanRecommendId || item.planRecommendedId}}' 
      data-type='{{item.planTableType}}'      
      catchtap='routerToCaseDetails'>
      <view class='case-image'>
        <!-- <image class='bigPth' 
            src='{{item.coverPath?resourcePath + item.coverPath:staticImageUrl+"home_pic_no.png"}}'>
        </image> -->
        <image class='bigPth' wx:if="{{item.designPlanCoverPath}}" 
            src='{{resourcePath + item.designPlanCoverPath}}'></image>
          <image class='bigPth' wx:if="{{item.coverPath&&!item.designPlanCoverPath}}" src='{{resourcePath + item.coverPath}}'></image>
        <image wx:if="{{!item.coverPath&&!item.designPlanCoverPath}}" 
            class='bigPth' src='{{staticImageUrl}}home_pic_no.png'></image>
<!-- 
        <block wx:if='{{item.designPlanRecommendId === item.groupPrimaryId||item.planRecommendedId===item.groupPrimaryId}}'>
        <image class='tag' src='{{staticImageUrl}}home_msg_group.png'></image>
        </block> -->
        <!-- <image class='tag' src='{{staticImageUrl}}home_msg_whole.png' wx:if='{{item.planTableType=="2"}}'></image> -->
        <view class='Renovation-house' catchtap='putInMyhouse' data-index='{{index}}' data-item='{{item}}' data-groupPrimaryId="{{item.planRecommendedId||item.designPlanRecommendId}}"> 
          装进我家 <image class='arrow' src='{{staticImageUrl}}page_icon_more_white@2x.png'></image>
        </view>
        <!-- rzd 181121随选网UI优化调整 方案收费标识 start -->
          <image class='page_icon_cost' wx:if="{{item.copyRightPermission==1||(item.copyRightPermission==0&&item.havePurchased==1)}}" data-item='{{item}}' catchtap='ShowplanCost' src='{{staticImageUrl}}page_icon_cost.png'></image>    
           <!-- rzd 181121随选网UI优化调整 方案收费标识 start -->       
        <view class='seven-two-ico clearfix'>
          <view class='video identical-class' wx:if='{{item.planTableType !== 2}}'>
            <image src='{{staticImageUrl}}place_icon_vedio@2x.png' data-item='{{item}}' data-type='video' catchtap='toThreeD'></image>
          </view>  
          <button form-type='submit' data-item='{{item}}' catchtap='{{true}}' data-type='720' class='identical-class'>
            <view class='single-point'>
              <image src='{{staticImageUrl}}place_icon_720@2x.png'></image>
            </view>
          </button>
          <button form-type='submit' wx:if='{{item.planTableType !== 2}}' data-item='{{item}}' catchtap='{{true}}' data-type='roam' class='identical-class'>
            <view class='multi-point'>
              <image src='{{staticImageUrl}}place_icon_morepoint@2x.png'></image>
            </view>
          </button>
          <view class='single-point identical-class'  data-item='{{item}}'  catchtap='toImg'>
            <image src='{{staticImageUrl}}place_icon_pic.png'></image>
          </view>
        </view>      
      </view>
      <view class='case-text'>
        <view class='case-mes'>
          <view class='case-mes-name {{(item.planTableType=="2")?"acts":""}}'>
            <image class='tag' src='{{staticImageUrl}}home_msg_all.png' wx:if='{{item.planTableType=="2"}}'></image>
            {{item.planName || item.designPlanName}}
          </view>
          <!-- <view class='case-mes-userphoto'>
            <image 
            src='{{item.designerHeadPic?resourcePath + item.designerHeadPic:staticImageUrl+"index (2).png"}}'>
            </image>
          </view> -->
          
          <view class='pageview-box-right fr'>
            <view class='collet fl clearfix' data-index='{{index}}' catchtap='collectCase' data-item='{{item}}'>
                <image 
                src='{{item.isFavorite!=1?staticImageUrl+"page_icon_collect_nor@2x.png":staticImageUrl+"page_icon_collect_sct@2x.png"}}'>
                </image> 
              <text>{{item.collectNum || 0}}</text>
            </view>
            <view class='thumbs-up fl clearfix' data-index='{{index}}'  data-item='{{item}}' catchtap='likeCase'>
                <image 
                src='{{item.isLike != 1?staticImageUrl+"page_icon_like_nor@2x.png":staticImageUrl+"page_icon_like_sct@2x.png"}}'>
                </image> 
              <text>{{item.likeNum || 0}}</text>
            </view>    
          </view>  
          <!-- <view class='case-mes-function fr clearfix' wx:if='{{item.planHouseType != 2}}'>
            <view class='collet fl clearfix' data-index='{{index}}' catchtap='collectCase' data-item='{{item}}'>
                <image 
                src='{{item.isFavorite!=1?staticImageUrl+"page_icon_collect_nor@2x.png":staticImageUrl+"page_icon_collect_sct@2x.png"}}'>
                </image> 
              <text>{{item.collectNum || 0}}</text>
            </view>
            <view class='thumbs-up fl clearfix' data-index='{{index}}'  data-item='{{item}}' catchtap='likeCase'>
                <image 
                src='{{item.isLike != 1?staticImageUrl+"page_icon_like_nor@2x.png":staticImageUrl+"page_icon_like_sct@2x.png"}}'>
                </image> 
              <text>{{item.likeNum || 0}}</text>
            </view>              
          </view>             -->
        </view>
        <view class='case-style'>
          <view class='fl'>
              <block>
                <text>{{item.spaceStyleName || item.styleName}} </text>
                <text wx:if='{{(item.spaceFunctionId != 13 || item.planTableType !=2) && !(item.designPlanRecommendId == item.groupPrimaryId||item.planRecommendedId==item.groupPrimaryId)}}'>| {{item.spaceArea || item.spaceAreas}}m²</text> 
                <text>{{item.planDecoratePriceList.length>0?'| ' + item.planDecoratePriceList[0].decorateTypeName + '￥' + item.planDecoratePriceList[0].decoratePrice + '元/m²': ''}} </text>
              </block>
          </view>
          <view class='fl case-style-image' 
          data-item='{{item.planDecoratePriceList}}' 
          wx:if='{{item.planDecoratePriceList.length>0}}' 
          catchtap='showDecoratePriceBox'>
            <image src='{{staticImageUrl}}home_icon_more_02.png'></image>
          </view>
        </view>        
      </view>
      <!-- 版权收费 -->
      <!-- <view 
        class="copyRightPrice {{item.havePurchased==1?'galys':''}}"
        wx-if="{{(item.copyRightPermission==1||(item.copyRightPermission==0&&item.havePurchased==1))&&!showCopyRightPrice}}">
       {{item.havePurchased==1?'已购买':'版权费: '+item.planPrice+'度币'}}
      </view> -->
      <!-- <view class='case-pageview'>
        <view class='case-pageview-box clearfix'>
          <view class='pageview-box-left fl'>
            浏览量 {{item.useCount || item.viewNum || item.visitCount || 0}}
          </view>
          <view class='pageview-box-right fr'>
            <view class='collet fl clearfix' data-index='{{index}}' catchtap='collectCase' data-item='{{item}}'>
                <image 
                src='{{item.isFavorite!=1?staticImageUrl+"page_icon_collect_nor@2x.png":staticImageUrl+"page_icon_collect_sct@2x.png"}}'>
                </image> 
              <text>{{item.collectNum || 0}}</text>
            </view>
            <view class='thumbs-up fl clearfix' data-index='{{index}}'  data-item='{{item}}' catchtap='likeCase'>
                <image 
                src='{{item.isLike != 1?staticImageUrl+"page_icon_like_nor@2x.png":staticImageUrl+"page_icon_like_sct@2x.png"}}'>
                </image> 
              <text>{{item.likeNum || 0}}</text>
            </view>    
          </view>   
        </view>   
      </view> -->
    </view>
  <!-- 装修报价说明弹框 -->
    <view class='decorate-mask' wx:if='{{decoratePriceBox}}' catchtouchmove='{{true}}'>
      <view class='decorate-price-box'>
          <view class='price-box-title'>装修报价</view>
          <view class='price-box-list'>
            <view class='price-box-item' wx:for='{{decoratePriceList}}' wx:key='{{index}}'>
              <view class='box-item-main clearfix'>
                <view class='fl'>{{item.decorateTypeName}}</view> <view class='fr'>￥{{item.decoratePrice}}/m²</view>
              </view>
              <view class='box-item-secondary'>{{item.text}}</view>
            </view>
          </view>
          <view class='price-box-confirm' catchtap='hideDecoratePriceBox'>我知道了</view>
      </view>
    </view>
    <view class='zzzcs' wx:if="{{getPhone}}">
        <view class='showView'>
            <image src='{{staticImageUrl}}zhuce02@2x.png' class='bgImg'></image>
            <view class='items' style='margin-top:50rpx;'>
                <input placeholder='手机号' maxlength='11' type='number' value='{{getObj.phone}}' bindinput='changeInput' data-type="phone"></input>
            </view>
            <view class='items code'>
                <input placeholder='验证码' data-type="code" maxlength='8' type='number' disabled='{{!inputFlag2}}' bindinput='changeInput' value='{{getObj.code}}'></input>
                <view class='pubCode  {{inputFlag?"":"disableds"}}' bindtap='{{inputFlag&&message=="获取验证码"?"getCodeFun":""}}'>
                    {{message?message:"获取验证码"}}
                </view>
            </view>
            <view class='items btns {{inputFlag2?"":"disableds"}}' bindtap='{{inputFlag2?"submitFun":""}}'>确认</view>
            <image src='{{staticImageUrl}}home_pop_close.png' class='close' bindtap='closeFun'></image>
        </view>
    </view>
    <!-- rzd181121随选网UI调整方案收费弹框 start-->
    <view class='planCost_bg' wx:if="{{planCostShow}}">
      <view class='planCost_box'>
        <image src='{{staticImageUrl}}pop_pic_cost.png'></image>
        <view class='planCost_title'>该方案为付费方案，在产品替换</view>
        <view class='planCost_title'>装进我家时需要支付设计师原创</view>
        <view class='planCost_price {{havePurchased==1?"galys":""}}'>
          版权费　{{planPrice}}度币　({{planPrice/10}}元)
        </view>
        <view class='planCost_but' bindtap='closePlanCost'>我知道了</view>
      </view>
    </view>
    <!-- rzd181121随选网UI调整方案收费弹框 end-->
  </form>

</view>
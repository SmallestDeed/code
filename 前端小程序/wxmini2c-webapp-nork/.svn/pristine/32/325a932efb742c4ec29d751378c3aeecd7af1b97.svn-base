}
.headimg{
	width: 108rpx;
	height: 108rpx;
	border-radius: 108rpx
}
.set-nickname{
	padding:31rpx 30rpx;
}
.fr{
	color:#8e8e8e
}

.img{
	margin-left:20rpx;
	width: 15rpx;
	height: 28rpx;
	vertical-align:-3rpx;

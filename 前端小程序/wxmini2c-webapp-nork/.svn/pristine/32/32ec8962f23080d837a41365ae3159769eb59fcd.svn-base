/* pages/confirm-order/confirm-order.wxss */
page{
  background:#e8e8e8;
}
.input-box {
  text-align: right;
  z-index:0;
}
.confirm-order{
  width:100%;
  min-height:100%;
  background-color: #ededed;
}
.order-address {
  padding:20rpx 0;
  box-sizing: border-box;
}
.order-address .add-address{
  padding: 55rpx 0 0 257rpx;
  width:750rpx;
  height: 144rpx;
	background-color: #fffcfa;
	color: #333333;
	font-size: 32rpx;
  box-sizing: border-box;
  border-bottom:solid 4rpx #ff9966;
}
.order-address .add-address image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 19rpx;
}
.order-address .add-text {
	color: #333333;
	font-size: 32rpx;
}
.order-address .choose-address{
  width:750rpx;
  height: 140rpx;
	background-color: #fff;
  position: relative;
  padding-left:129rpx;
	color: #333333;
  font-size: 28rpx;
  box-sizing: border-box;
}
.order-address .choose-address .botBg{
  position: absolute;
  width: 100%;
  bottom: 0;
  height: 6rpx;
  left: 0;
}
.choose-address .address-icon {
  position: absolute;
  left:46rpx;
  top:48rpx;
  width: 36rpx;
  height: 45rpx;
}
.choose-address .address-icon image{
  width: 36rpx;
  height: 45rpx;
}
.choose-address .address-arrow {
  position: absolute;
  top:51rpx;
  right:30rpx; 
  width: 15rpx;
  height: 28rpx;
}
.choose-address .address-arrow image{
  width: 15rpx;
  height: 28rpx;
}
.choose-address .user-mes {
  padding-top:10rpx;
  line-height: 69rpx;
}
.choose-address .address-mes {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width:550rpx;
}


.order-details {
	width: 750rpx;
	height: 220rpx;
	background-color: #ffffff;
  box-sizing: border-box;
  padding:30rpx 37rpx 30rpx 30rpx;
}
.order-details .details-image image {
  width:160rpx;
  height: 160rpx;
}
.details-text {
  margin-left: 27rpx;
}
.details-text .text-name {
	color: #333333;
	font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
   width: 480rpx; 
}
.details-text .text-model {
  color: #999999;
	font-size: 24rpx; 
  line-height: 79rpx; 
}
.details-text .text-price {
	color: #333333;
  font-size: 28rpx;
  width:496rpx;
}
.details-text .text-price .count {
  font-size: 32rpx;
}
.order-price {
  background-color: #fff;
  font-size: 28rpx;
  line-height: 88rpx;
  /* padding: 0 31rpx; */
  margin:20rpx 0;
}
.order-price .goods-price{
  padding: 0 31rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.order-price .logistics-price{
  padding: 0 31rpx;
}
.order-price  .text {
	color: #333333;
}
.order-price  .price {
	color: #ff6419;
}
.order-remarks {
  height: 88rpx;
  font-size: 28rpx;
  line-height: 88rpx;
  padding: 0 31rpx;
  background-color: #fff;
}
.order-remarks .text{
	color: #333333;  
}
.order-remarks .message input{
	border: none;
  height: 88rpx;
  width:550rpx;
  margin-left: 26rpx;
  z-index: 0；
}

/* 底部样式 */
.order-footer {
  position: fixed;
  bottom:0;
  left:0;
  height: 98rpx;
  line-height: 98rpx;
  width:750rpx;
}
.order-footer .footer-price{
  padding-left:30rpx;
	color: #333333;
  font-size: 28rpx;
  width: 490rpx;
  height: 98rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.order-footer .footer-price text{
	color: #ff6419;
	font-size: 32rpx;
}
.order-footer  .footer-payment {
	background-color: #ff6419;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  width: 260rpx;
}



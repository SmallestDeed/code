.zzcView{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  top: 0;
  left: 0;
  z-index: 9999;
}
.zzcView .rViews{
  width: 100%;
  box-sizing: border-box;
  background: #f5f5f5;
  position: absolute;
  bottom: 0;
  left: 0;
  max-height: 940rpx;
}
.zzcView .titles{
  width: 100%;
  box-sizing: border-box;
  padding-right: 70rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #fff;
  position: relative;
  text-align: center;
  font-size: 32rpx;
	color: #333333;
}
.zzcView .titles image{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 28rpx;
	height: 27rpx;
}
.zzcView .mains{
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
  padding-bottom: 104rpx;
  position: relative;
  overflow-y: scroll;
  max-height: 852rpx;
}
.zzcView .mains .btns{
  width: 690rpx;
  width: calc(100% - 60rpx);
  width: -webkit-calc(100% - 60rpx);
  width: -moz-calc(100% - 60rpx);
	height: 80rpx;
	background-color: #ff6419;
	border-radius: 100px;
  color: #fff;
  text-align: center;
	font-size: 32rpx;
  line-height: 80rpx;
  position: fixed;
  bottom: 14rpx;
  left: 30rpx;
	letter-spacing: 0px;
  z-index: 12;
}
.zzcView .mains .items{
  width: 100%;
  height: 240rpx;
	background-color: #ffffff;
	border-radius: 4rpx;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  padding: 40rpx 35rpx 40rpx 0;
}
.zzcView .mains .items>view{
  height: 100%;
  float: left;
  position: relative;
}
.zzcView .mains .items>view.itemL{
  width: 270rpx;
  text-align: center;
  color: #ff6419;
  font-family: PingFang-SC-Regular;
  line-height: 1;
}
.zzcView .mains .items .itemL .tops{
  font-size: 80rpx;
	font-weight: 600;
  margin-bottom: 35rpx;
}
.zzcView .mains .items .itemL .tops>view{
  display: inline-block;
  margin-right: 7rpx;
  position: relative;
  width: 22rpx;
  height: 26rpx;
  font-weight: 200;
  font-size: 36rpx;
  top: -35rpx;
}
.zzcView .mains .items .itemL .bots{
	font-size: 28rpx;
}
.zzcView .mains .items .itemR{
  width: calc(100% - 270rpx);
  width: -webkit-calc(100% - 270rpx);
  width: -moz-calc(100% - 270rpx);
}
.zzcView .mains .items .itemR .ps{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 22rpx;
  line-height: 1;
	font-family: PingFang-SC-Regular;
	font-size: 22rpx;
  margin-bottom: 20rpx;
	color: #000;
}
.zzcView .mains .items .itemR .chse{
  position: absolute;
  width: 36rpx;
	height: 36rpx;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
	border: solid 2rpx #cccccc;
  border-radius: 50%;
}
.zzcView .mains .items .itemR .chse.active{
  border: none;
  background-color: #ff6419;
}
.zzcView .mains .items:last-child{
  margin-bottom: 0;
}
::-webkit-scrollbar {
  display: none;
}
.zzcView .mains .def{
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  position: relative;
  text-align: center;
}
.zzcView .mains .def text{
  display: block;
  width: 220rpx;
  height: 100%;
  position: absolute;
  z-index: 2;
  font-size: 24rpx;
  top: 0;
  left: 50%;
  margin-left: -110rpx;
  background: #f5f5f5;
  color: #999;
}
.zzcView .mains .def::after{
  content: '';
  width: 400rpx;
  position: absolute;
  height: 1rpx;
  background: #999;
  left: 50%;
  top: 50%;
  margin-left: -200rpx;
  margin-top: -0.5rpx;
  z-index: 1;
}
.zzcView .mains .items.itemGlays>view.itemL{
  color: #999;
}


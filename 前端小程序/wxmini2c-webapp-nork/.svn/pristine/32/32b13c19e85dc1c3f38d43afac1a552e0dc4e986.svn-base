/* pages/goods-details/goods-details.wxss */
@import "../../component/render-type/render-type.wxss";

/* 头部样式 */

.goods-details {
    /* padding-top: 88rpx; */
    background-color: #ededed;
}

.goods-details-header {
    height: 88rpx;
    width: 750rpx;
    display: flex;
    margin-bottom: 4rpx;
    position: fixed;
    left: 0;
    top: 0;
    background-color: #fff;
    z-index: 10;
}

.goods-details-header view {
    flex: auto;
    text-align: center;
    line-height: 88rpx;
}

.goods-details-header view text {
    display: inline-block;
    font-size: 32rpx;
    line-height: 82rpx;
}

.goods-details-header view .text-active {
    color: #ff6419;
    border-bottom: #ff6419 solid 4rpx;
}

/* 輪播圖樣式 */

.goods-details-content {
    /* margin-top:3px; */
    padding-bottom: 98rpx;
}

.goods-details-content swiper {
    height: 750rpx;
}

.goods-details-content swiper image {
    width: 750rpx;
    height: 750rpx;
}

/* z主要字体样式 */

.goods-details-content .main-text {
    width: 750rpx;
    min-height: 165rpx;
    /* border-bottom:10rpx solid #f5f5f5; */
    padding-left: 29rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.goods-details-content .main-text .main-text-name {
    font-size: 28rpx;
    background-color: #fff;
    line-height: 48rpx;
    max-height: 96rpx;
    overflow: hidden;
    word-break: break-all;
    padding-top: 20rpx;
    margin-bottom: 30rpx;
    width: 690rpx;
}

.goods-details-content .main-text .main-text-funtion .funtion-price {
    line-height: 50rpx;
    font-size: 40rpx;
    color: #ff6419;
    font-weight: bold;
    margin-left: -6rpx;
}

.goods-details-content .main-text .main-text-funtion .identical {
    width: 50rpx;
    height: 50rpx;
}

.goods-details-content .main-text .main-text-funtion .ico-Text {
    color: #666;
    line-height: 50rpx;
    font-size: 22rpx;
    margin-left: 10rpx;
    margin-right: 29rpx;
}

.main-text-funtion .identical image {
    width: 50rpx;
    height: 50rpx;
}

/* 颜色款式样式 */

.chose-specifications {
    height: 128rpx;
    padding-left: 31rpx;
    font-size: 28rpx;
    line-height: 88rpx;
    color: #333;
    border-top: solid 20rpx #f5f5f5;
    border-bottom: solid 20rpx #f5f5f5;
    box-sizing: border-box;
    background-color: #fff;
}

.chose-specifications .arrow image {
    width: 15rpx;
    height: 28rpx;
    margin-top: 30rpx;
    margin-right: 30rpx;
}

/* 产品信息 */

.goods-message {
    padding: 0rpx 30rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.goods-message .g-m-title {
    line-height: 93rpx;
    font-size: 28rpx;
    color: #333;
}

.goods-message .g-m-reset-title {
    text-align: center;
    border-radius: 30px;
    border: solid 2rpx #ddd;
    height: 60rpx;
    width: 195rpx;
    margin: 0 auto;
    line-height: 60rpx;
    position: relative;
}

.goods-message .g-m-reset-title::before, .goods-message .g-m-reset-title::after {
    content: '';
    display: inline-block;
    height: 1px;
    background-color: #eee;
    width: 248rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.goods-message .g-m-reset-title::before {
    right: 200rpx;
}

.goods-message .g-m-reset-title::after {
    left: 200rpx;
}

.goods-message .g-m-info-box .g-m-info-title {
    border-bottom: 1px dashed #e0e0e0;
}

.goods-message .g-m-info-box .g-m-info-row {
    border-bottom: 1px dashed #e0e0e0;
    height: auto;
    display: -webkit-flex;
    display: flex;
}

.goods-message .g-m-info-box .g-m-info-row .g-m-info-row-title {
    font-size: 24rpx;
    color: #999;
    width: 145rpx;
    padding: 20rpx 0;
}

.goods-message .g-m-info-box .g-m-info-row .g-m-info-row-content {
    box-sizing: border-box;
    font-size: 24rpx;
    color: #666;
    -webkit-flex: 1;
    flex: 1;
    height: 80rpx;
    line-height: 80rpx;
}

.goods-message .g-m-table {
    padding-bottom: 24rpx;
    padding-top: 40rpx;
    overflow: hidden;
}

.goods-message .g-m-table .wxParse-img {
    width: 100%;
}

.goods-message .g-m-table .g-m-t-item {
    line-height: 58rpx;
    font-size: 24rpx;
    color: #666;
}

.goods-message .g-m-table .g-m-t-item:nth-child(1) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-item:nth-child(2) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-item:nth-child(3) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-describe {
    line-height: 43rpx;
    padding: 15rpx 0rpx;
    padding-left: 160rpx;
    position: relative;
    box-sizing: border-box;
}

.goods-message .g-m-table .g-m-t-describe text {
    position: absolute;
    left: 0rpx;
    top: 18rpx;
    margin: 0rpx;
}

.goods-message .g-m-table .g-m-t-item text {
    color: #999;
    margin-right: 50rpx;
}

/* 案例样式 */

.goods-details-case {
    padding: 0 30rpx 30rpx;
    background-color: #fff;
}

.goods-details-case .case-item {

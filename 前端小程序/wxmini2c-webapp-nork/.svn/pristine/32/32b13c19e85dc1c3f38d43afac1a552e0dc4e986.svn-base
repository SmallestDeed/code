}

.seven-two-ico .identical-class image {
    width: 70rpx;
    height: 70rpx;
}

.goods-details-case .case-item .case-text {
    padding-left: 31rpx;
}

.case-text .case-mes {
    padding-top: 31rpx;
}

.case-mes .case-mes-name {
    font-size: 32rpx;
    color: #333;
}

.case-mes .case-mes-function .collet, .case-mes .case-mes-function .thumbs-up {
    margin-right: 26rpx;
    vertical-align: middle;
}

.case-mes .case-mes-function image {
    width: 47rpx;
    height: 47rpx;
    float: left;
}

.case-mes .case-mes-function text {
    color: #999;
    font-size: 24rpx;
    line-height: 47rpx;
    float: left;
}

.case-text .case-style {
    font-size: 26rpx;
    line-height: 57rpx;
    color: #999;
}


<view class="head-portrait">
  <view class="head-iteam image " hover-class="list-cell_active">
    <block wx:if='{{wxInfo.userInfo!=null}}'>
      <image src="{{wxInfo.userInfo.avatarUrl}}" class="headimg"></image>
    </block>
    <block wx:if='{{wxInfo.userInfo==null}}'>
      <image src="./image/index.png" class="headimg"></image>
    </block>
  </view>
  <view class='head-information'>
    <view>
      <block wx:if='{{wxInfo.userInfo.nickName}}'>
        <view class="head-iteam name ">{{wxInfo.userInfo.nickName}}</view>
      </block>
      <view class='maney' bindtap='toBuyCoin'>
        <view class="head-iteam points " hover-class="list-cell_active">
          <text>度币</text>{{userInfo.balanceIntegral|Int}}</view>
      </view>
    </view>
  </view>
</view>

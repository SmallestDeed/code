<view class="person-nav">
  <view class='person-nav-top'>
    <navigator url="/pages/purchase-car/purchase-car" class="flex-iteam preson-nav-iteam" hover-class="none">
      <view class="img">
        <image src="../static/image/me_icon_shoppingcar.png"></image>
      </view>
      <view class="title">购物车</view>
    </navigator>

    <navigator url="/pages/my-order/my-order" class="flex-iteam preson-nav-iteam" hover-class="none">
      <view class="img">
        <image src="../static/image/mr_icon_order.png"></image>
      </view>
      <view class="title">订单</view>
    </navigator>
    <navigator url="/pages/my-card/my-card" class="flex-iteam preson-nav-iteam" hover-class="none">
      <view class="img">
        <image src="../static/image/me_icon_coupons.png"></image>
      </view>
      <view class="title">我的卡券</view>
    </navigator>
    <navigator url="/pages/mini-offcial-web/mini-offcial-web" class="flex-iteam preson-nav-iteam" hover-class="none">
      <view class="img">
        <image src="../static/image/me_icon_wei.png"></image>
      </view>
      <view class="title">线下门店</view>
    </navigator>
  </view>
  <image bindtap='showBIndingPhoneBox' wx:if='{{!issBindingMobile}}' class='preson-nav-image' src='../static/image/pop_pic_200.png'></image>
</view>
<view class="list-cells ">
  <navigator url="/pages/my-favorite/my-favorite" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_collect.png'></image>
      <view class='lise-cell_txt'>我的收藏</view>
    </view>
    <view class="list-cell__ft">
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
  <navigator url="/pages/my-favorite-fitment/my-favorite-fitment" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_decorate.png'></image>
      <view class='lise-cell_txt'>我的装修</view>
    </view>
    <view class="list-cell__ft">
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
  <view class='line'></view>
  <navigator url="/pages/my-house-type/my-house-type" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_type.png'></image>
      <view class='lise-cell_txt'>我的户型</view>
    </view>
    <view class="list-cell__ft">
      <!-- 剩余5套 -->
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
  <navigator url="/pages/my-tasks/my-tasks" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_task.png'></image>
      <view class='lise-cell_txt'>我的任务</view>
    </view>
    <view class="list-cell__ft">
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
  <navigator url="/pages/my-messages/my-messages" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_notice.png'></image>
      <view class='lise-cell_txt'>我的消息</view>
    </view>
    <view class="list-cell__ft">
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
  <navigator url="/pages/address-list/address-list" class="list-cell clearfix" hover-class="list-cell_active">
    <view class="list-cell__bd">
      <image class='lise-cell_icon' src='../static/image/me_icon_adress.png'></image>
      <view class='lise-cell_txt'>地址管理</view>
    </view>
    <view class="list-cell__ft">
      <image src="../static/image/list_icon_more.png"></image>
    </view>
  </navigator>
</view>


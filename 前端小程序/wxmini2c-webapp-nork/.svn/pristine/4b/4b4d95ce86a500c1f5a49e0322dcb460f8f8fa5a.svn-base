/* pages/web-detail-complicated/web-detail-complicated.wxss */
@import "/utils/wxParse/wxParse.wxss";

page {
    background-color: #fff;
    min-height: 100%;
}

.toparea {
    width: 750rpx;
    height: 560rpx;
    background-color: #f5f5f5;
}

.white {
    width: 750rpx;
    background-color: #fff;
}

.information {
    width: 690rpx;
    margin-left: 30rpx;
}

.information>view {
    width: 100%;
    box-sizing: border-box;
    padding-right: 150rpx;
    position: relative;
}

.information>view view.pullRight {
    height: 44rpx;
    position: absolute;
    top: 50%;
    margin-top: -22rpx;
    right: 0;
    width: 140rpx;
    text-align: right;
    line-height: 44rpx;
    font-weight: 300;
    font-size: 24rpx;
    color: #999;
}

.information>view view.pullRight image {
    display: inline-block;
    width: 44rpx;
    height: 44rpx;
}

.information .title {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-top: 40rpx;
    font-weight: bold;
}

.information .title .titletxt {
    font-size: 32rpx;
    color: #333;
    line-height: 32rpx;
    max-width: 600rpx;
    word-wrap: break-word;
    word-break: normal;
}

.information .title .range {
    font-size: 24rpx;
    color: #999;
    line-height: 32rpx;
}

.information .phone {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.information .phone .phonenumber {
    font-size: 28rpx;
    color: #333;
    line-height: 28rpx;
}

/* .information .phone .icon{
  height: 44rpx;
  width: 44rpx;
} */

.information .address {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.information .address .addresstxt {
    font-size: 28rpx;
    color: #999;
    line-height: 38rpx;
    max-width: 600rpx;
    word-wrap: break-word;
    word-break: normal;
}

.information .address .icon {
    height: 44rpx;
    width: 44rpx;
}

.information .line {
    width: 690rpx;
    height: 1px;
    background-color: #f5f5f5;
    margin-top: 30rpx;
}

.profile {
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.profile .text {
    font-size: 32rpx;
    color: #333;
    line-height: 32rpx;
}

.profile .content {
    font-size: 28rpx;
    color: #999;
    word-wrap: break-word;
    word-break: normal;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.activities {
    height: 160rpx;
    width: 690rpx;
    margin-top: 50rpx;
    margin-bottom: 35rpx;
    box-shadow: 0rpx 60rpx 25rpx -50rpx #ffd6d6;
}

.gary {
    height: 20rpx;
    width: 750rpx;
    background-color: #f5f5f5;
}

.plan {
    min-height: 500rpx;
    background-color: #f5f5f5;
}

.planchange {
    height: 90rpx;
    width: 750rpx;
    display: flex;
    flex-direction: row;
    border-bottom: 1px solid #f5f5f5;
}

.option {
    width: 187rpx;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #333;
    text-align: center;
    line-height: 90rpx;
}

.optionchecked {
    width: 187rpx;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    color: #ff6419;
    text-align: center;
    line-height: 90rpx;
    font-weight: bold;
}

.selectcolor {
    width: 60rpx;
    height: 4rpx;
    background-color: #ff6419;
    border-radius: 2rpx;
    margin-left: 63rpx;
    position: relative;
    bottom: 4rpx;
}

.comment {
    width: 690rpx;
    margin-left: 30rpx;
}

.comment .score {
    height: 190rpx;
    display: flex;
    flex-direction: row;
}

.house-case {
    box-sizing: border-box;
    background-color: #f5f5f5;
}

.top {
    height: 88rpx;
    width: 750rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    left: 0;
    border-bottom: 2rpx solid #ededed;
    color: #333;
    font-size: 28rpx;
    z-index: 10;
}

.top .active {
    color: #ff6419;
}

.top image {
    width: 16rpx;
    height: 16rpx;
}

.room {
    height: 88rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.area {
    height: 90rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.style {
    height: 90rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.space {
    height: 90rpx;
}

.goods-details-case {
    padding-left: 20rpx;
    padding-right: 20rpx;
    padding-top: 20rpx;
    padding-bottom: 0rpx;
    background-color: #f5f5f5;
}

.goods-details-case .case-item {
    height: 560rpx;
    padding-bottom: 20rpx;
    background-color: #f5f5f5;
    border-radius: 10rpx;
    overflow: hidden;
}

.goods-details-case .case-item .case-image {
    height: 420rpx;
}

.goods-details-case .case-item .case-image .bigPth {
    width: 100%;
    height: 100%;
}

.goods-details-case .case-item .case-text {
    height: 140rpx;
    background-color: white;
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.goods-details-case .case-item .case-image {
    position: relative;
}

.goods-details-case .case-item .case-image .Renovation-house {
    width: 171rpx;
    height: 60rpx;
    background-color: #ff6419;
    border-radius: 30px 0px 0px 30px;
    position: absolute;
    bottom: 35rpx;
    right: 0;
    color: #fff;
    font-size: 28rpx;
    line-height: 60rpx;
    padding-left: 39rpx;
}

.case-image .Renovation-house .arrow {
    width: 11rpx;
    height: 24rpx;
    position: absolute;
    right: 30rpx;
    top: 17rpx;
}

.case-image .seven-two-ico {
    position: absolute;
    bottom: 28rpx;
    left: 30rpx;
}

.case-image .seven-two-ico .identical-class {
    width: 70rpx;
    height: 70rpx;
    margin-right: 39rpx;
    float: left;
}

.seven-two-ico .identical-class image {
    width: 70rpx;
    height: 70rpx;
}

.goods-details-case .case-item .case-text {
    padding-left: 31rpx;
}

.case-text .case-mes {
    padding-top: 31rpx;
}

.case-mes .case-mes-name {
    font-size: 32rpx;
    color: #333;
}

.case-mes .case-mes-function .collet, .case-mes .case-mes-function .thumbs-up {
    margin-right: 26rpx;
    vertical-align: middle;
}

.case-mes .case-mes-function image {
    width: 47rpx;
    height: 47rpx;
    float: left;
}

.case-mes .case-mes-function text {
    color: #999;
    font-size: 24rpx;
    line-height: 47rpx;
    float: left;
}

.case-text .case-style {
    font-size: 26rpx;
    line-height: 57rpx;
    color: #999;
}

.contactview {
    height: 120rpx;
    width: 750rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
}

.contact {
    width: 690rpx;
    height: 80rpx;
    background-color: #ff6419;
    font-size: 32rpx;
    color: #fff;
    position: absolute;
    bottom: 20rpx;
    left: 30rpx;
    text-align: center;
    line-height: 80rpx;
    border-radius: 40rpx;
}

.noShop {
    height: 500rpx;
    width: 750rpx;
    font-size: 24rpx;
    color: #999;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    z-index: 10;
}

.noShopimg {
    height: 200rpx;
    width: 200rpx;
}

.noShoptxt {
    margin-top: 10rpx;
}

.shopcase {
    background-color: #fff;
    width: 690rpx;
    margin-left: 30rpx;
    margin-top: 20rpx;
}

.shopcase .title {
    height: 28rpx;
    width: 690rpx;
    margin-left: 30rpx;
    display: flex;
    flex-direction: row;
    margin-top: 58rpx;
}

.shopcase .line {
    width: 4rpx;
    height: 24rpx;
    background-color: #ff6419;
    border-radius: 2rpx;
    margin-top: 2rpx;
}

.shopcase .txt {
    font-size: 28rpx;
    line-height: 28rpx;
    color: #333;
    font-weight: bold;
    margin-left: 13rpx;
}

.shopcase .content {
    width: 690rpx;
    margin-left: 30rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    margin-top: 28rpx;
    color: #333;
}

.shopcase .shopmanager {
    height: 28rpx;
    width: 690rpx;
    margin-left: 30rpx;
    display: flex;
    flex-direction: row;
    margin-top: 65rpx;
}

.manager {
    height: 340rpx;
    width: 340rpx;
    margin-top: 30rpx;
    margin-left: 205rpx;
    border-radius: 50%;
    background-color: #e8e8e8;
}

.managerName {
    width: 750rpx;
    height: 28rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #333;
    font-weight: bold;
    margin-top: 30rpx;
    text-align: center;
}

.teamjs {
    height: 28rpx;
    width: 690rpx;
    margin-left: 30rpx;
    display: flex;
    flex-direction: row;
    margin-top: 65rpx;
    margin-bottom: 30rpx;
}

.team {
    margin-bottom: 30rpx;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.teamimage {
    width: 690rpx;
    border-radius: 6rpx;
}

.teamcontent {
    width: 690rpx;
    margin-left: 30rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    color: #333;
}

.bck {
    background-color: #f5f5f5;
}

.cases {
    width: 690rpx;
    margin-left: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
}

.bck .img {
    width: 690rpx;
    border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.bck .content {
    width: 690rpx;
    border-radius: 0rpx 0rpx 10rpx 10rpx;
}

.bck .title {
    width: 630rpx;
    height: 28rpx;
    margin-left: 30rpx;
    margin-top: 30rpx;
    margin-bottom: 20rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    color: #333;
    font-weight: bold;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.bck .txt {
    width: 630rpx;
    max-height: 96rpx;
    margin-top: 30rpx;
    margin-left: 30rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    color: #333;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 30rpx;
}

.space {
    height: 120rpx;
    widows: 750rpx;
    background-color: #f5f5f5;
}

.more {
    width: 630rpx;
    height: 88rpx;
    font-size: 28rpx;
    color: #333;
    text-align: right;
    line-height: 88rpx;
    margin-left: 30rpx;
    border-top: 1rpx solid #f5f5f5;
}

.more .icon {
    width: 15rpx;
    height: 28rpx;
    margin-top: 30rpx;
}

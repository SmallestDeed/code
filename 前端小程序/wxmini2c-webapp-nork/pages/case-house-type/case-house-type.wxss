/* pages/house-type/house-type.wxss */
page {
    display: block;
    min-height: 100%;
    background-color: #eeeeee;
}
.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    background: rgb(255, 255, 255)
}
.type-header .left,
.type-header .right {
    font-weight: 600;
    text-align: center;
    color: #313131;
    font-size: 36rpx;
    line-height: 88rpx;
}
.cut-off {
    color: #ddd;
    float: right;
    width: 1rpx;
    background: #ddd;
    margin-top: 28rpx;
    height: 36rpx;
}
.type-search {
    overflow: hidden;
    margin-top: 20rpx;
}
.list {
    position: absolute;
    left: 0;
    top: 558rpx;
    width: 100%;
    font-size: 28rpx;
    color: "#333333";
    background: #fff;
}
.list.spaceIteam{
    margin-top: 0rpx;
}
.list-iteam,
.list-iteam-active {
    padding: 31rpx 0;
    width: 100%;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1rpx solid #ddd;
}
.list-iteam:last-child {
    border-bottom: none;
}
.iteam_right {
    margin-right: 31rpx;
    text-align: right;
    color: #8e8e8e;
    position: relative;
}
.flex_iteam,
.iteam_left {
    margin-left: 31rpx;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    color: #333;
}
/*三级联动样式*/
/**app.wxss**/
/* 上传户型模块样式 */
.uploading-house{
  line-height: 106rpx;
  color: #333;
  font-size: 28rpx;
}
.uploading-house text {
  color: #999;
}
.search-history{
  margin-top: 60rpx;
}
.search-history .history-title{
  color: #999;
  font-size: 28rpx;
}
.history-title .title-icon image{
  width: 44rpx;
  height: 44rpx;
}
.search-history .history-list .list-item{
  padding: 15rpx 30rpx;
  background-color: #e8e8e8;
  border-radius: 30rpx;
  color: #666;
  font-size: 26rpx;
  margin: 28rpx 20rpx 0 0;
}
/* 上传户型模块样式 */
.label {
    padding-left: 31rpx;
    padding-right: 26rpx;
    width: 120rpx;
}
.my-container {
    position: absolute; 
    /* left:0;
    top:0; */
    background-color: #eee;
    padding:0 20rpx 0 20rpx;
    width: 100%;
    box-sizing: border-box;
}
.selectBody {
    font-size: 28rpx;
    color: #999999;
    background-color: #fff;
    width: 100%;
    padding: 50rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    padding-top:20rpx;
}
.selectContext{
background: #fff;
}
input {
    color: #333333;
}
.citypickers {
    position: fixed;
    height: 100%;
    width: 100%;
    min-height: 100%;
    background-color: red;
}
.citypicker {
    font-size: 34rpx;
}
.citybody {
    background:#fff;
    position: fixed;
    bottom: 0px;
    z-index: 2;
}
.maskLayer{
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1;
    background: rgba(0,0,0,.5);
}
.cityheader {
    position: absolute;
    top: 0px;
    width: 100%;
    z-index: 4;
    font-size: 34rpx;
}
.city-cancel {
    float: left;
    margin: 30rpx;
    color: #818181;
    font-size: 34rpx;
}
.city-true {
    float: right;
    margin: 30rpx;
    color: #ff6419;
    font-size: 34rpx;
}
.section .picker {
    background-color: #fff;
    border-bottom: 1rpx #d9d9d9 solid;
    border-top: 1rpx #d9d9d9 solid;
    padding: 20rpx;
}
.cell_select {
    padding: 30rpx 0;
    width: 100%;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    border-bottom: 1rpx solid #ddd;
}
.section {
    padding-top: 20rpx;
}
.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}
.userinfo-nickname {
    color: #aaa;
}
.usermotto {
    margin-top: 200px;
}
.type-button {
    margin-top: 40rpx;
    height: 80rpx;
}
.btn {
    position: relative;
    display: block;
    width: 610rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 39rpx;
    color: #fff;
    background-color: #ff6419;
    border:none;
}
button[type=primary] {
    color: #FFFFFF;
    background-color: #ff6419;
}
.flex_iteam.active {
    color: #ff6419;
}
.type-search-space {
    width: 100%;
    background: #fff;
}
.space-type {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    height: 89rpx;
}
.space-gird{
    width: 100%;
}
.space-gird-scroll{
    max-height: 320rpx;
}
.square .space-gird-scroll{
    max-height: 530rpx;
}
.space-type-iteam {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    font-size:28rpx;
    text-align: center;
    height: 89rpx;
    line-height: 89rpx;
    overflow: hidden;
}
.gird-view-iteam{
    float: left;
    margin-left:27rpx;
    margin-bottom: 20rpx;
    width: 216rpx;
    height: 80rpx;
    border-radius: 5rpx;
    background: #f5f5f5;
    text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;
}
.space-type-iteam.active{
    color: #ff6419;
}
.gird-view-iteam.active{
     color: #ff6419;
}






    <view>找装修</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/decoration/supplyList/supplyList?id=2,5&' data-type="信息栏">
    <image src='../../static/image/info.png'></image>
    <view>信息栏</view>
  </view>
</view>

<view class='caseMain'>
    <view class='title'>
        装修案例
        <view class='showMore' bindtap='toCase'>查看更多
            <image src='../../static/image/more.png'></image>
        </view>
    </view>
    <view class='caseBody'>
        <view class='items' wx:for="{{caseObj}}" wx:key="key" wx:for-index="key"  wx:for-item="value">
            <view class='typeName' wx:if='{{value.length > 0 && ( key=="spaceTypeVList3" ||key=="spaceTypeVList4" || showType)}}'>{{conditionNameList[key]}}</view>
            <scroll-view scroll-x='true' wx:if='{{value.length > 0 && (key=="spaceTypeVList3" ||key=="spaceTypeVList4" || showType)}}'>
                <view class="items-item scroll-item"  wx:for='{{value}}' wx:for-item="val" wx:key='{{index}}'>
                        <case-list
                        recommendCaseList='{{[val]}}' 
                        border='{{true}}' 
                        isHomeCase='{{true}}'
                        showCopyRightPrice='{{true}}'
                        bindshowDecoratePriceBox='showDecoratePriceBox'></case-list>
                </view>
            </scroll-view>
        </view>
        <view class='showType' bindtap='changeType'>{{showType?'收起分类':'更多分类'}} 
            <image class='{{showType?"active":""}}' src='../../static/image/more.png'></image>
        </view>
    </view>
    
</view>

<view class='showMoreView' wx:if='{{popularList1.length>0}}'>
    <view class='title'>
        设计大咖
        <view class='showMore' bindtap='navigatTo' data-url='/pages/decoration/brandList/brandList' data-tab='3' data-name="设计师">查看更多
            <image src='../../static/image/more.png'></image>
        </view>
    </view>
    <swiper>
        <swiper-item class="scroll-view-item swiper-cover" wx:for='{{popularList1}}' wx:key='{{index}}'>
            <view wx:for="{{item}}" wx:for-item="popular" wx:for-index="i" wx:key="{{i}}" catchtap='toDetail' data-id='{{popular.shopId}}'>
                <image src='{{resourcePath+popular.picPath}}'></image>
                <view class='views1'>
                    <view class='p1'>{{popular.shopName}}</view>
                </view>
            </view>
        </swiper-item>
    </swiper>

    <image src='../../static/image/furnishing.png'></image>
    <view>买家居</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/brandHall/bHSearchRes/bHSearchRes?id=1000'>
    <image src='../../static/image/material.png'></image>
    <view>买建材</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/brandHall/bHSearchRes/bHSearchRes?id=4000'>
    <image src='../../static/image/appliance.png'></image>
    <view>买电器</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/decoration/brandList/brandList?tab=2&name=家居建材&code='>
    <image src='../../static/image/store.png'></image>
    <view>找门店</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/decoration/brandList/brandList?tab=3&name=设计师&code='>
    <image src='../../static/image/design.png'></image>
    <view>找设计</view>
  </view>
  <view class='items' catchtap='navigat' data-url='/pages/decoration/brandList/brandList?tab=5&name=装修公司&code='>

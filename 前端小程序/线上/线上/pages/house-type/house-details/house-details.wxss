.space-type {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx 30rpx 26rpx;
  background-color: #fff;
}

.type-label {
  font-size: 28rpx;
}

.type-name {
  font-weight: bold;
}

.show3d {
  width: 160rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
  color: #fff !important;
  border-radius: 30rpx !important;
  background-color: #29cccc !important;
}

.panorama {
  padding: 0 30rpx 20rpx;
  background-color: #fff;
}

.panorama-content {
  position: relative;
  height: 517.5rpx;
  /* overflow: hidden; */
}

.panorama-pic {
  width: 100%;
  height: 517.5rpx;
  border-radius: 5px;
}

.question,
.enlarge {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width: 48rpx;
  height: 48rpx;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-size: cover;
  z-index: 10;
}

.question {
  right: 70rpx;
}

.enlarge {
  right: 10rpx;
}

.whole-house {
  position: absolute;
  right: 16rpx;
  bottom: 16rpx;
  width: 64rpx;
  height: 36rpx;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-size: cover;
}

.localtion-label-bg {
  position: absolute;
  width: 72rpx;
  height: 72rpx;
  opacity: 0.6;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: cover;
  transform: translate(-42rpx, -45rpx);
  transform-origin: left top;
}

.localtion-label {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: cover;
  transform: translate(-30rpx, -35rpx);
  transform-origin: left top;
}

.recom-type {
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  line-height: 88rpx;
  padding: 0 190rpx;
  background-color: #fff;
}

.recom-type-text {
  position: relative;
  font-size: 32rpx;
}

.recom-type-text.active {
  font-weight: bold;
}

.recom-type-text.active::after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  width: 40rpx;
  height: 6rpx;
  background-color: #29cccc;
  border-radius: 3rpx;
  transform: translateX(-50%);
}

.recom-condition {
  position: relative;
  border-top: 1rpx solid #efefef;
  background-color: #fff;
}

.condition-nav {
  display: flex;
  justify-content: space-between;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  padding: 0 30rpx;
}

.condition-nav-item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-basis: 30%;
}

.condition-nav-icon {
  width: 18rpx;
  height: 10rpx;
  margin-left: 15rpx;
}

.condition-list {
  position: absolute;
  top: 89rpx;
  left: 0;
  width: 100%;
  max-height: 0;
  opacity: 0.5;
  overflow: hidden;
  transition: all .2s ease-in;
  z-index: 10;
}

.condition-list.active {
  max-height: 700rpx;
  opacity: 1;
}

.condition-list-content {
  display: flex;
  flex-wrap: wrap;
  padding: 30rpx 30rpx 10rpx;
  border-top: 1rpx solid #efefef;
  background-color: #fff;
}

.condition-list-item {
  flex-basis: 32%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #333;
  text-align: center;
	background-color: #f5f5f5;
	border-radius: 5rpx;
  margin-bottom: 20rpx;
}

.condition-list-item:not(:nth-of-type(3n+3)) {
  margin-right: 2%;
}

.condition-list-item.active {
  color: #29cccc;
  background-color: #ebffff;
}

.recom-list {
  padding: 30rpx;
}

.dialog,
.large-pic {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 15;
}

.dialog {
  opacity: 0;
  transform: scale(0);
  transition: all 100ms ease-in;
}

.dialog.active {
  opacity: 1;
  transform: scale(1);
}

.dialog-content,
.render-content,
.select-space {
  width: 540rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.dialog-body {
  display: flex;
  flex-direction: column;
  padding: 60rpx 60rpx 40rpx;
}

.dialog-title {
  line-height: 30rpx;
  margin-bottom: 38rpx;
  font-size: 36rpx;
  font-weight: 600;
  text-align: center;
  color: #29cccc;
}

.dialog-msg {
  font-size: 28rpx;
  line-height: 48rpx;
}

.dialog-footer {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  border-top: 1rpx solid #efefef;
}

.new-plan,
.replace-plan {
  flex-basis: 50%;
  text-align: center;
  box-sizing: border-box;
}

.new-plan {
  border-right: 1rpx solid #efefef;
}

.replace-plan {
  color: #29cccc;
}

.render-content {
  position: relative;
}

.close-render {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width: 48rpx;
  height: 48rpx;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  background-size: cover;
}

.render-body {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 60rpx 85rpx;
}

.render-loading {
  text-align: center;
}

.loading-img {
  width: 240rpx;
  height: 240rpx;
}

.render-tip {
  font-size: 28rpx;
  color: #333;
  white-space: nowrap;
}

.render-extrainfo {
  line-height: 54rpx;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #999;
}

.render-btn {
  width: 300rpx;
	height: 80rpx;
  line-height: 80rpx;
  margin-top: 50rpx;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
	background-color: #29cccc;
	border-radius: 40rpx;
}

.space-title {
  margin-top: 60rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
}

.space-body {
  position: relative;
  padding: 40rpx;
}

.space-img {
  width: 100%;
  height: 345rpx;
  border-radius: 6rpx;
}

.space-footer {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  border-top: 1rpx solid #efefef;
}

.space-cancel,
.space-render {
  flex-basis: 50%;
  text-align: center;
  font-size: 32rpx;
}

.space-render {
  border-left: 1rpx solid #efefef;
  color: #29cccc;
}

.large-pic {
  display: flex;
  align-items: center;
  opacity: 0;
  background-color: #000;
  transform: scale(0);
  transition: all 0.2s ease-in;
}

.large-pic.active {
  opacity: 1;
  transform: scale(1);
}

.close-mask {
  display: flex;
  align-items: center;
  position: absolute;
  top: 40rpx;
  left: 20rpx;
  height: 50rpx;
  font-size: 28rpx;
  color: #fff;
  z-index: 10;
}

.close-mask-img {
  width: 50rpx;
  height: 50rpx;
}

.moveBg {
  width: 100%;
  height: 562.5rpx;
  /* height: 100%; */
}

.moveview {
  width: 100%;
  height: 562.5rpx;
}

.large-content {
  position: relative;
  width: 100%;
  /* transition: all 0.2s ease-in; */
}

.large-img {
  width: 100%;
  height: 562.5rpx;
}

.salcePic {
  display: flex;
  align-items: center;
  position: absolute;
  right: 100rpx;
  bottom: 24rpx;
}

.salcePic.only {
  right: 20rpx;
}

.subtract,
.increase {
  width: 48rpx;
  height: 48rpx;
  margin-right: 12rpx;
}

.large-whole-btn {
  position: absolute;
  right: 30rpx;
  bottom: 30rpx;
  width: 64rpx;
  height: 36rpx;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-size: cover;
}

.coordinateDesc {
  width: 540rpx;
	height: 510rpx;
  padding: 50rpx 60rpx;
	border-radius: 10rpx;
	background-color: #fff;
  box-sizing: border-box;
}

.desc-title {
  text-align: center;
  font-weight: bold;
  font-size: 32rpx;
  color: #333;
}

.desc-body {
  display: flex;
  flex-direction: column;
  margin: 20rpx 0;
}

.desc-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}

.desc-icon {
  width: 48rpx;
  height: 48rpx;
}

.splite-line {
  width: 185rpx;
  border-bottom: 1rpx dashed #ccc;
}

.desc-text {
  font-size: 24rpx;
  color: #333;
}

.desc-footer {
  display: flex;
  justify-content: center;
}

.desc-footer-btn {
  width: 420rpx;
	height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
	border-radius: 40px;
	background-color: #29cccc;
}

.emptyImage{
  width: 690rpx;
	height: 662rpx; 
  margin-left: 30rpx;
  text-align: center;
}
.emptyImage image{
  height: 280rpx;
  width: 420rpx;
  margin-bottom: 50rpx;
}
.emptyImage .txt{
  font-size: 26rpx;
  color: #999999;
  line-height: 50rpx;
}
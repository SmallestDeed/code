view {
    box-sizing: border-box;
}

.upload-input {
    color: #999;
    font-size: 28rpx;
    z-index: 0;
}

.upload-house {
    width: 100%;
    background-color: #ededed;
    padding-bottom: 140rpx;
    position: relative;
}

.upload-house .house-title {
    width: 750rpx;
    height: 80rpx;
    background-color: #fff5f0;
    color: #ff6419;
    font-size: 24rpx;
    line-height: 80rpx;
    text-align: center;
}

/* 户型主要信息模块 */

.upload-house .house-address {
    background-color: #fff;
    padding-left: 30rpx;
    line-height: 88rpx;
}

.upload-house .house-address text {
    color: #333;
    font-size: 28rpx;
    margin-right: 28rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
    position: relative;
}

.upload-house .house-address text text {
    margin: 0 !important;
    padding: 0 !important;
    display: block;
    width: 30rpx;
    height: 100%;
    line-height: 88rpx;
    position: absolute;
    left: 0;
    top: 0;
    color: red;
    font-size: 30rpx;
}

.upload-house .house-address input {
    width: 549rpx;
    height: 88rpx;
    font-size: 28rpx;
    position: relative;
    z-index: 3;
}

.house-address .common {
    border-bottom: solid #e8e8e8 1rpx;
}

/* 户型上传图片模块 */

.upload-house .house-message {
    background-color: #fff;
    margin-top: 20rpx;
    padding-left: 30rpx;
    padding-top: 30rpx;
}

.upload-house .message-pictrue {
    padding-bottom: 30rpx;
    padding-left: 169rpx;
    position: relative;
    border-bottom: #e8e8e8 solid 1rpx;
}

.message-pictrue .pictrue-text {
    position: absolute;
    left: 0;
    top: 0;
    /* margin-right: 88rpx; */
    color: #333;
    font-size: 28rpx;
}

.message-pictrue .pictrue-img image {
    width: 160rpx;
    height: 160rpx;
    position: relative;
    z-index: 3;
}

.pictrue-reminder {
    margin-top: 10rpx;
}

.pictrue-reminder .reminder-img {
    width: 160rpx;
    height: 160rpx;
    margin-right: 20rpx;
}

.pictrue-reminder .reminder-explain {
    width: 326rpx;
}

.reminder-img image {
    width: 100%;
    height: 100%;
}

.reminder-explain view {
    font-size: 22rpx;
    color: #999;
    line-height: 41rpx;
}

.reminder-explain text {
    color: #ff6419;
}

.reminder-explain .explain-main {
    color: #ff6419;
    line-height: 41rpx;
    font-size: 24rpx;
}

.message-describe {
    margin-top: 30rpx;
}

.message-describe .describe-text {
    margin-right: 59rpx;
    color: #333;
    font-size: 28rpx;
}

.message-describe {
    height: 290rpx;
    position: relative;
}

.message-describe view {
    position: relative;
    z-index: 1;
}

.message-describe view.titles {
    position: absolute;
    z-index: 0;
    width: 530rpx;
    top: -4rpx;
    left: 170rpx;
    color: #999;
    font-size: 28rpx;
    height: 290rpx;
}

.message-describe .describe-textarea textarea {
    width: 480rpx;
    height: 290rpx;
    font-size: 28rpx;
    position: relative;
}

.rows {
    width: 100%;
    height: 100%;
    position: fixed;
    box-sizing: border-box;
    left: 0;
    bottom: 0;
}

.house-submit {
    /* margin-top: 60rpx; */
    width: 100%;
    height: 120rpx;
    background-color: #fff;
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 10;
}

.house-submit .submit-btn {
    width: 690rpx;
    height: 80rpx;
    position: fixed;
    box-sizing: border-box;
    left: 30rpx;
    bottom: 20rpx;
    background-color: #ff6419;
    text-align: center;
    font-size: 32rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    color: #fff;
}

.house-submit .submit-text {
    margin-top: 30rpx;
    text-align: center;
    color: #999;
    font-size: 24rpx;
}

/* 地址选择模块 */

.house-provincial {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 5;
}

.provincial-linkage {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 750rpx;
    height: 600rpx;
    color: #000 !important;
    text-align: center;
    font-size: 28rpx;
}

.provincial-linkage .controller {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    display: flex;
    border-bottom: solid 1px #ededed;
    background-color: #fff;
}

.provincial-linkage .controller view:nth-child(1) {
    flex: auto;
    padding-left: 30rpx;
    box-sizing: border-box;
    text-align: left;
}

.provincial-linkage .controller view:nth-child(2) {
    color: #ff6419;
    flex: auto;
    padding-right: 30rpx;
    box-sizing: border-box;
    text-align: right;
}

/* 修改选中框的颜色 */

picker-view {
    background-color: #fff;
}

.zzc {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1000;
}

.navMain {
    display: inline-block;
    min-width: 60rpx;
    height: 98rpx;
    right: 0rpx;
    bottom: 164rpx;
    position: fixed;
    z-index: 1001;
}
.navMain .navBody{
    width: 480rpx;
    height: 98rpx;
    padding:12rpx 66rpx 18rpx 34rpx;
    background: #fff;
    position: absolute;
    right: -480rpx;
    top: 0;
    transition-duration: .2s;
    border-radius: 10rpx 0 0 10rpx;
}
.navMain .navBody.active{
    right: 0;
}
.navMain .navBody image.close{
    width: 28rpx;
    height: 28rpx;
    position: absolute;
    right: 16rpx;
    top: 35rpx;
}
.navMain .navBody::after{
    content: '';
    position: absolute;
    top: 0;
    right: 60rpx;
    background: #e8e8e8;
    width: 2rpx;
    height: 100%;
}
.navMain .navBody>view{
    height: 100%;
    box-sizing: border-box;
}
.navMain .navImg {
    width: 60rpx;
    height: 98rpx;
    display: block;
    position: absolute;
    right: 0;
    top: 0;
}
.closeView{
    width: 60rpx;
    height: 98rpx;
    position: absolute;
    top: 0;
    right: 0;
}
.navMain .navBody>view.items{
    display: block;
    float: left;
    width: 48rpx;
    text-align: center;
    font-size: 22rpx;
    margin-right: 28rpx;
    color: #333333;
    box-sizing: border-box;
}
.navMain .navBody>view.items image{
    display: block;
    width: 48rpx;
    height: 48rpx;
    margin: 0 auto;
}
.navMain .navBody>view.items view{
    line-height: 1;
}
/* pages/decoration/supplyDetail/supplyDetail.wxss */
page{
  background-color: #ffffff;
}
.tilie{
  position: relative;
  padding: 40rpx 0rpx 60rpx 0;
  border-top: 1rpx solid #e8e8e8;
  font-size: 40rpx;
  font-weight: bold;
  line-height: 60rpx;
  overflow: hidden;
  text-indent:30rpx;
  min-height: 60rpx;
}
.tilie-icon{
  position: absolute;
  top: 51rpx;
  left: 30rpx;
  width: 40rpx;
	height: 40rpx;
	border-radius: 2rpx;
}
.announce-info{
  padding: 0 30rpx 40rpx;
  font-size: 24rpx;
  color: #999999;
}
.time{
  display: flex;
  justify-content: flex-start;
  
  line-height: 60rpx;
}
.time view{
  margin: 0 10rpx;
}
.separation{
  width: 3rpx;
  height: 25rpx;
  background-color:  #999999;
  margin: 0 20rpx;
}
.site{
  margin-top: 23rpx;
}
.name{
  height: 60rpx;
  font-size: 24rpx;
  color: #333333;
  line-height: 60rpx;
  font-weight: bold;
  display: flex;
  justify-content: flex-start;
 
}
.uname{
   overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 40%;
}
.nickname{
  width: 60rpx;
  height: 60rpx;
  margin-right: 15rpx;
  background-color: #f5f5f5;
  border-radius: 50%;
}
.message{
  font-size: 28rpx;
  line-height: 48rpx;
  color: #333333;
  margin-bottom: 35rpx;
  margin-top: 40rpx;
}
.message-img{
  width: 100%;
  border-radius: 10rpx;
}
.btn-box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  background-color: #fff;
}
.btn{
  width: 670rpx;
	height: 80rpx;
  margin: 40rpx auto;
	background-color: #f7f7f7;
	border-radius: 40rpx;
  display: flex;
  justify-content: center;
}
.btn image{
  width: 28rpx;
  height: 25rpx;
  margin: 29rpx 10rpx 0 0;
}
.btn-text{
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  line-height: 80rpx;
}
.contact-btn{
  width: 670rpx;
	height: 80rpx;
  margin: 40rpx auto;
  border-radius: 40rpx;
  background-color: #ff6419;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.bottomBar{
  min-height: 98rpx;
  width: 750rpx;
  background-color: #ffffff;
  position: fixed;
  bottom: 0;
  border-top: 1rpx solid #e8e8e8;
  z-index: 11
}
.bottomBar .top{
  display: flex;
}
.ppg_search {
    width:320rpx;
    display: block;
    height: 64rpx;
    background-color: #f5f5f5;
    border-radius: 32rpx;
    margin-left: 30rpx;
    margin-top: 17rpx;
    /* border: 1rpx solid #000; */
}

.ppg_search .input {
  margin-left: 65rpx;
  height:64rpx;
  line-height:64rpx;
  border:none;
  background:none;
  color:#666 !important;
  outline:none;
  font-size:24rpx;
  margin-top: 17rpx;
  width:280rpx
}
.ppg_search2 {
    width:610rpx;
    min-height: 64rpx;
    display: block;
    background-color: #f5f5f5;
    border-radius: 32rpx;
    margin-left: 30rpx;
    margin-top: 17rpx;
    margin-bottom:17rpx;
    padding: 30rpx 0;
    /* border: 1rpx solid #000; */
}

.ppg_search2 .input {
  margin-left: 65rpx;
  width: 480rpx;
  line-height:64rpx;
  border:none;
  background:none;
  color:#666 !important;
  outline:none;
  font-size:24rpx;
  max-height: 300rpx;
  margin-top: 17rpx
}
.textarea{
  text-align: center;
}
.pageview-box-right{
  margin-top: 25rpx;
  margin-left: 40rpx;
}
.pageview-box-right .collet {
  margin-right:45rpx;
}
.pageview-box-right .thumbs-up{
  vertical-align:middle;
  margin-right:45rpx;
}
.pageview-box-right image{
  width:47rpx;
  height: 47rpx;
  float: left;
}
.pageview-box-right text {
  color:#999999;
  font-size: 24rpx;
  line-height: 47rpx;
  float:left;
}


 /* 户型、方案 */
 .list{
  height: 640rpx;
  border-radius: 8rpx;
  width: 690rpx;
  margin-top:20rpx;
}
.scroll{
  margin-top: 40rpx;
  
}
.listView{
  display: flex;
  flex-direction: row;
}
.list .top{
  width: 690rpx;
	height: 518rpx;
	background-color: #e0e0e0;
  border: 1rpx solid #e8e8e8;
	border-radius: 8rpx 8rpx 0rpx 0rpx;
}
.list .top image{
  width: 690rpx;
	height: 518rpx;
  border-radius:8rpx 8rpx 0rpx 0rpx;

}
.list .top2{
  width: 690rpx;
	height: 388rpx;
	background-color: #e0e0e0;
  
  border: 1rpx solid #e8e8e8;
	border-radius: 8rpx 8rpx 0rpx 0rpx;
}
.list .top2 image{
  width: 690rpx;
	height: 388rpx;
}
.list .bottom{
  width: 690rpx;
	height: 138rpx;
	background-color: #fff;
	border-radius: 0rpx 0rpx 18rpx 18rpx;
  display: flex;
  flex-direction: row;
  border: 1rpx solid #e8e8e8;
}
.list .bottom .left{
  height: 138rpx;
  width: 280rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  display: flex;
  flex-direction: column;
}
.list .bottom .left .tit{
  font-size: 32rpx;
  color: #403834;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
margin-top: 10rpx;
}
.list .bottom .left .area{
  font-size: 24rpx;
  color: #999999;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
margin-top: 15rpx;
}

.list .bottom .right{
  height: 138rpx;
  width: 320rpx;
}
.toHouseDetail{
  width:180rpx;
height:72rpx;
background-color:#29cccc;
border-radius:36rpx;
font-size:28rpx;
color:#ffffff;
margin-top:33rpx;
text-align:center;
line-height:72rpx;
}
.gary{
  width: 750rpx;
  height: 20rpx;
  background-color: #f5f5f5;
}
.addMore{
  height: 60rpx;
  width: 60rpx;
  margin-left: 20rpx;
  margin-top: 19rpx
}
.addType{
  width: 750rpx;
	height: 280rpx;
	background-color: #ffffff;
  display: flex;
  justify-content:space-around;
}
.addType image{
  width: 120rpx;
	height: 120rpx;
  margin-top: 30rpx;
}
.addType .txt{
  font-size: 28rpx
}
.imgList{
  min-height: 160rpx;
  width: 550rpx;
  border-bottom:1rpx solid #e0e0e0;
  margin-left:30rpx;
  margin-bottom:10rpx;
  display:flex;
  flex-wrap:wrap;
}
.imgList .element{
  display: flex;
  width: 120rpx;
	height: 150rpx;
  margin-right: 12rpx;
  position:relative;
}
.imgList image{
  width: 120rpx;
	height: 120rpx;
}
.imgList .close{
  height: 44rpx;
  width: 44rpx;
  position:absolute;
  right:-22rpx;
  top:-20rpx;
}
.comment{

}
.comment .top{
  height: 90rpx;
  width: 690rpx;
  background-color: #fff;
  padding:0rpx 30rpx;
  justify-content: cneter;
  line-height: 90rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.comment .top .left{
  font-size: 32rpx;
  color: #333333;
}
.comment .top .right{
  font-size: 28rpx;
  color: #999999;
}
.content {
  width: 690rpx;
  margin-left: 30rpx;
  padding: 40rpx 0;
   border-bottom: 1rpx solid #e8e8e8;
}
.content .info{
  height: 68rpx;
  width: 690rpx;
  display: flex;
  justify-content:space-between;
}
.content .info .personal{
  display: flex;
}
.content .info .personal .head{
  width: 68rpx;
	height: 68rpx;
}
.content .info .personal .other{
  height: 68rpx;
  margin-left: 20rpx;
}
.other .username{
  color: #333333;
  font-size: 28rpx;
}
.other .data{
  color: #999999;
  font-size: 22rpx;
}
.like{
  height: 68rpx;
  display: flex;
}
.like image{
  height: 47rpx;
  width: 47rpx;
  margin-top: 10rpx;
}
.like .txt{
  font-size: 24rpx;
  margin-left: 5rpx;
  line-height: 68rpx;
}
.content .textCot{
  width: 602rpx;
  margin-left: 88rpx;
  word-wrap:break-word;
  margin-top: 30rpx;
}
.content .img{
  margin-left: 88rpx;
  margin-top: 30rpx;
}
.content .img image{
  width: 180rpx;
	height: 180rpx;
  margin-right:15rpx;
}
.content .house{
  margin-left: 88rpx;
  margin-top: 30rpx;
  position: relative;
}
.content .house .image{
  width: 600rpx;
	height: 450rpx;
}
.content .plan{
  margin-left: 88rpx;
  margin-top: 30rpx;
  position: relative;
}
.content .plan .image{
  width: 600rpx;
	height: 450rpx;
}
.content .tag{
   position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 180rpx;
	height: 68rpx;
  opacity: 0.9;
}

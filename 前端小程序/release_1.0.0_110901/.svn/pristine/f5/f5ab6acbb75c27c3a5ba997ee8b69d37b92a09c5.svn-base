/* pages/decoration/supplyDetail/supplyDetail.wxss */
page{
  background-color: #ffffff;
}
.tilie{
  position: relative;
  padding: 40rpx 30rpx 60rpx 30rpx;
  border-top: 1rpx solid #e8e8e8;
  font-size: 40rpx;
  font-weight: bold;
  line-height: 60rpx;
  overflow: hidden;
  min-height: 60rpx;
}
.tilie-icon{
  position: absolute;
  top: 51rpx;
  left: 30rpx;
  width: 40rpx;
	height: 40rpx;
	border-radius: 2rpx;
}
.announce-info{
  padding: 0 30rpx 40rpx;
  font-size: 24rpx;
  color: #999999;
}
.time{
  display: flex;
  justify-content: flex-start;
  
  line-height: 60rpx;
}
.time view{
  margin: 0 10rpx;
}
.separation{
  width: 3rpx;
  height: 25rpx;
  background-color:  #999999;
  margin: 0 20rpx;
}
.site{
  margin-top: 23rpx;
}
.name{
  height: 60rpx;
  font-size: 24rpx;
  color: #333333;
  line-height: 60rpx;
  font-weight: bold;
  display: flex;
  justify-content: flex-start;
 
}
.uname{
   overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  width: 40%;
}
.nickname{
  width: 60rpx;
  height: 60rpx;
  margin-right: 15rpx;
  background-color: #f5f5f5;
  border-radius: 50%;
}
.message{
  font-size: 28rpx;
  line-height: 48rpx;
  color: #333333;
  margin-bottom: 35rpx;
  margin-top: 40rpx;
}
.message-img{
  width: 100%;
  border-radius: 10rpx;
}
.btn-box{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  background-color: #fff;
}
.btn{
  width: 670rpx;
	height: 80rpx;
  margin: 40rpx auto;
	background-color: #f7f7f7;
	border-radius: 40rpx;
  display: flex;
  justify-content: center;
}
.btn image{
  width: 28rpx;
  height: 25rpx;
  margin: 29rpx 10rpx 0 0;
}
.btn-text{
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  line-height: 80rpx;
}
.contact-btn{
  width: 670rpx;
	height: 80rpx;
  margin: 40rpx auto;
  border-radius: 40rpx;
  background-color: #ff6419;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  text-align: center;
}
.bottomBar{
  min-height: 98rpx;
  width: 750rpx;
  background-color: #ffffff;
  position: fixed;
  bottom: 0;
  border-top: 1rpx solid #e8e8e8;
  z-index: 11
}
.bottomBar .top{
  display: flex;
}
.ppg_search {
    width:320rpx;
    display: block;
    height: 64rpx;
    background-color: #f5f5f5;
    border-radius: 32rpx;
    margin-left: 30rpx;
    margin-top: 17rpx;
    /* border: 1rpx solid #000; */
}

.ppg_search .input {
  height:64rpx;
  line-height:64rpx;
  border:none;
  background:none;
  color:#666 !important;
  outline:none;
  font-size:24rpx;
  width:280rpx;
  margin-left: 40rpx
}
.ppg_search2 {
    width:610rpx;
    min-height: 64rpx;
    display: block;
    background-color: #f5f5f5;
    border-radius: 32rpx;
    margin-left: 30rpx;
    margin-top: 17rpx;
    margin-bottom: 17rpx;
    padding: 30rpx 0;
}

.ppg_search2 .input {
  width: 480rpx;
  height: 64rpx;
  line-height:64rpx;
  border:none;
  background:none;
  color:#666 !important;
  outline:none;
  font-size:24rpx;
  max-height: 300rpx;
  margin-left:40rpx;
}
.textarea{
  text-align: left;
}
.pageview-box-right{
  margin-top: 25rpx;
  margin-left: 32rpx;
  width: 365rpx;
  justify-content: space-around;
}
.pageview-box-right .collet {
  margin-right:45rpx;
}
.pageview-box-right .thumbs-up{
  vertical-align:middle;
  margin-right:35rpx;
}
.pageview-box-right image{
  width:60rpx;
  height: 60rpx;
  float: left;
}
.pageview-box-right text {
  color:#999999;
  font-size: 24rpx;
  line-height: 60rpx;
  float:left;
}



/**index.wxss**/
/* 错误示范 */
/* .index-first {
  text-align: center;
} */
/* 正确写法，要有层级关系 */
.img{
  width: 100% !important;
  object-fit: contain;
  border-radius: 6rpx;
  margin-bottom: 20rpx;
  margin-top: 20rpx;
}
.index .index-first {
  text-align: center;
}
.index .index-second {
  text-align: center;
}
.index .index-three{
  width: 100%;
  height: 400rpx;
}
.index .imgwidth{
  width: 100%;
}
.imgwidth image{
  width: 100%;
  height: 400rpx;
}
.intro{
  margin: 30rpx;
  padding: 40rpx;
  background-color: #ffffff;
	box-shadow: 0rpx 0rpx 40rpx 0rpx rgba(204, 204, 204, 0.4);
	border-radius: 6rpx;
  display: flex;
  justify-content: flex-start;
}
.logo{
  width: 140rpx;
	height: 140rpx;
  border-radius: 50%;
}
.intro-message{
  margin-left: 30rpx;
  font-size: 36rpx;
  color: #333333;
}
.intro-message .name{
  width: 400rpx;
  min-height: 30rpx;
  margin-top: 20rpx;
  font-weight: bold;
}
.intro-message .num{
  margin-top: 25rpx;
  font-size: 24rpx;
  color: #999999;
}

.icon-contact{
  position: fixed;
  right: 30rpx;
  bottom: 240rpx;
  z-index: 2;
  width: 80rpx;
	height: 80rpx;
  border-radius: 50%;
	background-color: #ffffff;
	box-shadow: 0rpx 0rpx 40rpx 0rpx rgba(204, 204, 204, 0.4);
}
.icon-contact image{
  width: 56rpx;
  height: 56rpx;
  margin: 14rpx 0 0 14rpx;  
}
.icon-phone{
  position: fixed;
  right: 30rpx;
  bottom: 140rpx;
  z-index: 2;
  width: 80rpx;
	height: 80rpx;
  border-radius: 50%;
	background-color: #ffffff;
	box-shadow: 0rpx 0rpx 40rpx 0rpx rgba(204, 204, 204, 0.4);
}
.icon-phone image {
  width: 56rpx;
  height: 56rpx;
  margin: 14rpx 0 0 14rpx;  
}

.swiperStyle{
  height:400rpx; 
  background:url('/static/images/design_def.png') no-repeat 100% 100%;
}
.topBanner{
  width: 750rpx;
  height: 400rpx;
  display: flex;
}

.topBanner image{
  width: 750rpx;
  height: 400rpx;
}
.index .serviceInfo{
  padding:31rpx;
}

.serviceInfo .title,
.caseTeam .title{
  font-size: 36rpx;
  font-weight: bold;
}
.serviceInfo .serviceContent{
  background-color: #ffffff;
  margin:40rpx 0;
  padding:31rpx;
  box-shadow: 0rpx 0rpx 40rpx 0rpx rgba(204, 204, 204, 0.4);
  border-radius: 6rpx;
}

.serviceItem .Item{
  display: flex;
  color: #999;
  font-size: 28rpx;
  margin-bottom:40rpx;
}

.Item .detail{
   color: #000;
   width: 470rpx;
   display: inline-block;
}
.Item .color{
  color: #29cccc
}
.Item .head{
  width: 160rpx;
}
/* 设计师团队 */
.index .designTeam{
  padding:0 31rpx;
  margin-bottom:75rpx;
}
.designTeam .title{
  margin-bottom: 39rpx;
  font-size: 36rpx;
  font-weight: bold;
  display: flex;
  justify-content: space-between;
}
.designTeam .title view{
  min-width: 100rpx;
  line-height: 56rpx;
  font-size: 25rpx;
  font-weight: 	normal;
  color: #666;
  display: flex;
  justify-content: space-between;
}
.designTeam .title image{
  width: 56rpx;
  height: 56rpx;
}
.designTeam .scrollItem{
  text-align: center;
  width: 200rpx;
}
.designTeam .scrollViewContent{
  margin-bottom: 30rpx;
  display: flex;
}
.scrollViewContent .scrollItem{
  margin-right: 30rpx;
}
.scrollItem .imgs{
  background: #f8f8f8;
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
}
.scrollItem .designer{
  margin-top: 18rpx;
  font-size: 28rpx;
  color:#333;
  width: 200rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.scrollItem .designYear{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  margin-top: 8rpx;
  color:#999;
}
/* 工程案例 */
.index .caseTitle{
  margin: 0 30rpx;
  display: flex;
  justify-content: space-between
}
.caseTitle .title{
  font-weight: bold;
  font-size: 36rpx;
}
.index .caseTitle .arrow,
.bowen .title .arrow{
  vertical-align:-18rpx;
  width:56rpx;
  height:56rpx;
  font-size: 28px;
  color: #666;
}
.caseTitle .counter{
  font-size: 25rpx;
  color: #666;
}
.caseTeam{
  
  border-radius: 10rpx;
  margin: 30rpx;
}

.caseTeam .caseShow{
  display: flex;
  width:690rpx;
}
.caseShow .caseItem{
  box-shadow: 0 0 40rpx rgba(204, 204, 204, 0.4);
  margin: 0 auto;
  margin-right: 16rpx;
	border-radius: 6rpx;
  width:690rpx;
}
.index .scrollV{
  height: 600rpx;
}
.caseShow .caseTitle{
  font-weight: bold;
  font-size: 32rpx;
  padding:30rpx 20rpx;
  
}
.caseShow .scrollTitle{
  font-weight: bold;
  font-size: 32rpx;
  height: 98rpx;
  line-height: 98rpx;
  white-space: nowrap;
  width: 93.5%;
  overflow: hidden;
  padding: 0 20rpx;
  text-overflow: ellipsis;
  border-radius: 0 0 10rpx 10rpx;
  margin-top:-10rpx;
}
.caseItem .caseImgs{
  border-radius: 6rpx;
}
.caseImgs .imgs{
  width:690rpx;
  height:388rpx;
  margin: 0;
  border-radius:10rpx 10rpx 0 0;
}
/* 博文 */
.index .bowen{
  margin:30rpx 30rpx;
}
.bowen .title{
  display: flex;
  
  justify-content: space-between
}
.title .counter{
  font-size: 28rpx;
  color: #666;
}
.title .BigTitle{
  font-size: 36rpx;
  font-weight: bold;
}

.bowen .bowenList{
  margin-top:40rpx;  
}
.bowenList .bowenImg{
  width: 200rpx;
  height: 150rpx;
  border-radius: 6rpx
}
.bowenList .listItem{
  margin-bottom:60rpx;
  display: flex;
  justify-content: start
}
.listItem .bowenTitle{
  width:446rpx;
  word-break: break-all;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  color:#333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.listItem .bowenInfo{
  margin-left: 31rpx;
}
.listItem .times{
  font-size: 24rpx;
  margin-top:20rpx;
  color: #999;
}
/* 公司介绍 */
.index .companyInfo{
  /* margin: 30rpx; */
  width: 690rpx;
  margin: 0 auto;
}
.companyInfo .title{
  font-size: 34rpx;
  font-weight: bold;
  margin-top: 70rpx;
  margin-bottom:20rpx;
}
.companyInfo .detailInfo{
  width: 100%;
  font-size: 28rpx;
  max-height: 800rpx;
  line-height: 48rpx;
  color:#333;
  overflow: hidden;
  word-break:break-all;
  /* transition: 1s all linear; */
}
.companyInfo .detailInfo-avtive{
  width: 100%;
  font-size: 28rpx;
  line-height: 48rpx;
  color:#333;
  word-break:break-all;
  box-sizing: border-box;
  /* transition: 1s all linear; */
}
.companyInfo .detailInfo-avtive img{
   width: 100%;
   object-fit: contain;
}
.companyInfo .expandFrame{
  text-align: center;
  font-size: 32rpx;
  margin-top: 20rpx;
}
.expandFrame .expInfo{
  width: 32rpx;
  height: 18rpx;
}
/* 公司资质 */
.index .companyzz{
  margin: 30rpx 0;
}
.companyzz .title{
  font-size: 34rpx;
  font-weight: bold;
  margin-left: 30rpx;
}
.companyzz .scrollArea {
  padding:30rpx 0;
  display: flex
}
.scrollArea .scrollItems{
  margin-left: 30rpx;
  width: 400rpx;
	height: 300rpx;
	background-color: #eeeeee;
	box-shadow: 0 0 60rpx	rgba(204, 204, 204, 0.3);
	border-radius: 6rpx;
	border: solid 20rpx #ffffff;
}
 .scrollArea .name{
   padding:20rpx 0 20rpx 50rpx;
   white-space: nowrap;
   text-overflow: ellipsis;
   font-size: 28rpx;
   color: #333;
 }
.scrollItems image{
  width: 400rpx;
	height: 300rpx;
}
/* 合作伙伴 */
.index .partner{
  margin: 20rpx;
}
.partner .title{
  font-size: 34rpx;
  font-weight: bold;
  padding-left: 10rpx;
  margin-bottom: 20rpx;
  color: #333;
}
.partner .partnerItem{
   align-content: center;
   justify-content: space-between;
}
.partnerItem .imgs{
   margin-left:21rpx;
   margin-bottom:21rpx;
  width:160rpx;
  height:160rpx;
  padding:28rpx;
  background-color:#ffffff;
  box-shadow:0 0 40px rgba(204, 204, 204, 0.2);
  border-radius:4px;

}
.index .nomore{
  font-size: 28rpx;
  padding:40rpx 0 60rpx 0;
  text-align: center;
  color: #999
}
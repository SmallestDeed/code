page{
  background-color: #f5f5f5;
}
.seek-house{
  padding: 30rpx 30rpx 0;
}
.seek-box{
  padding:  10rpx 50rpx 50rpx;
  border-radius: 10rpx;
  margin-bottom: 100rpx;
  background-color: #fff;
}
.input-box{
  height: 80rpx;
  margin-top: 40rpx;
  font-size: 28rpx;
  color: #333333;
  border-bottom: 1rpx solid #e8e8e8;
  display: flex;
  justify-content: flex-start
}
.region-icon{
  width: 30rpx;
  height: 30rpx;
  margin-top: 27rpx;
}
.seek-title{
  line-height: 80rpx;
  margin: 0 40rpx 0 10rpx;
  color: #999999;
}
.gray{
  min-width: 300rpx;
  color: #333;
  line-height: 80rpx;
}
.text{
  color: #333;
  line-height: 80rpx;
  font-weight: bold;
}
.input{
  height: 80rpx;
  font-weight: bold;
}
.seek-btn{
	height: 80rpx;
  line-height: 80rpx;
  margin-top: 50rpx;
  font-size: 32rpx;
  font-weight: bold;
  border-radius: 40rpx;
  text-align: center;
  color: #fff;
	background-color: #29cccc;
}
.history-box{

}
.history-title{
  height: 44rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  color: #999999;
  display: flex;
  justify-content: space-between;
}
.history-title image{
  width: 44rpx;
  height: 44rpx;
}
.history-list{
  font-size: 26rpx;
  color: #666666;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.history-list text {
  margin: 0 20rpx 20rpx 0;
  padding: 18rpx 30rpx;
  border-radius: 40rpx;
  background-color: #e8e8e8;
}
.empty{
  margin-top: 300rpx;
  font-size:26rpx;
  color:#999999;
  line-height:50rpx;
  text-align: center;
}
.result-box{
  padding-bottom: 30rpx;
  overflow: hidden;
}
.result-title{
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: #999999;
  display: flex;
  justify-content: space-between;
}
.result-title text{
  width: 200rpx;
  height: 1rpx;
  margin-top: 12rpx;
  background-color: #e1e1e1;
}
.result-list{
  margin-top: 40rpx;
  border-radius: 6rpx;
  background-color: #fff;
}
.result{
  height: 100rpx;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #e8e8e8;
  display: flex;
  justify-content: space-between;
}
.name{
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333333;
}
.quantity{
  line-height: 100rpx;
  font-size: 24rpx;
  color: #8e8e8e;
}
.shade{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background: rgba(000, 000, 000, 0.3)
}
/* 选择器 */
.selector-box{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
}
.selector-header{
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  font-size:34rpx;
  display: flex;
  justify-content: space-between;
}
.cancel{color:#818181;}
.confirm{color: #29cccc;}
.picker-box{
  width: 100%; 
  height: 500rpx;
}
.picker-item{
  line-height: 40px;
  text-align: center;
}
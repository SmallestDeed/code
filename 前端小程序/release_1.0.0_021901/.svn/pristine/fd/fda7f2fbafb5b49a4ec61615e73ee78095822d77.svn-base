page{
  background-color: #f5f5f5;
}
.myself{
  font-size: 32rpx;
  color: #333333;
}
.header{
  position: relative;
	height: 150rpx;
  padding: 65rpx 40rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.left{
  display: flex;
  justify-content: flex-start;
}
.head{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.login-btn{
  position: absolute;
  top: 106rpx;
  left: 216rpx;
  width: 180rpx;
  height: 68rpx;
  line-height: 64rpx;
  margin: 0;
  padding: 0;
  border-radius: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333;
  background-color: #fff;
  border: 1rpx solid #333;
}
button[class="login-btn"]::after {
  border: 0;
}
.message-box{
  width: 400rpx;
  height: 130rpx;
  margin-left: 40rpx;
  padding: 20rpx 0 0; 
}
.name{
  width: 400rpx;
  height: 50rpx;
  font-size: 40rpx;
  font-weight: bold;
}
.money{
  font-size: 32rpx;
  margin-top: 15rpx;
  font-weight: bold;
  color: #ffaa33;
  display: flex;
  justify-content: flex-start;
}
.right-icon{
  position: absolute;
  top: 112rpx;
  right: 30rpx;
  width: 56rpx;
  height: 56rpx;
}
.content{
  position:relative;
  height: 140rpx;
  padding: 0 30rpx 0 40rpx;
  border-bottom: 1rpx solid  #e8e8e8;
  background-color: #ffffff;
  display: flex;
  justify-content: space-between;
}
.my-content{
  line-height: 140rpx;
}
.hint{
  position: absolute;
  top: 65rpx;
  right: 90rpx;
  width: 18rpx;
	height: 18rpx;
  border-radius: 50%;
	background-color: #f94141;
}
.content-icon{
  width: 56rpx;
  height: 56rpx;
  margin-top: 47rpx;
}
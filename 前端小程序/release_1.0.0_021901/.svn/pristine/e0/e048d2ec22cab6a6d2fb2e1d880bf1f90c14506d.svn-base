page{
  background-color: #f5f5f5;
}
.Onekey-finish{

}
.house-type-box{
  height: 80rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #f5f5f5;
  color: #333333;
	background-color: #ffffff;
  display: flex;
  justify-content: center;
}
.house-type{
  white-space: nowrap; 
  width: 100%;
  font-size: 28rpx;
  color: #333333;
}
.house-type-box view{
  display: inline-block;
  line-height: 80rpx;
  margin: 0 15rpx;
}
.blue{
  color: #29cccc;
}
.blueb{
  color: #29cccc;
  font-weight: bold;
}

.house-img{
  white-space:  nowrap; 
  display: block;
  height: 578rpx;
}
.house-img image{
  width: 690rpx;
	height: 518rpx;
  margin: 30rpx;
	border-radius: 10rpx;
}
.plan{
  padding: 0 30rpx;
}
.plan-type-box{
  height: 80rpx;
  margin-bottom: 40rpx;
  display: flex;
  justify-content: space-between
}
.scroll{
  white-space:  nowrap; 
  width: 630rpx;
  font-size: 28rpx;
  color: #333333;
}
.type-item{
  position: relative;
  display: inline-block;
  height: 60rpx;
  margin-right: 50rpx;
  padding-top: 6rpx;
}
.type-item text{
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  display: block;
  width: 40rpx;
	height: 6rpx;
	background-color: #29cccc;
	border-radius: 3rpx;
}
.plan-type-icon{
  width: 48rpx;
  height: 48rpx;
}
.plan-list{
  /* padding-bottom: 30rpx;  */
}
.plan-box{
  margin-bottom: 50rpx;
}
.now-plan-title{
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 25rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  display: flex;
  justify-content: flex-start;
}
.now-plan-title text{
  width: 6rpx;
	height: 30rpx;
  margin-right: 15rpx;
	background-color: #ffaa33;
	border-radius: 2rpx;
}
.now-plan-title view{
  margin-left: 20rpx;
  color:#ff6419;
  font-size:24rpx;
  font-weight: normal;
}
.now-plan-item{
  background-color: #fff;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
}
.shade{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(000, 000, 000, 0.3);
}
.box{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 540rpx;
  border-radius: 10rpx;
  background-color: #fff;
  overflow: hidden;
}
.rendering-icon{
  display:block;
  width:240rpx;
  height:240rpx;
  margin: 60rpx auto 15rpx;
}
.rendering-title{
  font-size:32rpx;
  color:#333333;
  text-align: center;
}
.rendering-text{
  margin:20rpx 0 0;
  font-size:24rpx;
  color:#999;
  text-align: center;
}
.balance{
  margin-top:65rpx;
  font-size:28rpx;
  text-align:center;
  color:#333;
}
.btn-box{
  height:88rpx;
  font-size:32rpx;
  margin-top: 40rpx;
  color:#999;
  border-top: 1rpx solid #e0e0e0;
  overflow:hidden;
  display: flex;
  justify-content: space-between;
}
.btn-box text{
  width: 50%;
  line-height: 88rpx;
  text-align: center;
}
.house-title{
  font-size:36rpx;
  color:#29cccc;
  margin:35rpx auto 15rpx;
  box-sizing:border-box;
  text-align: center;
}
.house-text{
  font-size:28rpx;
  color:#000000;
  text-align: center;
}
.shut-icon{
  position: absolute;
  top: 0;
  right: 0;
  display: block;
  width: 48rpx;
  height: 48rpx;
}
.price{
  margin-top: 60rpx;
  font-size:60rpx;
  color:#29cccc;
  text-align: center;
}
.pay-btn{
  width:420rpx;
  height:80rpx;
  margin: 20rpx auto 40rpx;
  line-height:80rpx;
  background-color:#29cccc;
  border-radius:40rpx;
  text-align:center;
  color:#fff;
}
.pay-icon{
  display: block;
  width: 110rpx;
  height: 120rpx;
  margin: 60rpx auto 50rpx;
}
.filtrate{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  font-size:28rpx;
  background-color: #fff;
}
.filtrate-box{
  padding: 0 30rpx;
}
.filtrate-title{
  height: 80rpx;
  line-height: 60rpx;
  color:#333;
}
.filtrate-title text{
  color:#999;
  font-size:24rpx;
}
.filtrate-list{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.filtrate-list text{
  width:213rpx;
  height:80rpx;
  line-height: 80rpx;
  margin: 0 15rpx 20rpx 0;
  border-radius: 10rpx;
  text-align: center;
  background-color:#f5f5f5;
}
.filtrate-list .bgColor{
  background-color: #94f1f1;
  color: #fff;
}
.filtrate-btn{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  font-size:32rpx;
  border-top: 1rpx solid #e8e8e8;
  display: flex;
  justify-content: flex-start;
}
.filtrate-btn text{
  line-height: 88rpx;
  text-align: center;
  width: 30%;
}
.filtrate-btn .confirm{
  background-color: #29cccc;
  width: 70%;
  color: #fff;
}
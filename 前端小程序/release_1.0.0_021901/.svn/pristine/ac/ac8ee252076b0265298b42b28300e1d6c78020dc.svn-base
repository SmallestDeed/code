<view class='house-list'>
  <view class='house' wx:for='{{houseList}}' wx:key='{{index}}'>
    <image  class='house-img' 
      mode='aspectFill' 
      data-img='{{item.thumbnailPath?resourcePath+item.thumbnailPath:""}}'
      bindtap='examineImg'
      src='{{item.thumbnailPath?resourcePath+item.thumbnailPath:"/static/images/huxing_pic_no.png"}}'></image>
    <view class='message-box'>
      <view class='message'>
        <view class='name ellipsis'>{{item.houseCommonCode}}</view>
        <view class='area ellipsis'>{{item.houseTypeStr}} {{item.totalArea}}㎡</view>
      </view>
      <view class='btn' data-item='{{item}}' bindtap='goOnekeyFinish'>一键装修</view>
    </view>
  </view>
</view>
<empty wx:if='{{isShowEmpty}}'></empty>

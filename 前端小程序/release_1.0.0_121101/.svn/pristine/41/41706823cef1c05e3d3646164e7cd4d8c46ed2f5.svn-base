<view class="head-portrait">
    <view class="head-info" bindtap='toMyAccount' wx:if='{{wxInfo.userInfo}}'>
      <image src="{{wxInfo.userInfo?wxInfo.userInfo.avatarUrl:'/static/image/index.png'}}" class='userImg'></image>
      <view class='p1'>{{wxInfo.userInfo.nickName}}</view>
      <view class='p2'>
        <image src="/static/image/money.png"></image>
        <text>度币：</text>
        {{userInfo.balanceIntegral|Int}}
      </view>
      <image class='goNext' src='/static/image/list_icon_more.png'></image>
    </view>
    
      <view class="head-info"  wx:if='{{wxInfo.userInfo!="" || wxInfo.userInfo=="null"}}'>
        <image src='/static/image/index.png' class='userImg'></image>
        <button class='userInfo' open-type='getUserInfo' bindgetuserinfo='getInfo'>
          点击登录
        </button>
      </view>
      
    
</view>
<view class='h20rpx'></view>

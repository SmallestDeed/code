/*装修服务*/

page {
    background-color: #fff;
    box-sizing: border-box;
    padding-top: 90rpx;
}

image {
    height: 0;
}

view {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.sjsdm_logo {
    border-radius: 4rpx;
}

.header_top {
    margin: auto;
}

.header_top img {
    display: block;
    width: 100%;
    height: auto;
}

/* .main_body{ margin: auto;} */

.header {
    width: 100%;
    text-align: center;
    height: 90rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 6;
    background-color: #fff;
}

.header_title {
    line-height: 90rpx;
    font-size: 32rpx;
    color: #323232;
    width: 50%;
    float: left;
    position: relative;
}

.header_title .txt {
    color: #333;
    margin: 0;
}

.header_active .txt {
    color: #ff6419;
    font-weight: 600;
    margin: 0;
}

.header_active:after {
    content: "";
    position: absolute;
    bottom: 4rpx;
    height: 7rpx;
    left: 50%;
    margin-left: -25rpx;
    background-color: #ff6419;
    display: block;
    width: 50rpx;
    border-radius: 50px;
}

.header_xian {
    height: 32rpx;
    width: 2rpx;
    background: #e8e8e8;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -16rpx;
}

.zxfw_banner {
    position: relative;
    width: 100%;
}

swiper {
    height: 400rpx;
}

.viewRows {
    width: 95%;
    box-sizing: border-box;
    height: 68rpx;
    position: absolute;
    left: 5%;
    bottom: 40rpx;
    z-index: 3;
    display: flex;
    justify-content: flex-start;
}

.viewRows .message{
  width: 68rpx;
  height: 68rpx;
}

.viewRows .seek{
  width: 68rpx;
  height: 68rpx;
  position: absolute;
  right: 40rpx;
}

.search {
    display: inline-block;
    padding: 14rpx 30rpx;
    height: 68rpx;
    background-color: rgba(0, 0, 0, 0.6);
    box-shadow: 0px 4px 40px 0px rgba(140, 129, 122, 0.2);
    border-radius: 50px;
    margin-right: 20rpx;
    font-size: 0;
    position: relative;
    /* top: -22rpx;
    margin-top: -34rpx; */
}

.newsView {
    display: inline-block;
    width: 68rpx;
    height: 68rpx;
    text-align: center;
    background-color: rgba(0, 0, 0, 0.6);
    box-shadow: 0px 4px 40px 0px rgba(140, 129, 122, 0.2);
    border-radius: 50px;
    font-size: 0;
    position: relative;
}

.newsView image {
    width: 35rpx;
    height: 35rpx;
    position: absolute;
    margin: auto;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.search .arrow_down {
    border-top-color: #fff;

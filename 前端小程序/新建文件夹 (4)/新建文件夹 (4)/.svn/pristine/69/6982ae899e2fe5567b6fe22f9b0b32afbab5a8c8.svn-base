.list .title{
  position: relative;
  min-height: 35rpx;
  max-height: 90rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #403834;
  text-indent:42rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.list .title-icon{
  position: absolute;
  top: 4rpx;
  left: 0;
  width: 32rpx;
  height: 32rpx;
}

.list .imagelist {
    display: flex;
    flex-direction: row;
    margin-bottom: 25rpx;
}

.list .imagelist .diagram {
    width: 260rpx;
    height: 180rpx;
    margin-right: 15rpx;
    border-radius: 10rpx;
}

.information {
    margin-left: 10rpx;
    width: 420rpx;
}

.detail {
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 24rpx;
    color: #999;
    margin-top: 15rpx;
}


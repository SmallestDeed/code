.empty-page{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
  width: 100%;
  height: 100%;
  background-color: #fff;
  overflow: hidden;
}
.empty-page image{
  width: 100%;
  margin-top: 220rpx;
}
.text{
  font-size: 28rpx;
  color: #333333;
  margin-top: 30rpx;
  text-align: center;
}
.btn{
  display: inline-block;
  padding: 0 69rpx;
  height: 64rpx;
  line-height: 64rpx;
  margin-top: 40rpx;
  background-color: #ff6419;
	border-radius: 32rpx;
  font-size: 28rpx;
  color: #ffffff;
  margin-left: 50%;
  transform: translateX(-50%);
}
/* pages/mine/myPublish/myPublish.wxss */
.type{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
  height: 88rpx;
  width: 750rpx;
  display: flex;
  flex-direction: row;
  border-top:1rpx solid #f5f5f5;
  background-color: #ffffff;
}

.supplyselect{
  width: 375rpx;
  line-height: 88rpx;
  text-align: center;
  justify-content:center;
  font-size: 32rpx;
  color: #ff6419;
}
.supply{
  width: 375rpx;
  line-height: 88rpx;
  text-align: center;
  justify-content:center;
  font-size: 32rpx;
  color: #333333;
}
.line{
  position: absolute;
  top: 0;
  left: 50%;
  width: 1rpx;
	height: 26rpx;
	background-color: #e8e8e8;
  margin-top: 31rpx;
}
.list{
  padding-top: 103rpx;
}
.list-item{
  height: 272rpx;
  margin-bottom: 16rpx;
  padding: 30rpx 30rpx 0 30rpx;
  background-color: #fff;
}
.item-content{
  height: 150rpx;
  display: flex;
  padding:16rpx;
  background-color: #fafafa;
  justify-content: flex-start;
}
.item-content image{
  width: 200rpx;
  height: 100%;
}
.item-info{
  width: 440rpx;
  margin-left: 20rpx;
  color: #999999;
}
.title{
  width: 440rpx;
  margin-top: 9rpx;
  font-size: 28rpx;
  line-height: 27rpx;
  font-weight: bold;
  color: #333333;
}
.describe{
  width: 440rpx;
  font-size: 24rpx;
   line-height: 23rpx;
  margin-top: 20rpx;
}
.data-box{
  height: 25rpx;
  margin-top: 40rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-start;
}
.glance-over{
  width: 30%;
  height: 25rpx;
  line-height: 24rpx;
  display: flex;
  justify-content: flex-start;
}
.glance-over image{
  width: 25rpx;
  height: 25rpx;
  margin-right: 8rpx;
}
.time{
  width: 30%;
  height: 25rpx;
  line-height: 24rpx;
  display: flex;
  justify-content: flex-start;
}
.time image{
  width: 20rpx;
  height: 20rpx;
  margin-top: 3rpx;
  margin-right: 8rpx;
}
.item-footer{
  height: 28rpx;
  margin-top: 32rpx;
  font-size: 28rpx;
  font-weight: bold;
  position: relative;
  display: flex;
  justify-content: space-between;
}
.midcourt-line{
  position: absolute;
  top: 0;
  left: 50%;
  width: 1rpx;
  height: 28rpx;
  background-color: #e8e8e8;
}
.compile{
  line-height: 28rpx;
  text-align: center;
  width: 50%;
  height: 100%;
}
.putaway{
  line-height: 28rpx;
  text-align: center;
  width: 50%;
  height: 100%;
}
.publish{
  height: 120rpx;
  width: 120rpx;
  position: fixed;
  right: 40rpx;
  bottom: 84rpx;
  z-index: 99;
}
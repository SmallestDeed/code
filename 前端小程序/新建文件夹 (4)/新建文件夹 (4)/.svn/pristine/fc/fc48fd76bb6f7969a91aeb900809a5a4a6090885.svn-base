/* 方案 */
page {
    background-color: #fff;
    box-sizing: border-box;
    height: 100%;
    overflow-y: scroll;
}

.house-case {
    padding-top: 88rpx;
    box-sizing: border-box;
    background-color: #ededed;
}

.navTop {
    width: 100%;
    height: 88rpx;
    position: fixed;
    top: 0;
    left: 0;
    background-color: #fff;
    padding-right: 80rpx;
    box-sizing: border-box;
    z-index: 33;
}

.navTop.showBot::after {
    content: '';
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    background: #e8e8e8;
    height: 2rpx;
    transform: scaleY(0.5);
    z-index: 36;
}

.navTop .showMore {
    position: absolute;
    width: 80rpx;
    height: 88rpx;
    right: 0;
    text-align: center;
    line-height: 88rpx;
    background: #fff;
    z-index: 35;
}

.navTop .showMore image {
    width: 50rpx;
    height: 50rpx;
    margin-top: 19rpx;
    margin-left: 15rpx;
    display: block;
}

.navTop scroll-view {
    width: 100%;
    height: 88rpx;
    position: absolute;
    left: 0;
    z-index: 34;
    box-sizing: border-box;
    padding: 4rpx 0;
    padding-right: 80rpx;
    white-space: nowrap;
}

.navTop scroll-view .items {
    width: 21.3%;
    display: inline-block;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    position: relative;
    color: #000;
    font-size: 28rpx;
}

.navTop scroll-view .items.active {
    color: #ff6419;
    font-weight: bold;
}

.navTop scroll-view .items.active::after {
    content: '';
    position: absolute;
    left: 50%;
    bottom: 0;
    margin-left: -15rpx;
    width: 30rpx;
    height: 4rpx;
    background-color: #ff6419;
    border-radius: 2px;
}

.top {
    height: 88rpx;
    width: 750rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    left: 0;
    border-bottom: 2rpx solid #ededed;
    color: #333;
    font-size: 28rpx;
    z-index: 10;
}

.top .active {
    color: #ff6419;
}

.top image {
    width: 16rpx;
    height: 16rpx;
}

.room {
    height: 88rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.area {
    height: 90rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.style {
    height: 90rpx;
    width: 250rpx;
    text-align: center;
    line-height: 90rpx;
}

.space {
    height: 90rpx;
}

.goods-details-case {
    padding: 20rpx;
    padding-bottom: 0rpx;
    background-color: #ededed;
}

.goods-details-case .case-item {
    height: 560rpx;
    padding-bottom: 20rpx;
    background-color: #ededed;
    border-radius: 10rpx;
    overflow: hidden;
}

.goods-details-case .case-item .case-image {
    height: 420rpx;
}

.goods-details-case .case-item .case-image .bigPth {
    width: 100%;
    height: 100%;
    background: #fff;
}

.goods-details-case .case-item .case-text {
    height: 140rpx;
    background-color: white;
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.goods-details-case .case-item .case-image {
    position: relative;
}

.goods-details-case .case-item .case-image .Renovation-house {
    width: 171rpx;
    height: 60rpx;
    background-color: #ff6419;
    border-radius: 30px 0px 0px 30px;
    position: absolute;
    bottom: 35rpx;
    right: 0;
    color: #fff;
    font-size: 28rpx;
    line-height: 60rpx;
    padding-left: 39rpx;
}

.case-image .Renovation-house .arrow {
    width: 11rpx;
    height: 24rpx;
    position: absolute;
    right: 30rpx;
    top: 17rpx;
}

.case-image .seven-two-ico {
    position: absolute;
    bottom: 28rpx;
    left: 30rpx;
}

.case-image .seven-two-ico .identical-class {
    width: 70rpx;
    height: 70rpx;
    margin-right: 39rpx;
    float: left;
}

.seven-two-ico .identical-class image {
    width: 70rpx;
    height: 70rpx;
}

.goods-details-case .case-item .case-text {
    padding-left: 31rpx;
}

.case-text .case-mes {
    padding-top: 31rpx;
}

.case-mes .case-mes-name {
    font-size: 32rpx;
    max-width: 70%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #333;
}

.case-mes .case-mes-function .collet, .case-mes .case-mes-function .thumbs-up {
    margin-right: 26rpx;
    vertical-align: middle;
}

.case-mes .case-mes-function image {
    width: 47rpx;
    height: 47rpx;
    float: left;
}

.case-mes .case-mes-function text {
    color: #999;
    font-size: 24rpx;
    line-height: 47rpx;
    float: left;
}

.case-text .case-style {
    font-size: 26rpx;
    line-height: 57rpx;
    color: #999;
}

.case-style image {
    width: 56rpx;
    height: 56rpx;
}

.case-condition {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 90rpx;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 10;
}

.condition-box {
    width: 100%;
    max-height: 740rpx;
    position: absolute;
    left: 0;
    top: 0;
    background-color: #fff;
    padding: 31rpx 0rpx 8rpx 31rpx;
    box-sizing: border-box;
    overflow: auto;
}

.condition-box .single-box {
    width: 216rpx;
    height: 80rpx;
    background-color: #f5f5f5;
    border-radius: 5rpx;
    margin: 0 20rpx 20rpx 0;
    text-align: center;
    font-size: 28rpx;
    line-height: 80rpx;
    color: #333;
}

.condition-box .single-box.condition-box-active {
    background-color: #fff1eb;
    color: #ff6419;
}

/* 渲染弹框 */

.tag {
    height: 68rpx;
    width: 40rpx;
    position: absolute;
    top: 0;
    left: 20rpx;
}

.conditions {
    width: 100%;
    position: fixed;
    padding: 88rpx 0rpx 100rpx 40rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    background: #fff;
    top: 0;
    right: 0;
    overflow-y: scroll;
    transition: all 0.15s ease;
    height: 100%;
    z-index: 30;
    transform: translateY(100%);
}

::-webkit-scrollbar {
    display: none;
}

.conditions.active {
    transform: translateY(0);
}

.conditions .showTitle {
    line-height: 90rpx;
    color: #000;
}

.conditions .showTitle text {
    color: #999;
    font-size: 24rpx;
}

.conditions .show-rows {
    width: 100%;
    display: inline-block;
    min-height: 100rpx;
}

.conditions .show-rows .show-rows-items {
    width: 216rpx;
    margin-right: 20rpx;
    background-color: #f5f5f5;
    border-radius: 5px;
    float: left;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    margin-bottom: 20rpx;
    color: #333;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.conditions .show-rows .show-rows-items.unMr {
    margin-right: 0;
}

.conditions .show-rows .show-rows-items.active {
    background-color: #fff1eb;
    color: #ff6419;
}

.btnRows {
    width: 100%;
    height: 88rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    font-size: 32rpx;
    z-index: 35;
    left: 0;
    line-height: 88rpx;
}

.btnRows::after {
    content: '';
    width: 100%;
    left: 0;
    top: 0;
    background: #f2f2f2;
    height: 1rpx;
    position: absolute;
}

.btnRows view {
    height: 100%;
    float: left;
}

.btnRows view.resets {
    width: 33.33333%;
    width: calc(100% / 3);
    width: -webkit-calc(100% / 3);
    width: -moz-calc(100% / 3);
    text-align: center;
}

.btnRows view.submits {
    width: 66.66667%;
    width: calc((100% / 3) * 2);
    width: -webkit-calc((100% / 3) * 2);
    width: -moz-calc((100% / 3) * 2);
    background-color: #ff6419;
    text-align: center;
    color: #fff;
}

.showTit {
    box-sizing: border-box;
    width: 100%;
    line-height: 1;
    text-align: center;
    color: #999;
    font-size: 24rpx;
    top: 50%;
    margin-top: 200rpx;
}

/* 装修报价说明弹框 */

.decorate-mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    position: fixed;
    left: 0;
    top: 0;
}

.decorate-price-box {
    width: 540rpx;
    /* height: 680rpx; */
    border-radius: 10rpx;
    background-color: #fff;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 0 60rpx 28rpx;
    box-sizing: border-box;
}

.decorate-price-box .price-box-title {
    margin-top: 38rpx;
    line-height: 53rpx;
    text-align: center;
    color: #333;
    font-size: 32rpx;
}

.decorate-price-box .price-box-list .price-box-item {
    margin-top: 32rpx;
}

.price-box-list .price-box-item .box-item-main {
    color: #333;
    line-height: 48rpx;
    font-size: 28rpx;
}

.price-box-list .price-box-item .box-item-secondary {
    color: #999;
    font-size: 24rpx;
    line-height: 28rpx;
}

.decorate-price-box .price-box-confirm {
    margin-top: 50rpx;
    width: 420rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    background-color: #ff6419;
    font-size: 32rpx;
    border-radius: 40rpx;
}


.cityView{
    width: 100%;
    position: fixed;
    height: 858rpx;
    left: 0;
    top: 174rpx;
    background: #f9f9f9;
    overflow: hidden;
    z-index: 6;
}
.cityView>view{
    display: inline-block;
    float: left;
    overflow-x: hidden;
    overflow-y: scroll;
    font-size: 0;
    min-height: 100%;
    height: 858rpx;
}
.cityView>view .items{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 28rpx;
    text-align: center;
    color: #333333;
    position: relative;
    overflow: hidden;
}

.cityView .pull-left .items.active{
    background: #f9f9f9;
}
.cityView .pull-left .items.active::before{
    width: 0;
}
.cityView .pull-left .rows:first-child .items:first-child::after{
    height: 0;
}
.cityView>view .items.active{
    color: #ff6419;
}
.cityView>view .items>text{
    display: inline-block;
    height: 100%;
    width: 50rpx;
    left: 5rpx;
    position: absolute;
    color: #999999;
    text-align: center;
}
.rows{
    width: 100%;
}
.cityView .pull-left{
    width: 35%;
}
.cityView .pull-left .items{
    background: #fff;
    box-sizing: border-box;
    padding-left: 60rpx;
    text-align: left;
    overflow-x: auto;
}
.cityView .pull-left .items::after{
    width: 100%;
    left: 0;
    top: 0;
    height: 2rpx;
}
.cityView .pull-left .items::before{
    height: 100%;
    right: 0;
    top: 0;
    width: 2rpx;
}
.cityView .items::after,
.cityView .items::before{
    content: '';
    position: absolute;
    background-color: #ebebeb;
}
.cityView .pull-right{
    width: 65%;
}

.cityView .pull-right .items{
    text-align: left;
    box-sizing: border-box;
    padding-left: 55rpx;
    padding-right: 100rpx;
}
.cityView .pull-right .items::after{
    bottom:0;
    width:100%;
    height: 2rpx;
    left: 40rpx;
}
.cityView .pull-right .items.active{
    color: #ff6419;
}

.cityView .pull-right .items image{
    position: absolute;
    width: 36rpx;
    height: 30rpx;
    right: 60rpx;
    top: 50%;
    margin-top: -15rpx;
}

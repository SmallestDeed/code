/* pages/mine/myPublish/myPublish.wxss */
.type{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
  height: 88rpx;
  width: 750rpx;
  display: flex;
  flex-direction: row;
  border-top:1rpx solid #f5f5f5;
  background-color: #ffffff;
}

.supplyselect{
  width: 375rpx;
  line-height: 88rpx;
  text-align: center;
  font-weight: 700;
  justify-content:center;
  font-size: 28rpx;
  color: #333;
  position: relative;
}
.supply{
  width: 375rpx;
  line-height: 88rpx;
  text-align: center;
  font-family: PingFang-SC-Medium;
  justify-content:center;
  font-size: 28rpx;
  color: #333333;
}
.supplyselect_active:after {
  content: "";
  position: absolute;
  bottom: 4rpx;
  height: 6rpx;
  left: 50%;
  margin-left: -20rpx;
  background-color: #29cccc;
  display: block;
  width: 40rpx;
  border-radius: 3px;
}
.line{
  position: absolute;
  top: 0;
  left: 50%;
  width: 1rpx;
	height: 26rpx;
	background-color: #e8e8e8;
  margin-top: 31rpx;
}
/* .list{
  padding-top: 105rpx;
} */
.list-item{
  height: 272rpx;
  margin-bottom: 16rpx;
  padding: 30rpx 30rpx 0 30rpx;
  background-color: #fff;
}
.item-content{
  height: 150rpx;
  display: flex;
  padding:16rpx;
  background-color: #fafafa;
  justify-content: flex-start;
}
.item-content image{
  width: 200rpx;
  height: 100%;
}
.item-info{
  width: 440rpx;
  margin-left: 20rpx;
  color: #999999;
}
.title{
  width: 440rpx;
  margin-top: 9rpx;
  font-size: 28rpx;
  line-height: 27rpx;
  color: #333333;
}
.describe{
  width: 440rpx;
  font-size: 24rpx;
  /* line-height: 24rpx; */
  margin-top: 20rpx;
}
.data-box{
  height: 28rpx;
  margin-top: 40rpx;
  font-size: 20rpx;
  display: flex;
  justify-content: flex-start;
}
.glance-over{
  width: 30%;
  height: 28rpx;
  line-height: 28rpx;
  display: flex;
  justify-content: flex-start;
}
.glance-over image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.comment{
  width: 30%;
  height: 28rpx;
  line-height: 28rpx;
  display: flex;
  justify-content: flex-start;
}
.comment image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.time{
  width: 30%;
  height: 28rpx;
  line-height: 28rpx;
  display: flex;
  justify-content: flex-start;
}
.time image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.item-footer{
  height: 28rpx;
  margin-top: 32rpx;
  font-size: 28rpx;
  font-weight: bold;
  position: relative;
  display: flex;
  justify-content: space-between;
}
.midcourt-line{
  position: absolute;
  top: 0;
  left: 50%;
  width: 1rpx;
  height: 28rpx;
  background-color: #e8e8e8;
}
.compile{
  line-height: 28rpx;
  text-align: center;
  width: 50%;
  height: 100%;
}
.putaway{
  line-height: 28rpx;
  text-align: center;
  width: 50%;
  height: 100%;
}
.publish{
  height: 80rpx;
  width: 80rpx;
  line-height: 80rpx;
  font-size:24rpx;
  font-weight: bold;
  position: fixed;
  right:40rpx;
  bottom:240rpx;
  z-index: 99;
  background-color: #29cccc;
  color: #fff;
  text-align: center;
  border-radius: 50%;
}
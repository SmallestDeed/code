/* pages/mine/couponList/couponList.wxss */
page{
  background-color: #f5f5f5;
}
.yhq_main_body{margin: auto;margin-bottom: 100rpx; overflow: hidden;}
.yhq_header{text-align: center; height: 90rpx; background-color: #fff; margin-bottom: 10rpx;}
.yhq_header_title{line-height: 90rpx;font-size: 32rpx;color: #000; width: 33.333333%; float: left;position: relative;}
.yhq_header_active {color: #ff6419; font-weight: 600;}
.yhq_header_active:after{content: "";position: absolute;bottom: 4rpx; z-index: 9; height: 4rpx;left: 50%; margin-left: -20rpx; background-color: #ff6419;display: block;width: 40rpx;}
.yhq_liebiao{margin: 30rpx 4% 20rpx; background-color: #fff; border-radius: 6rpx; padding: 50rpx 30rpx 40rpx; position: relative; overflow: hidden;height:158rpx;}
.liebiao_left_xrzx{position: absolute; top: 0rpx; left: 0rpx; line-height: 44rpx; font-size: 24rpx; color: #fff; background: #ffab19; width: 128rpx; text-align: center; border-radius: 6rpx 0rpx 18rpx 0rpx;}
.yhq_liebiao_left{width: 30%; float: left; text-align: center;}
.yhq_liebiao_right{width: 65%; float: right;z-index:1; position: relative;}
.liebiao_left_title{font-size: 80rpx; color: #ff6419; font-family: arial; font-weight: bold; margin-top: 10rpx; margin-bottom: 20rpx;}
.liebiao_left_title .unit{font-size: 36rpx; color: #ff6419; font-weight: normal; display: inline-block; vertical-align: top; padding-top: 16rpx;}
.liebiao_left_text{font-size: 28rpx; color: #ff6419;}
.liebiao_right_title,.liebiao_right_text{font-size: 24rpx; color: #000; line-height: 44rpx;}
.liebiao_button01 {width: 180rpx; line-height: 56rpx; background: #ff6619; border: 1px solid #ff6619; font-size: 28rpx; color: #fff; text-align: center; display: block; border-radius: 28rpx; margin-top: 20rpx;}
.liebiao_button02 {width: 180rpx; line-height: 56rpx; background: #fff; border: 1px solid #ff6619; font-size: 28rpx; color: #ff6619; text-align: center; display: block; border-radius: 28rpx; margin-top: 20rpx;}
.yhq_coupons_label image{width: 160rpx; height: 160rpx; display: block;}
.yhq_coupons_label{position: absolute; right: 30rpx; bottom: 20rpx; z-index: 0;}
.yhq_ysy .liebiao_left_xrzx{background: #cccccc !important;}
.yhq_ysy .liebiao_left_title{color: #666666 !important;}
.yhq_ysy .liebiao_left_title .unit{color: #666666 !important;}
.yhq_ysy .liebiao_left_text{color: #666666 !important;}
.yhq_ysy .liebiao_right_title,.yhq_ysy .liebiao_right_text{color: #666666 !important;}

.yhq_wdkq {position: fixed;left: 0rpx;right: 0rpx;bottom: 0rpx;max-width: 1500rpx;margin: auto;height: 96rpx;line-height: 96rpx;background-color: #ffffff;border-top: 1px solid #e8e8e8;z-index: 9;}
.yhq_wdkq .foot-link {line-height: 96rpx;display: block;color: #ff6419;font-size: 32rpx;text-align: center;}


.yhq_rxsp{margin: 50rpx 4% 10rpx; overflow: hidden;}
.yhq_rxsp_h1{font-size: 28rpx; color: #333; line-height: 48rpx; font-weight: bold;margin-bottom: 20rpx; overflow: hidden;}
.yhq_rxsp_more{display: block;float: right;color: #999; font-size: 24rpx;}
.yhq_rxsp_more image{width: 12rpx; display: inline-block; vertical-align: middle; margin-top: -8rpx; margin-left: 12rpx;}
.yhq_rxsp .rxsp_item{width: 49%; margin-right: 2%; float: left; margin-bottom: 20rpx;text-align: center}
.yhq_rxsp .rxsp_item image{width: 338rpx;height:338rpx; display: block;}
 .yhq_rxsp .rxsp_item:nth-child(2n+1) {margin-right: 0rpx;} 
.yhq_rxsp_title{font-size: 28rpx; color: #333; display: block; line-height: 48rpx; margin-top: 20rpx;}
.yhq_rxsp_text{font-size: 28rpx; color: #333; font-family: arial; font-weight: bold; display: block; line-height: 48rpx; margin-top: 10rpx;}

.liebiao_left_xrzx{position: absolute; top: 0rpx; left: 0rpx; line-height: 44rpx; font-size: 24rpx; color: #fff; background: #ffab19; width: 128rpx; text-align: center; border-radius: 6rpx 0rpx 18rpx 0rpx;}
.conditions{
  width: 100%;
  position: fixed;
  padding: 88rpx 0rpx 100rpx 40rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  background: #fff;
  top: -200px;
  right: 0;
  overflow-y: scroll;
  transition-timing-function: ease-in-out;
  height: 0;
  opacity: 0.6;
  z-index: 10;
}
.conditions.active{
  height: 100%;
  top: 0;
  opacity: 1;
}
.conditions .showTitle{
  line-height:90rpx;
  color:#000000;
}
.conditions .showTitle text{
  color: #999;
  font-size: 24rpx;
}
.conditions .show-rows {
  width: 100%;
  display: inline-block;
  min-height: 100rpx;
}
.conditions .show-rows .show-rows-items{
  width: 216rpx;
  margin-right: 20rpx;
  background-color: #f5f5f5;
	border-radius: 5px;
  float: left;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin-bottom: 20rpx;
  color: #333333;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.conditions .show-rows .show-rows-items.unMr{
  margin-right: 0;
}
.conditions .show-rows .show-rows-items.active{
  background-color: #fff1eb;
  color: #ff6419;
}
.btnRows{
  width: 100%;
  height: 88rpx;
	background-color: #ffffff;
  position: fixed;
  bottom: 0;
  font-size: 32rpx;
  z-index: 15;
  left: 0;
  line-height: 88rpx;
}
.btnRows::after{
  content: '';
  width: 100%;
  left: 0;
  top: 0;
  background: #f2f2f2;
  height: 1rpx;
  position: absolute;
}
.btnRows view{
  height: 100%;
  float: left;
}
.btnRows view.resets{
  width: 33.33333%;
  width: calc(100% / 3);
  width: -webkit-calc(100% / 3);
  width: -moz-calc(100% / 3);
  text-align: center;
}
.btnRows view.submits{
  width: 66.66667%;
  width: calc((100% / 3) * 2);
  width: -webkit-calc((100% / 3) * 2);
  width: -moz-calc((100% / 3) * 2);
  background-color: #ff6419;
  text-align: center;
  color: #fff;
}
.showTit{
  box-sizing: border-box;
  width: 100%;

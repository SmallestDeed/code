/* 装进我家开始 */
.packDialog{
  width: 100%;
  height: 100%;
  background-color:rgba(0, 0, 0, .5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10000;
}
.packDialogRenderBox{
  width: 540rpx;
  border-radius: 10rpx;
  background-color: #ffffff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top:-284rpx;
  margin-left: -270rpx;
}
.packDialogTop{
  height: 480rpx;
  text-align: center;
}
.packDialogImg{
  display: inline-block;
  width: 240rpx;
  height: 240rpx;

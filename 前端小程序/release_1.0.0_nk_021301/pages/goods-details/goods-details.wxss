.goods-message {
    padding: 0rpx 30rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.goods-message .g-m-title {
    line-height: 93rpx;
    font-size: 28rpx;
    color: #333;
}

.goods-message .g-m-reset-title {
    text-align: center;
    border-radius: 30px;
    border: solid 2rpx #ddd;
    height: 60rpx;
    width: 195rpx;
    margin: 0 auto;
    line-height: 60rpx;
    position: relative;
}

.goods-message .g-m-reset-title::before, .goods-message .g-m-reset-title::after {
    content: '';
    display: inline-block;
    height: 1px;
    background-color: #eee;
    width: 248rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.goods-message .g-m-reset-title::before {
    right: 200rpx;
}

.goods-message .g-m-reset-title::after {
    left: 200rpx;
}

.goods-message .g-m-info-box .g-m-info-title {
    border-bottom: 1px dashed #e0e0e0;
    font-weight: bold;
}

.goods-message .g-m-info-box .g-m-info-row {
    border-bottom: 1px dashed #e0e0e0;
    height: auto;
    display: -webkit-flex;
    display: flex;
}

.goods-message .g-m-info-box .g-m-info-row .g-m-info-row-title {
    font-size: 24rpx;
    color: #999;
    width: 145rpx;
    padding: 20rpx 0;
}

.goods-message .g-m-info-box .g-m-info-row .g-m-info-row-content {
    box-sizing: border-box;
    font-size: 24rpx;
    color: #666;
    -webkit-flex: 1;
    flex: 1;
    height: 80rpx;
    line-height: 80rpx;
}

.goods-message .g-m-table {
    padding-bottom: 24rpx;
    padding-top: 40rpx;
    overflow: hidden;
}

.goods-message .g-m-table .wxParse-img {
    width: 100%;
}

.goods-message .g-m-table .g-m-t-item {
    line-height: 58rpx;
    font-size: 24rpx;
    color: #666;
}

.goods-message .g-m-table .g-m-t-item:nth-child(1) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-item:nth-child(2) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-item:nth-child(3) {
    border-bottom: #e0e0e0 dashed 1px;
}

.goods-message .g-m-table .g-m-t-describe {

/* pages/goods-details/goods-details.wxss */
@import "../../component/render-type/render-type.wxss";

/* 头部样式 */

.goods-details {
    /* padding-top: 88rpx; */
    background-color: #f5f5f5;
}
contact-button{
  position: fixed;
  left:65rpx;
  bottom:20rpx;
  opacity:0;

}
.goods-details-header {
    height: 88rpx;
    width: 750rpx;
    display: flex;
    margin-bottom: 4rpx;
    position: fixed;
    left: 0;
    top: 0;
    background-color: #fff;
    z-index: 10;
}
.goods-details .header-bottombox {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    background: linear-gradient(to left, #ffbb33, #ff8833);
}

.goods-details .header-bottombox__text {
    font-size: 32rpx;
    color: white;
    float: left;
    font-weight: bold;
}

.goods-details .header-bottombox-timebox {
    float: right;
    color: white;
    font-size: 24rpx;
}

.goods-details .header-bottombox-timebox .numbg {
    width: 40rpx;
    height: 40rpx;
    background: #ff7733;
    font-size: 24rpx;
    border-radius: 4px;
    margin: 0 10rpx;
    display: inline-block;
    line-height: 40rpx;
    text-align: center;

}
.goods-details-header view {
    flex: auto;
    text-align: center;
    line-height: 88rpx;
}

.goods-details-header view text {
    display: inline-block;
    font-size: 32rpx;
    line-height: 82rpx;
}

.goods-details-header view .text-active {
    color: #ff6419;
    border-bottom: #ff6419 solid 4rpx;
}

/* 輪播圖樣式 */

.goods-details-content {
    /* margin-top:3px; */
    padding-bottom: 98rpx;
}

.goods-details-content swiper {
    height: 750rpx;
}

.goods-details-content swiper image {
    width: 750rpx;
    height: 750rpx;
}

/* z主要字体样式 */

.goods-details-content .main-text {
    width: 750rpx;
    min-height: 165rpx;
    /* border-bottom:10rpx solid #f5f5f5; */
    padding-left: 29rpx;
    box-sizing: border-box;
    background-color: #fff;
}

.goods-details-content .main-text .main-text-name {
    font-size: 28rpx;
    background-color: #fff;
    line-height: 48rpx;
    max-height: 96rpx;
    overflow: hidden;
    word-break: break-all;
    padding-top: 20rpx;
    margin-bottom: 24rpx;
    width: 690rpx;
    font-weight: bold;
}

.goods-details-content .main-text .main-text-funtion .funtion-price {
    line-height: 50rpx;
    font-size: 40rpx;
    color: #ff6419;
    font-weight: bold;
    margin-left: -6rpx;
}
.goods-details-content  .main-text__money{
    line-height: 50rpx;
    font-size: 40rpx;
    color: #ff6419;
    font-weight: bold;
    margin-left: -6rpx;
}
.goods-details-content .main-text__tab {
    padding: 0 10rpx;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 24rpx;
    color: #ff6419;
    background: #fff1eb;
    text-align: center;
    border-radius: 2rpx;
     margin-left: 10rpx;
     margin-top: 8rpx;
}
.goods-details-content .main-text .main-text-funtion .identical {
    width: 50rpx;
    height: 50rpx;
}

.goods-details-content .main-text .main-text-funtion .ico-Text {
    color: #666;
    line-height: 50rpx;
    font-size: 22rpx;
    margin-left: 10rpx;
    margin-right: 29rpx;
}

.main-text-funtion .identical image {
    width: 56rpx;
    height: 56rpx;
}

/* 颜色款式样式 */

.chose-specifications {
    height: 128rpx;
    padding-left: 31rpx;
    font-size: 28rpx;
    line-height: 88rpx;
    color: #333;
    border-top: solid 20rpx #f5f5f5;
    border-bottom: solid 20rpx #f5f5f5;
    box-sizing: border-box;
    background-color: #fff;
    font-weight: bold;
}

.chose-specifications .arrow image {
    width: 15rpx;
    height: 28rpx;
    margin-top: 30rpx;
    margin-right: 30rpx;
}

/* 产品信息 */

.goods-details .main-changePersonbox {
    margin-bottom:20rpx;
}

.goods-details .main-changePersonbox .bgwrap {
    background: white;
    padding: 30rpx 0 10rpx 0;
}

.goods-details .main-changePersonbox__changebox {
    display: flex;
    align-items: center;
    padding: 0 20rpx 0 30rpx;
    box-sizing: border-box;
}

.goods-details .main-changePersonbox__changebox .text {
    flex: 1;
    font-size: 28rpx;
    color: #333333;
    font-weight: bold;
}

.goods-details .main-changePersonbox__changebox .changeBtn {
    flex-basis: 124rpx;
    font-size: 24rpx;
    color: #999999;
    height: 44rpx;
    line-height: 44rpx;
}

.goods-details .main-changePersonbox__changebox .changeBtn .image {
    width: 44rpx;
    height: 44rpx;
    vertical-align: -12rpx;
}

.goods-details  .main-changePersonbox__person {

    box-sizing: border-box;
}

.goods-message .g-m-table .g-m-t-describe text {
    position: absolute;
    left: 0rpx;
    top: 18rpx;
    margin: 0rpx;
}

.goods-message .g-m-table .g-m-t-item text {
    color: #999;
    margin-right: 50rpx;
}


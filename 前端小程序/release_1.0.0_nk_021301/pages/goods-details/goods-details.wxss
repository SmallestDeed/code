    padding: 30rpx 0;
    margin: 0 30rpx;
    box-sizing: border-box;
    align-items: center;
}

.goods-details .main-changePersonbox__person .personImg {
    flex-basis: 88rpx;
    height: 88rpx;
    border-radius: 50%;
}

.goods-details .main-changePersonbox__person .personInfo {
    margin-left: 24rpx;
    flex: 1;
}

.goods-details .main-changePersonbox__person .personInfo .name {
    color: #333333;
    font-size: 28rpx;
    font-weight: bold;
    margin-bottom: 14rpx;
}
.line{
    border-top: solid #e8e8e8 2rpx; 
}
.goods-details .main-changePersonbox__person .personInfo .time {
    color: #999999;
    font-size: 24rpx;
}

.goods-details .main-changePersonbox__person .personInfo .time .num {
    color: #ff6419;
}

.goods-details .main-changePersonbox__person .joinBtn {
    flex-basis: 120rpx;
    height: 60rpx;
    background: #ff6419;
    color: white;
    text-align: center;
    line-height: 60rpx;
    font-size: 32rpx;
    border-radius: 4rpx;
}
.goods-details .main-changePersonbox__person  .joinBtn--none{
      flex-basis: 120rpx;
      height: 60rpx;
      background: #e8e8e8;
      color: white;
      text-align: center;
      line-height: 60rpx;
      font-size: 32rpx;
      border-radius: 4rpx;
}

.goods-details .main-assembleStatus {
    padding: 60rpx 0;
    background: white;
    margin-bottom: 20rpx;
}

.goods-details .main-assembleStatus__personImgbox {
    text-align: center;
}

.goods-details .main-assembleStatus__personImgbox .Imgbox {
    display: inline-block;
    position: relative;
}

.goods-details .main-assembleStatus__personImgbox .Imgbox .personImg {
    width: 120rpx;
    height: 120rpx;
    background: #ccc;
    border-radius: 50%;
    margin-right: 28rpx;
}

.goods-details .main-assembleStatus__personImgbox .Imgbox__commander {
    position: absolute;
    bottom: 0;
    left: 26rpx;
}

.goods-details .main-assembleStatus__text {
    text-align: center;
    color: #333333;
    font-size: 32rpx;
    margin: 50rpx 0 30rpx 0;
}

.goods-details .main-assembleStatus__text .num {
    color: #ff6419;
}

.goods-details .main-assembleStatus__text .timeImg {
    width: 44rpx;
    height: 44rpx;
    vertical-align: -8rpx;
    margin-right: 10rpx;
}

.goods-details .main-assembleStatus__timebox {
    text-align: center;
    color: #333333;
    font-size: 24rpx;
}

.goods-details .main-assembleStatus__timebox .numbg {
    width: 40rpx;
    height: 40rpx;
    background: #333333;
    color: white;
    font-size: 24rpx;
    border-radius: 4px;
    margin: 0 10rpx;
    display: inline-block;
    line-height: 40rpx;
    text-align: center;
}


.goods-details .main-playbox {
   margin-top: 20rpx;
}
.goods-details .main-playbox .bgwrap {
    background: white;
    padding: 26rpx 0 36rpx 0;
}

.goods-details .main-playbox__title {
    color: #333333;
    font-size: 28rpx;
    font-weight: bold;
    margin-left: 30rpx;
}

.goods-details .main-playbox__numbox {
    display: flex;
}

.goods-details .main-playbox__numbox .numberwrap {
    flex: 1;
}

.goods-details .main-playbox__numbox .numberwrap .number {
    color: #ff6419;
    border-radius: 50%;
    border: 1px solid #ff6419;
    width: 44rpx;
    height: 44rpx;
    line-height: 44rpx;
    text-align: center;
    margin: 30rpx auto 24rpx auto;
}

.goods-details .main-playbox__numbox .numberwrap .numbertext {
    color: #333333;
    font-size: 24rpx;
    text-align: center;
}

.goods-details .main-playbox__numbox .numberwrap .line {
    position: relative;
}

.goods-details .main-playbox__numbox .numberwrap .line::before {
    content: '';
    position: absolute;
    left: -192rpx;
    top: 22rpx;
    width: 180rpx;
    height: 2rpx;
    background: #f5e4dc;
}

.goods-details .main-playbox__numbox .numberwrap .line::after {
    content: '';
    position: absolute;
    right: -192rpx;
    top: 22rpx;
    width: 180rpx;
    height: 2rpx;
    background: #f5e4dc;
}


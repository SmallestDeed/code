.assemble-detail {
  background: #f5f5f5;
}

.assemble-detail .header-shopDetail {
  padding: 20rpx 0;
}

.assemble-detail .header-shopDetail .bgwrap {
  background: white;
}

.assemble-detail .header-shopDetail__detailbox {
  display: flex;
  padding: 30rpx;
}

.assemble-detail .header-shopDetail__detailbox .shopImg {
  flex-basis: 160rpx;
  height: 160rpx;
}

.assemble-detail .header-shopDetail__detailbox .shopInfo {
  flex: 1;
  margin-left: 20rpx;
}

.assemble-detail .header-shopDetail__detailbox .shopInfo .title {
  color: #333333;
  font-size: 32rpx;
  font-weight: bold;
}

.assemble-detail .header-shopDetail__detailbox .shopInfo .money {
  color: #ff6419;
  font-size: 44rpx;
  margin: 20rpx 0 10rpx 0;
}

.assemble-detail .header-shopDetail__detailbox .shopInfo .money .tab {
  padding: 0 10rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #ff6419;
  background: #fff1eb;
  display: inline-block;
  vertical-align: 6rpx;
  text-align: center;
  border-radius: 2rpx;
  margin-left: 8rpx;
}

.assemble-detail .header-shopDetail__detailbox .shopInfo .costPrice {
  color: #999999;
  text-decoration: line-through;
  font-size: 24rpx;
}

.assemble-detail .main-playbox {
  padding-bottom: 20rpx;
}

.assemble-detail .main-playbox .bgwrap {
  background: white;
  padding: 26rpx 0 36rpx 0;
}

.assemble-detail .main-playbox__title {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
  margin-left: 30rpx;
}

.assemble-detail .main-playbox__numbox {
  display: flex;
}

.assemble-detail .main-playbox__numbox .numberwrap {
  flex: 1;
}

.assemble-detail .main-playbox__numbox .numberwrap .number {
  color: #ff6419;
  border-radius: 50%;
  border: 1px solid #ff6419;
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  margin: 30rpx auto 24rpx auto;
}

.assemble-detail .main-playbox__numbox .numberwrap .numbertext {
  color: #333333;
  font-size: 24rpx;
  text-align: center;
}

.assemble-detail .main-playbox__numbox .numberwrap .line {
  position: relative;
}

.assemble-detail .main-playbox__numbox .numberwrap .line::before {
  content: '';
  position: absolute;
  left: -192rpx;
  top: 22rpx;
  width: 180rpx;
  height: 2rpx;
  background: #f5e4dc;
}

.assemble-detail .main-playbox__numbox .numberwrap .line::after {
  content: '';
  position: absolute;
  right: -192rpx;
  top: 22rpx;
  width: 180rpx;
  height: 2rpx;
  background: #f5e4dc;
}

.assemble-detail .main-assembleStatus {
  padding: 60rpx 0;
  background: white;
}

.assemble-detail .main-assembleStatus__personImgbox {
  text-align: center;
   height: 130rpx;
   overflow: hidden;
}

.assemble-detail .main-assembleStatus__personImgbox .Imgbox {
  display: inline-block;
  position: relative;
}

.assemble-detail .main-assembleStatus__personImgbox .Imgbox .personImg {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin:0 14rpx;
}

.assemble-detail .main-assembleStatus__personImgbox .Imgbox__commander {
  position: absolute;
  bottom: 0;
  left: 36rpx;
}

.assemble-detail .main-assembleStatus__text {
  text-align: center;
  color: #333333;
  font-size: 32rpx;
  margin: 50rpx 0 30rpx 0;
}

.assemble-detail .main-assembleStatus__text .num {
  color: #ff6419;
}

.assemble-detail .main-assembleStatus__text .timeImg {
  width: 44rpx;
  height: 44rpx;
  vertical-align: -8rpx;
  margin-right: 10rpx;
}

.assemble-detail .main-assembleStatus__timebox {
  text-align: center;
  color: #333333;
  font-size: 24rpx;
}

.assemble-detail .main-assembleStatus__timebox .numbg {
  width: 40rpx;
  height: 40rpx;
  background: #333333;
  color: white;
  font-size: 24rpx;
  border-radius: 4px;
  margin: 0 10rpx;
  display: inline-block;
  line-height: 40rpx;
  text-align: center;
}

.assemble-detail .main-assembleDetail {
  padding: 20rpx 0;
}

.assemble-detail .main-assembleDetail .bgwrap {
  background: white;
}

.assemble-detail .main-assembleDetail__personDetailbox {
  padding-top: 30rpx;
  padding-bottom: 10rpx;
}

.assemble-detail .main-assembleDetail__personDetailbox .title {
  color: #333333;
  font-size: 28rpx;
  margin-left: 30rpx;
}

.assemble-detail .main-assembleDetail__personDetailbox .line {
  border-top: 1px solid #e8e8e8;
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox {
  display: flex;
  margin: 30rpx;
  padding-top: 30rpx;
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox .personImg {
  flex-basis: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  margin-right: 24rpx;
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox .namebox {
  flex: 1;
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox .namebox .name {
  color: #333333;
  font-size: 28rpx;
  font-weight: bold;
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox .namebox .name__commander {
  display: inline-block;
  vertical-align:4rpx
}

.assemble-detail .main-assembleDetail__personDetailbox .infobox .namebox .time {
  color: #999999;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.assemble-detail .friendJoin {
  background: white;
  padding: 20rpx 0;
}

.assemble-detail .friendJoin__joinBtn {
  width: 690rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background: #ff6419;
  border-radius: 40rpx;
  color: white;
  font-size: 32rpx;
  margin: 0 auto;
}
/*公共的团长样式*/
.assemble-detail .commander {
  width: 68rpx;
  height: 30rpx;
  color: white;
  background: #ff6419;
  border-radius: 15rpx;
  text-align: center;
  line-height: 30rpx;
  font-size: 22rpx;
}
/*分享图片蒙层弹框*/
.assemble-detail .photoDialog {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}


.assemble-detail .photoDialog .photoWrap {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.assemble-detail .photoDialog .photoWrap .photoBox {
  width: 540rpx;
  height: 924rpx;
  border-radius: 10rpx;
  background: white;
  position: relative;
}

.assemble-detail .photoDialog .photoWrap .photoBox .close {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 10rpx;
  right: 10rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .personInfo {
  color: #333333;
  font-size: 28rpx;
  padding: 40rpx 40rpx 30rpx 40rpx;
  box-sizing: border-box;
}

.assemble-detail .photoDialog .photoWrap .photoBox .personInfo .Img {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  vertical-align: -30rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .personInfo .name {
  font-weight: bold;
  margin: 0 20rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .Imgbox {
  text-align: center;
}

.assemble-detail .photoDialog .photoWrap .photoBox .Imgbox .shopImg {
  width: 460rpx;
  height: 480rpx;
  border-radius: 10rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .infoBox {
  margin-left: 40rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .infoBox .title {
  color: #333333;
  font-size: 26rpx;
  font-weight: bold;
}

.assemble-detail .photoDialog .photoWrap .photoBox .infoBox .money {
  color: #ff6419;
  font-size: 28rpx;
  margin: 30rpx 0 20rpx 0;
}

.assemble-detail .photoDialog .photoWrap .photoBox .infoBox .money .tab {
  padding: 0 10rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #ff6419;
  background: #fff1eb;
  display: inline-block;
  vertical-align: 6rpx;
  text-align: center;
  border-radius: 2rpx;
  margin-left: 8rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .infoBox .costPrice {
  color: #999999;
  text-decoration: line-through;
  font-size: 24rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .codeBox {
  text-align: center;
  display: inline-block;
  position: absolute;
  bottom: 30rpx;
  right: 20rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .codeBox .twoCode {
  width: 100rpx;
  height: 100rpx;
}

.assemble-detail .photoDialog .photoWrap .photoBox .codeBox .codeText {
  font-size: 22rpx;
  color: #999999;
}

.assemble-detail .photoDialog .photoWrap .saveBtn {
  width: 540rpx;
  height: 80rpx;
  background: #ff6419;
  border-radius: 40rpx;
  font-size: 32rpx;
  color: white;
  line-height: 80rpx;
  text-align: center;
  margin: 40rpx auto 0 auto;
}
/*分享底部菜单弹框样式*/
.assemble-detail .shareWX {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
   height: 100%;

}
.assemble-detail .shareWX .btnBox{
  height: 306rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
    animation: sport .2s;
    transform: translate3d(0, 0, 0);
}
.assemble-detail .shareWX__friendly, .assemble-detail .shareWX__takePhoto {
  text-align: center;
  font-size: 34rpx;
  line-height: 100rpx;
}

.assemble-detail .shareWX__takePhoto {
  border-top: 1rpx solid #e8e8e8;
}

.assemble-detail .shareWX__cancel {
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  line-height: 100rpx;
  border-top: 4rpx solid #e8e8e8;
}
/*公共的蒙层样式*/
 .dialogWrap {
   width: 100%;
   height: 100%;
   background: rgba(0, 0, 0, 0.5);
   position: fixed;
   top: 0;
   left: 0;
   transform: translate3d(0, 0, 0);
   animation: .2s opacity2;
   -webkit-animation: .2s opacity2;
 }

.assemble-detail button::after {
  border: none;
}

.assemble-detail button {
  background-color: #fff;
  border-radius: 0rpx;
}

@keyframes opacity2 {
  0% {
    opacity: 0;
  }
  50% {
    opacity: .5;
  }
  100% {
    opacity: 1;
  }
}

@-webkit-keyframes opacity2 {
  0% {
    opacity: 0;
  }
  50% {
    opacity: .5;
  }
  100% {
    opacity: 1;
  }
}
@keyframes sport {
  0% {
    transform: translateY(150rpx);
    opacity: 1;
  }
  25% {
    transform: translateY(112.5rpx);
    opacity: 1;
  }

  50% {
    transform: translateY(75rpx);
    opacity: 1
  }

  75% {
    transform: translateY(37.5rpx);
    opacity: 1;
  }
  100% {
    transform: translateY(0px);
    opacity: 1
  }
}

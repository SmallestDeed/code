@import "/utils/wxParse/wxParse.wxss";

page {
    background-color: #fff;
    min-height: 100%;
}

.toparea {
    width: 750rpx;
    height: 400rpx;
    background-color: #f5f5f5;
}
.toparea image{
    width: 100%;
    height: 100%;
}

.white {
    width: 750rpx;
    background-color: #fff;
}

.information {
    width: 690rpx;
    margin: 40rpx auto 0;
    box-shadow: 0px 0px 40px 0px 
		rgba(204, 204, 204, 0.4);
	border-radius: 10rpx;
    box-sizing: border-box;
    padding: 0 40rpx;
}
.information .items{
    box-sizing: border-box;
    padding: 24rpx 60rpx 24rpx 140rpx;
    line-height: 48rpx;
    font-size: 28rpx;
    min-height: 96rpx;
    color: #333333;
    position: relative;
}
.information .items .txts{
    width: 120rpx;
    position: absolute;
    text-align: right;
    height: 48rpx;
    top: 24rpx;
    left: 0;
    color: #999999;
}
.information .items image{
    width: 48rpx;
    height: 48rpx;
    position: absolute;
    top: 24rpx;
    right: 0;
}
.information .items.as:after{
    content: '';
    position: absolute;
    width: 100%;
    height: 2rpx;
    transform: scaleY(.5);
    bottom: 0;
    left: 0;
    background: #e8e8e8;
}
.profile {
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
}

.profile .text {
    font-size: 32rpx;

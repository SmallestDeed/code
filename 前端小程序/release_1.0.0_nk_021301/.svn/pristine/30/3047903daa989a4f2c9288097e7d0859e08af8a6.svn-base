/* pages/goods-details-plan/goods-details-plan.wxss */
/* 案例样式 */
page{
  background-color: #f5f5f5;
}
.goods-details-case {
  margin-top:30rpx;
  margin-left:30rpx;
  margin-bottom: 40rpx;
  width: 690rpx;
  /* background-color:#f5f5f5; */
}
.goods-details-case .case-item {
  height:560rpx;
  /* background-color:#f5f5f5;   */
  margin-bottom: 40rpx;
}
.goods-details-case .case-item .case-image{
  height:420rpx;
  
}
.goods-details-case .case-item .case-image .bigPth {
  width:100%;
  height: 100%;
  border-top-left-radius:10rpx;
  border-top-right-radius: 10rpx; 
}
.goods-details-case .case-item .case-text {
  height:140rpx;
  background-color: white;
  border-bottom:1rpx solid #e8e8e8;
  border-left: 1rpx solid #e8e8e8;
  border-right:1rpx solid #e8e8e8;
  border-bottom-left-radius:10rpx;
border-bottom-right-radius:10rpx;

}
.goods-details-case .case-item .case-image {
  position:relative;
}
.goods-details-case .case-item .case-image .Renovation-house {
  width: 171rpx;
  height: 60rpx;
	background-color: #ff6419;
	border-radius: 30px 0px 0px 30px;
  position: absolute;
  bottom: 35rpx;
  right:0;
  color:#ffffff;
  font-size: 28rpx;
  line-height: 60rpx;
  padding-left:39rpx;
}
.case-image .Renovation-house .arrow {
  width:11rpx;
  height:24rpx;
  position:absolute;
  right:30rpx;
  top:17rpx;
 
}
.case-image .seven-two-ico{
  position: absolute;
  bottom:28rpx;
  left:30rpx;
}
.case-image .seven-two-ico .identical-class{
  width:70rpx!important;
  height: 70rpx!important;
  margin-right: 39rpx;
  float: left;
}
.seven-two-ico .identical-class image {
  width:70rpx;
  height: 70rpx;
}
.goods-details-case .case-item .case-text {
  padding-left:31rpx;
}
.case-text .case-mes {
  padding-top:31rpx;
}
.case-mes .case-mes-name {
  font-size: 32rpx;
  color:#333;
}
.case-mes .case-mes-function .collet, .case-mes .case-mes-function .thumbs-up {
  margin-right:26rpx;
  vertical-align:middle;
}
.case-mes .case-mes-function image{
  width:47rpx;
  height: 47rpx;
  float: left;
}
.case-mes .case-mes-function text {
  color:#999999;
  font-size: 24rpx;
  line-height: 47rpx;
  float:left;
}
.case-text .case-style {
  font-size: 26rpx;
  line-height: 57rpx;
  color:#999;
}
/* 底部样式 */
.goods-details-footer {
  height:98rpx;
  line-height: 98rpx;
  font-size: 32rpx;
  color:#ffffff;
  position:fixed;
  bottom:0;
  left:0;
}
.goods-details-footer .footer-purchase .purchase-icon{
  width: 310rpx;
  height: 98rpx;
  border-top: solid #f5f5f5 1px;
  background-color: #fff;
  position:relative;
}
.goods-details-footer .footer-purchase .purchase-icon image{
  width:48rpx;
  height: 48rpx;
  margin: 25rpx 0 0 70rpx;
}
.goods-details-footer .footer-purchase .purchase-icon .count {
  position:absolute;
  left:226rpx;
  top:18rpx;
  color: #fff;
  width: 28rpx;
  height: 28rpx;
  font-size: 18rpx;  
  line-height: 28rpx;
  text-align: center;
  background-color: #ff6419; 
  border-radius: 50%;
}
.goods-details-footer .footer-purchase .purchase-join-car{
  width: 220rpx;
  height: 98rpx;
  text-align: center;
  background-color: #ffbb33;
}
.goods-details-footer .footer-purchase .purchase-join-carban{
  width: 220rpx;
  height: 98rpx;
  text-align: center;
  background-color: #e0e0e0;
}
.goods-details-footer .footer-purchase .purchase-btn{
  width: 220rpx;
  height: 98rpx;
  text-align: center;
  background-color: #ff6419;
}
.goods-details-footer .footer-purchase .purchase-btnban{
  width: 220rpx;
  height: 98rpx;
  text-align: center;
  background-color: #e0e0e0;
}
.goods-details-footer .footer-car {
  height:98rpx;
  width: 750rpx;
  line-height: 98rpx;
  text-align: center;
	color: #ffffff;
	font-size: 32rpx;
	background-color: #ff6419;
}
/* 渲染模板新增 */
button {
  background-color: transparent !important;
  border: none;
  border-radius: 0rpx !important;
	color: #ff6419 !important;
  font-size: 32rpx;
  outline: none;
  padding:0 !important;
}
button:after{
  border: none;
  outline: none;
}
.goods-details-case .case-item .case-image button {
  width:100%;
  height: 100%;
  background: #fff;
}
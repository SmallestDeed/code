page {
    display: block;
    min-height: 100%;
    background-color: #f5f5f5;
}

button[class="userInfo"]{
  width: 200rpx;
	height: 68rpx;
	border-radius: 34rpx;
	border: solid 1rpx #cccccc;
  font-size: 32rpx;
  color: #333333;
  line-height: 68rpx;
  background-color: #fff;
  margin-left: 10rpx;
}
button[class="userInfo button-hover"]{
  width: 200rpx;
	height: 68rpx;
	border-radius: 34rpx;
	border: solid 1rpx #cccccc;
  font-size: 32rpx;
  color: #333333;
  line-height: 68rpx;
  background-color: #fff;
  margin-left: 10rpx;
}
button[class="userInfo"]::after {
  border: 0;
} 
.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    width: 100%;
}

.flex-iteam {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

.head-portrait {
	  position: relative;
    width: 100%;
    height: 200rpx;
    background-color: #ffffff;
    overflow: hidden;
}


.head-info{
	width: 100%;
	height: 200rpx;
	background-color: #ffffff;
  padding: 62rpx 40rpx;
  padding-left: 185rpx;
  padding-right: 80rpx;
  box-sizing: border-box;
  position: relative;
}
.head-info>image.userImg{
  width: 110rpx;
	height: 110rpx;
  position: absolute;
  left: 40rpx;
  top: 45rpx;
  border-radius: 50%;
  background: #e8e8e8;
}
.head-info>view{
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  min-height: 26rpx;
  line-height: 1;
}
.head-info>view.p1{
  font-family: PingFang-SC-Bold;
	font-size: 32rpx;
	font-weight: 600;
	color: #403834;
  margin-bottom: 20rpx;
  line-height: 36rpx;
}
.head-info>view.p2{
  font-family: PingFang-SC-Bold;
	font-size: 26rpx;
	font-weight: 600;
	color: #ffaa33;
  position: relative;
  padding-left: 40rpx;
}
.head-info>view.p2 image{
  position: absolute;
  left: 0;
  top:1rpx;
  height: 24rpx;
  width: 25rpx;
}
.head-info>image.goNext{
  width: 15rpx;
	height: 28rpx;
  position: absolute;
  top: 50%;
  right: 40rpx;
  margin-top: -14rpx;
}

.person-nav {
    width: 100%;
    margin: 20rpx 0;
    background-color: #ffffff;
}
.person-nav-top {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.preson-nav-iteam {
    text-align: center;
    margin-top:30rpx;
    margin-bottom: 30rpx;
}
.preson-nav-image{
  height: 140rpx;
  width: 690rpx;
  margin-left: 30rpx;
  margin-top:40rpx;
  margin-bottom: 40rpx;
  box-shadow: 0rpx 8rpx 16rpx 0rpx 
		rgba(255, 194, 75, 0.4);
	border-radius: 10rpx;
}

.list-cells {
    position: relative;
    margin-top: 20rpx;
    background-color: #fff;
    line-height: 1.41176471;
    font-size: 17rpx;
}

.list-cells.first,.list-cell.about {
	border: none;
}
.list-cell{
	width: 720rpx;
	margin-left:30rpx;
	height: 108rpx;
    border-bottom: 1rpx solid #e8e8e8;

}
.list-cell__bd,.list-cell__ft{
	height: 108rpx;
	line-height: 108rpx;

}
.list-cell__bd{
	float: left;
	font-size: 28rpx;
	color: #333333;
  display: flex;
  align-items: center;
}
.lise-cell_txt{
  margin-left: 20rpx;
}
.list-cell__ft{
	margin-right:30rpx;
	float: right;
	font-size: 24rpx;
	color: #999999;
}
.lise-cell_icon{
  height: 40rpx;
  width: 38rpx;
  
}
.preson-nav-iteam{
	font-size: 24rpx;
	color: #333333;
	text-align: center;
}
.preson-nav-iteam .img{
	margin:0 auto;
text-align:center;
margin-bottom:20rpx;
width:56rpx;
height:56rpx;
overflow:hidden;


}
.preson-nav-iteam .img image{
	vertical-align: middle;
	width: 56rpx;
	height: 56rpx;
	margin:0 auto;
}
.list-cell.first{
	border: none;
}
.list-cell__ft image{
	width: 15rpx;
	height: 28rpx;
	vertical-align:-3rpx;
}
/* 绑定手机号样式 */
.bindingphone{
  box-sizing: border-box;
  width: 100%;
  height:80rpx;
  background-color: #ffeee5;
  overflow: hidden;
}
.bindingphone .phone-text{
  color:#ff6419;
  font-size: 24rpx;
  line-height: 80rpx;
  margin-left: 71rpx;
  margin-right: 20rpx;  
}
.bindingphone .phone-btn{
  width: 130rpx;
  height: 48rpx;
  color: #fff;
  border-radius: 24rpx;
  text-align: center;
  line-height: 48rpx;
  font-size: 24rpx;
  margin-top: 16rpx;
  background-color: #ff6419;
}




                </text>
                <!-- <text class="deatail-count fr">共<text class="deatail-count-count">{{item.orderProductList.length}}</text>件商品</text> 
                <text class="deatail-count ">共<text class="deatail-count-count">{{item.orderProductList.length}}</text>件商品</text>
            </view>
        </view>
        <view class='deleteBtn fr' catchtap='delete' data-item="{{item}}">删除订单</view>
        </block> -->
    </view>

var filters = {
    toFix: function(v, n) {
        return v.toFixed(n);
    },
    phoneFilter: function(v) {
        var arr = v.split('');
        var str = '';
        for(var i = 0;i < arr.length; i++){
            str = str + ((i<=2||i>=7)?arr[i]:'*')
        }
        return str;
    }
}
module.exports = {
    toFix: filters.toFix,
    phoneFilter: filters.phoneFilter
}
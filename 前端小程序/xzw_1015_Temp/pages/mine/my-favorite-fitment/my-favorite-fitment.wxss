/* pages/house-type/house-type.wxss */
page {
    display: block;
    height: 100%;
    background-color: #f5f5f5;
}
.flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    background: rgb(255, 255, 255)
}
.list{
  padding: 30rpx 30rpx 0;
}
.list-item{
  height: auto;
  background-color: #ffffff;
	border-radius: 10rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.item-img{
   width: 100%;
  height: 388rpx;
  position: relative;
}
.operate{
  position: absolute;
  top: 0;
  right: 0;
  width: 68rpx;
	height: 44rpx;
  background:rgba(17, 17, 17, 0.5);
	border-radius: 0rpx 10rpx 0rpx 20rpx;
  color: #fff;
  text-align: center;
  line-height: 22rpx;
  letter-spacing:5rpx;
}
.operate-box{
  position: absolute;
  top:70rpx;
  right:20rpx;
  width: 160rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 6px;
  z-index: 11
}
.operate-box .options{
  height: 80rpx;
  font-size: 28rpx;
  color: #333333;
  text-align: center;
  line-height: 80rpx;
}
.item-img .btn{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 180rpx;
	height: 68rpx;
  opacity: 0.9;
}
.item-img image{
  width: 100%;
  height: 100%;
}
.bottom-box{
  padding: 30rpx;
  justify-content: space-between;
}
.left{
  
}
.left .name{
  width: 260rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.left .date{
  width: auto;
  /* margin-top: 15rpx;  */
  font-size: 24rpx;
  color: #999999;
}
.bottomBtn{
  display: flex;
  margin-top: 50rpx;
}
.price-btn{
  width: 148rpx;
	height: 60rpx;
  margin: 10rpx 0 0 0rpx;
	border-radius: 6rpx;
	border: solid 1rpx #ff8c19;
  font-size: 28rpx;
  color: #ff8c19;
  line-height: 60rpx;
  text-align: center;
}

.continue-btn{
  width: 148rpx;
	height: 60rpx;
  margin: 10rpx 0 0 16rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  text-align: center;
  background-color:#fff;
  border-radius:6rpx;
  color:#29cccc;
  border: solid 1rpx #29cccc;

  
}
.shareBtn{
  width: 148rpx;
	height: 60rpx;
  margin: 10rpx 0 0 16rpx;
	border-radius: 6rpx;
  font-size: 28rpx;
  color: #29cccc;
  line-height: 60rpx;
  text-align: center;
  background-color: #fff;
  border:solid 1rpx #29cccc;
  box-sizing: inherit;
  padding: 0;
}

.shareBtn::after{
  border: none;
}
.rechristen{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  z-index: 12
}
.rechristen-box{
  width: 540rpx;
  height: 343rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 420rpx auto 0;
  overflow: hidden;
}
.rechristen-box .title{
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  padding: 60rpx 0 40rpx;
  text-align: center;
  line-height: 34rpx;
}
.rechristen-box input{
  width: 360rpx;
	height: 80rpx;
	border: solid 1rpx #999999;
  margin: 0 auto;
  font-size: 28rpx;
  padding: 0 30rpx;
  margin-bottom: 40rpx;
}
.btn-box{
  height: 88rpx;
  font-size: 32rpx;
  font-weight: bold;
  border-top: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
}
.cancel{
  width: 50%;
  color: #999999;
  line-height: 88rpx;
  text-align: center;
}
.confirm{
  width: 50%;
  color: #29cccc;
  box-sizing: border-box;
  border-left: 1rpx solid #f5f5f5;
  line-height: 88rpx;
  text-align: center;
}


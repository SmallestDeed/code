/* pages/decoration/supplyDetail/supplyDetail.wxss */
page{
  background-color: #ffffff;
}
/* 顶部 */
.header{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  background-color: #fff;
}
.tab-control{
  height: 88rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  border-bottom: 1rpx solid #f5f5f5;
}
.line{
  width: 2rpx;
  height: 26rpx;
  background-color: #e8e8e8;
  margin: 31rpx 80rpx;
}
.supply{
  width: 200rpx;
  text-align: center;
  color: #333333;
  line-height: 88rpx;
}
.supplyselect{
  width: 200rpx;
  text-align: center;
  color: #ff6419;
  line-height: 88rpx;
}
.choose{
  height: 88rpx;
  font-size: 28rpx;
  color: #333333;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-around;
}
.choose-item{
  width: 30%;
  display: flex;
  justify-content: center;
}
.choose-item view{
  line-height: 88rpx;
}
.choose-item image{
  width:16rpx;
  height: 16rpx;
  margin: 39rpx 0 0 10rpx;
}

/* 选项弹框 */
.options{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}
.options-box{
  margin-top: 177rpx;
  background-color: #fff;
}
.rank-item{
  height: 88rpx;
  color: #333333;
  line-height: 88rpx;
  font-size: 28rpx;
  text-align: center;
  border-bottom: 1rpx solid #f5f5f5;
}
.rank-item-active{
  height: 88rpx;
  color: #ff6419;
  line-height: 88rpx;
  font-size: 28rpx;
  text-align: center;
  border-bottom: 1rpx solid #f5f5f5;
}
.promulgator-box{
  padding: 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.promulgator-item{
  width: 216rpx;
	height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  text-align: center;
  color: #333333;
	background-color: #f5f5f5;
	border-radius: 5rpx;
  margin: 10rpx;
}
.promulgator-item-active{
  width: 216rpx;
	height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ff6419;
	background-color:  #fff1eb;
	border-radius: 5rpx;
  margin: 10rpx;
}
.site-box{
  position: relative;
  height: 767rpx;
  display: flex;
  justify-content: space-between;
}
.left{
  width: 35%;
  background-color: #fff;
  overflow-y: auto;
}
.left-item{
  padding-left: 30rpx;
	height: 88rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 88rpx;
}
.left-item-active{
  padding-left: 30rpx;
	height: 88rpx;
  font-size: 28rpx;
  color: #ff6419;
  line-height: 88rpx;
  background-color: #f5f5f5;
}
.right{
  width: 65%;
  background-color: #f5f5f5;
  overflow-y: auto;
}
.common-hot-box{
  padding: 0 30rpx;
}
.common-hot-title{
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  line-height: 88rpx;
}
.item-box{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  overflow-y: auto;
}
.item-box .item{
  width: 190rpx;
	height: 56rpx;
  font-size: 28rpx;
  line-height: 56rpx;
  margin: 10rpx;
  text-align: center;
  color: #333333;
	background-color: #ffffff;
	border-radius: 6rpx;
	border: solid 1rpx #e8e8e8;
}
.ordinary-city-box{
  padding: 0 30rpx 0 60rpx;
}
.city-item{
  position: relative;
	height: 88rpx;
  font-size: 28rpx;
  line-height: 88rpx;
  color: #333333;
}
.city-item-active{
  position: relative;
	height: 88rpx;
  font-size: 28rpx;
  line-height: 88rpx;
  color: #ff6419;
}
.item-img{
  position: absolute;
  top: 34rpx;
  right: 0;
  width: 24rpx;
  height: 20rpx;
}



/* 列表 */
.list{
  padding: 216rpx 40rpx 0;
}
list-active{
  padding: 216rpx 40rpx 0;
  height: 100%;
  overflow: hidden;
}
.list-item{
  height: 210rpx;
  margin-bottom: 60rpx;
  display: flex;
  justify-content: space-between;
}
.list-item image{
  width: 280rpx;
	height: 210rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.message{
  width: 360rpx;
  overflow: hidden;
}
.title{
  max-height: 90rpx;
  line-height: 45rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #403834;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.site{
  font-size: 24rpx;
  color: #999999;
  margin-top: 20rpx;
}
.data-box{
  font-size: 24rpx;
  margin-top: 20rpx;
  color: #999999;
  display: flex;
  justify-content: flex-start;
}
.empty{
  width: 100%;
  margin-top: 100rpx;
  font-size: 28rpx;
  text-align: center;
  color: #333;
  overflow: hidden;
}
.empty image{
  width: 100%;
}
.empty view{
 color: #999;
}



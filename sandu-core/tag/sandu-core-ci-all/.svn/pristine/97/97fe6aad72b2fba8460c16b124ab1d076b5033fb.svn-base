  <select id="selectAllDemandPlanAndHouse" resultType="com.sandu.api.base.input.UserDecorationPlanVo">
        SELECT
		su.id AS ownerUserId,
		bsd.id AS supplyDemandId,
		dr.plan_id AS planId,
		dr.plan_type AS planType,
		su.uuid
		from demand_info_rel dr
					left join base_supply_demand bsd on bsd.id = dr.supply_demand_id
					left join sys_user su on su.id = bsd.creator_id and su.is_deleted = 0
					where
				    dr.plan_id > 0 and (dr.plan_type = 3 or dr.plan_type = 4)
					and bsd.type = 2
					and bsd.is_deleted = 0
					and su.uuid = #{ownerUserSessionId}
					and bsd.id in (SELECT supply_demand_id from design_plan where user_id = #{userId} and supply_demand_id is not null)
  </select>

  <select id="getDemandHouse" resultType="com.sandu.api.base.input.UserDecorationPlanVo">
        SELECT
		su.id AS ownerUserId,
		bsd.id AS supplyDemandId,
		dr.house_id AS houseId,
		su.uuid
		from demand_info_rel dr
					left join base_supply_demand bsd on bsd.id = dr.supply_demand_id
					left join sys_user su on su.id = bsd.creator_id and su.is_deleted = 0
					where
				    dr.house_id > 0
					and bsd.type = 2
					and bsd.is_deleted = 0
					and su.uuid = #{currentChatUserSessionId}
					and bsd.id in (SELECT supply_demand_id from design_plan where user_id = #{userId} and supply_demand_id is not null)
  </select>

  <select id="selectAllMsgPlanAndHouse" parameterType="object" resultType="com.sandu.api.base.input.UserDecorationPlanVo">
      SELECT
      su.id AS ownerUserId,
      mhm.msg_type AS msgType,
      mhm.msg_body AS msgBody,
      mhm.id AS msgHistoryMessageId
      from msg_history_message  mhm
      left join sys_user su on su.uuid = mhm.from_user_session_id and su.is_deleted = 0
      where mhm.from_user_session_id = #{ownerUserSessionId} AND mhm.to_user_session_id = #{loginUserSessionId}
      and ( mhm.msg_type = 2 or mhm.msg_type = 3 or mhm.msg_type = 4)
      and mhm.is_deleted = 0
  </select>
    <select id="listByIds" resultType="com.sandu.api.base.model.SysUser">
      select id,head_pic from sys_user where is_deleted = 0
      and <foreach collection="ids" separator="," item="it" open="(" close=")">
      		#{it}
      </foreach>
    </select>



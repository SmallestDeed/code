<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- 生成远程服务代理，可以像使用本地bean一样使用demoService -->
    <dubbo:reference id="redisService"
                     interface="com.sandu.api.base.service.RedisService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="basePlatformService"
                     interface="com.sandu.api.base.service.BasePlatformService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="baseCompanyService"
                     interface="com.sandu.api.base.service.BaseCompanyService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="baseProductStyleService"
                     interface="com.sandu.api.base.service.BaseProductStyleService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="sysUserService"
                     interface="com.sandu.api.base.service.SysUserService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="sysDictionaryService"
                     interface="com.sandu.api.base.service.SysDictionaryService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="designPlanStoreReleaseService"
                     interface="com.sandu.panorama.service.DesignPlanStoreReleaseService" retries="${dubbo.retries}" version="1.0.1"/>

    <dubbo:reference id="baseBrandService"
                     interface="com.sandu.api.base.service.BaseBrandService" retries="${dubbo.retries}" version="${dubbo.version}"/>
</beans>
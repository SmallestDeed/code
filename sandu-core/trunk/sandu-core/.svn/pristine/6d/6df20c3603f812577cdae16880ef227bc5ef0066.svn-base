    <artifactId>sandu-web-core</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>jar</packaging>

    <dependencies>
	
		<dependency>
			<groupId>com.sandu.collect</groupId>
			<artifactId>collect-client</artifactId>
			<version>1.0.5</version>
		</dependency>
	
        <dependency>
            <groupId>com.sandu</groupId>
            <artifactId>sandu-api-base</artifactId>

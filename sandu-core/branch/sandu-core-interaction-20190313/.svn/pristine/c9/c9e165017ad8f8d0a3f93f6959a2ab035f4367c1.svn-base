<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.base.dao.SupplyDemandPicMapper" >

  <select id="selectByIds" resultType="com.sandu.supplydemand.model.SupplyDemandPic">

    select * from supply_demand_pic where  is_deleted = 0
    and <foreach collection="ids" separator="," item="it" open="(" close=")">
    		#{it}
    </foreach>
  </select>
</mapper>
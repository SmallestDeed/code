package com.sandu.api.springFestivalActivity.output;

import lombok.AllArgsConstructor;
import lombok.Data;

import java.io.Serializable;

@Data
@AllArgsConstructor
public class UserTaskVo implements Serializable {
    /**
     * 用户任务表ID
     */
    private Long id;
    /**
     * 任务1状态（0：可领取，1：待完成，2：已完成，3：不可领取）
     */
    private Byte taskOneState;
    /**
     * 任务2状态（0：可领取，1：待完成，2：已完成，3：不可领取）
     */
    private Byte taskTwoState;
    /**
     * 任务3状态（0：可领取，1：待完成，2：已完成，3：不可领取）
     */
    private Byte taskThreeState;
}

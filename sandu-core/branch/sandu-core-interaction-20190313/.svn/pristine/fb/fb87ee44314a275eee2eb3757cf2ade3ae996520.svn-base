<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.sandu</groupId>
    <artifactId>sandu-core</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>${project.artifactId}</name>
    <description>Sandu deal Project</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>

        <sandu-jar.version>1.0.0-SNAPSHOT</sandu-jar.version>
        <springboot.version>1.5.12.RELEASE</springboot.version>
        <spring-mybatis.version>1.3.2</spring-mybatis.version>
        <spring-pagehelper.version>1.2.3</spring-pagehelper.version>
        <mysql-connector.version>5.1.44</mysql-connector.version>
        <zkclient.version>0.10</zkclient.version>
        <zookeeper.version>3.4.10</zookeeper.version>
        <dubbo.version>2.5.8</dubbo.version>
        <mybatis.version>3.4.6</mybatis.version>
        <common-lang3.version>3.7</common-lang3.version>
        <servlet-api.version>4.0.0</servlet-api.version>
        <druid.version>1.1.8</druid.version>
        <gson.version>2.8.2</gson.version>
        <guava.version>24.1-jre</guava.version>
        <lombok.version>1.16.18</lombok.version>
        <swagger.version>2.7.0</swagger.version>
        <nimbus-jose-jwt.version>4.41</nimbus-jose-jwt.version>
    </properties>

    <modules>
        <module>sandu-api-base</module>
        <module>sandu-provider-base</module>
        <module>sandu-api-house</module>
        <module>sandu-provider-house</module>
		<module>sandu-api-product</module>
        <module>sandu-provider-product</module>
		<module>sandu-api-design</module>
        <module>sandu-provider-design</module>
        <module>sandu-web-core</module>
    </modules>

	<distributionManagement>
        <repository>
            <id>Releases</id>
            <url>http://192.168.1.118:8081/nexus/content/repositories/releases/</url>
        </repository>
        <snapshotRepository>
            <id>Snapshots</id>
            <url>http://192.168.1.118:8081/nexus/content/repositories/snapshots/</url>
        </snapshotRepository>
    </distributionManagement>
    <repositories>
        <repository>
            <id>SanduDev</id>
            <name>Sandu Public Repositories</name>
            <url>http://192.168.1.118:8081/nexus/content/groups/public/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>
</project>
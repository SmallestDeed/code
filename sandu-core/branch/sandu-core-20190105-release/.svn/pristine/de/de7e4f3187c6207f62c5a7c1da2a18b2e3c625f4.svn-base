<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.sandu.api.base.service.RedisService"
                   ref="redisService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.BaseCompanyService"
                   ref="baseCompanyService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.BasePlatformService"
                   ref="basePlatformService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

	<dubbo:service interface="com.sandu.api.base.service.SysDictionaryService"
                   ref="sysDictionaryService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.BaseProductStyleService"
                   ref="baseProductStyleService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.SysUserService"
                   ref="sysUserService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.BaseBrandService"
                   ref="baseBrandService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.BaseProductCategoryService"
                   ref="baseProductCategoryService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.CompressImageService"
                   ref="compressImageService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.base.service.UserCardService"
                   ref="userCardService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>
</beans>
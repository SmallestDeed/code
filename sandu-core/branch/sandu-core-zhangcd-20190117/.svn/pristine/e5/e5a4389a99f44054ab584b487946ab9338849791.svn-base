package com.sandu.api.springFestivalActivity.model;

import lombok.Builder;
import lombok.Data;

import java.math.BigDecimal;
import java.util.Date;

@Data
@Builder(toBuilder = true)
public class WxUserSignin {
    /**
     * 主键
     */
    private Long id;
    /**
     * 活动ID(关联wx_spring_activity)
     */
    private Long activityId;
    /**
     * 用户ID
     */
    private Long userId;
    /**
     * 签到日期(年-月-日)
     */
    private Date signinDate;
    /**
     * 签到日期(精确到天)
     */
    private Integer signinDay;
    /**
     * 领取的红包金额
     */
    private BigDecimal redPacketNum;
    /**
     * 红包领取状态(0-待领取;1-已领取)
     */
    private Byte receiveStatus;
    /**
     * 红包领取时间
     */
    private Date receiveTime;
    /**
     * 是否兑换抽奖机会(0-否;1-是)
     */
    private Byte isLotteryFlag;
    /**
     * 创建者
     */
    private String creator;
    /**
     * 创建时间
     */
    private Date gmtCreate;
    /**
     * 修改人
     */
    private String modifier;
    /**
     * 修改时间
     */
    private Date gmtModified;
    /**
     * 是否删除
     */
    private Integer isDeleted;
}
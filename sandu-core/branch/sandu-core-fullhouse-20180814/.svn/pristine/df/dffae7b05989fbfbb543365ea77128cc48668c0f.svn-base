    <!-- 按主键查询 -->
    <select id="selectByPrimaryKey" resultMap="AllResultMap"  parameterType="java.lang.Integer">
        select
          <include refid="All_Column_List"/>
        from base_company
        where id = #{id,jdbcType=INTEGER}
    </select>

    <!-- 查询所有，可排序 -->
    <select id="selectList" resultMap="AllResultMap"
            parameterType="com.sandu.api.base.model.BaseCompany">
        select
        <include refid="All_Column_List"/>
        from base_company
        where 1=1
        <if test="sysCode!= null">and sys_code = #{sysCode,jdbcType=VARCHAR}</if>
        <if test="smallType!= null">and small_type = #{smallType,jdbcType=VARCHAR}</if>
        <if test="creator!= null">and creator = #{creator,jdbcType=VARCHAR}</if>

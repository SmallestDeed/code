    <select id="getCompanyByBrandId" resultMap="AllResultMap">
        select
          bc.id, bc.company_name, bc.company_url, bc.company_domain_name
        from base_brand bb left join base_company bc on bb.company_id = bc.id
        where bb.id = #{brandId}
        and bb.is_deleted = 0
    </select>

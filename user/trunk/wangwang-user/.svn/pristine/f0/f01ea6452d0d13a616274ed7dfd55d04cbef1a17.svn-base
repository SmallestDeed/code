package com.sandu.common.model.output;

import java.io.Serializable;

import lombok.Data;

/**
 * 通用的DTO
 * 
 * @author huangsongbo
 *
 */
@Data
public class CommonDTO implements Serializable {

	private static final long serialVersionUID = -3341178232251795838L;

	private boolean flag;
	
	private String message;

	public CommonDTO(boolean flag, String message) {
		super();
		this.flag = flag;
		this.message = message;
	}
	
}

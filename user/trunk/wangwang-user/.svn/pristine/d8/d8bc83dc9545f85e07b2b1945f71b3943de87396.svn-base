	<update id="delByPrimaryKey">
		UPDATE
		   base_company
		   SET
		   is_deleted = 1
		   WHERE
		   id = #{id}
	</update>

	<update id="updatePid">
		UPDATE
		   base_company
		   SET
		   pid = #{companyId}
		   WHERE
		   id = #{id}
		   and is_deleted = 0
	</update>

	<select id="selectFCompanys" resultType="com.sandu.api.user.model.bo.CompanyInfoBO">
		SELECT
			id,
			company_name AS NAME,
			company_logo logo,
			gmt_create AS gmtCreate
		FROM
			base_company
		WHERE
			id IN (
				SELECT
					u.company_id
				FROM
					sys_user u,
					user_jurisdiction j
				WHERE
					 u.id = j.user_id and u.mobile =#{mobile} and u.password=#{password} and u.user_type in(3,14) and platform_type=2
				AND j.jurisdiction_status = 1
				AND j.platform_id = #{platformId}
				AND u.is_deleted = 0
			)
	</select>

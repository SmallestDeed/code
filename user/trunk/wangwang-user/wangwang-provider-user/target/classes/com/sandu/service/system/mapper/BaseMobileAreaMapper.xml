<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.system.dao.BaseMobileAreaDao">

    <select id="queryBaseMobileAreaByMobilePrefix" resultType="com.sandu.api.system.model.BaseMobileArea">
        SELECT
           *
           FROM
           base_mobile_area
           WHERE
           mobile_prefix = #{mobilePrefix}
    </select>

</mapper>
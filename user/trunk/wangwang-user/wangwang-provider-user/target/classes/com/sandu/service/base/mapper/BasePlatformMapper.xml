<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.base.dao.BasePlatformDao">

    <select id="selectByPlatformCode" resultType="com.sandu.api.base.model.BasePlatform">
		select * from base_platform
			where platform_code=#{platformCode}
	</select>

	<select id="queryList" resultType="com.sandu.api.base.model.BasePlatform">
		SELECT
		 *
		 FROM
		 base_platform
		 WHERE
		 is_deleted = 0
	</select>
</mapper>

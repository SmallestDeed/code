<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sandu.service.base.dao.BaseCompanyCustomLoginConfigDao">

    <select id="selectByCompanyId" resultType="com.sandu.api.base.model.BaseCompanyCustomLoginConfig">
        SELECT * FROM
        base_company_custom_login_config
        WHERE is_deleted = 0
        AND  company_id = #{companyId}
    </select>

</mapper>
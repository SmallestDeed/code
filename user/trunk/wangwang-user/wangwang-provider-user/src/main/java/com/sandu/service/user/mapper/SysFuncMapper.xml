<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.user.dao.SysFuncDao">
	<select id="selectById" resultType="com.sandu.api.user.model.SysFunc">
		select *
			from sys_func
			where id = #{funcId}
			and is_deleted = 0
	</select>
</mapper>

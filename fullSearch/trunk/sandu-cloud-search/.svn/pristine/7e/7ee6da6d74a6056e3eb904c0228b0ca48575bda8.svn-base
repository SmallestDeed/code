package com.sandu.search.exception;

/**
 * Rabbit异常
 *
 * @date 2018/5/4
 * @auth pengxuangang
 * @mail xuangangpeng@gmail.com
 */
public class RabbitException extends Exception {

    private final static String CLASS_LOG_PREFIX = "[RABBIT异常]:";

    public RabbitException() {
    }

    public RabbitException(String message) {
        super(CLASS_LOG_PREFIX + message);
    }

    public RabbitException(String message, Throwable cause) {
        super(CLASS_LOG_PREFIX + message, cause);
    }

    public RabbitException(Throwable cause) {
        super(cause);
    }

    public RabbitException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(CLASS_LOG_PREFIX + message, cause, enableSuppression, writableStackTrace);
    }
}

public class GoodsIndexException extends Exception{

    public GoodsIndexException() {
    }

    public GoodsIndexException(String message) {
        super(message);
    }

    public GoodsIndexException(String message, Throwable cause) {
        super(message, cause);
    }

    public GoodsIndexException(Throwable cause) {
        super(cause);
    }

    public GoodsIndexException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }

}

package com.sandu.search.exception;

/**
 * 户型服务异常
 *
 * @date 20180109
 * @auth pengxuangang
 */
public class HouseIndexException extends Exception {
    public HouseIndexException() {
    }

    public HouseIndexException(String message) {
        super(message);
    }

    public HouseIndexException(String message, Throwable cause) {
        super(message, cause);
    }

    public HouseIndexException(Throwable cause) {
        super(cause);
    }

    public HouseIndexException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
}

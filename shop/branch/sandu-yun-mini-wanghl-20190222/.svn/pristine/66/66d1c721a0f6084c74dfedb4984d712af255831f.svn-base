spring.application.name=yun-cache-provider
debug=true

### Dubbo
dubbo.registry.address=127.0.0.1:2181
dubbo.protocol.port.cache=21010
dubbo.timeout=10000
dubbo.version=1.0.0
dubbo.retries=0

redis.pool.maxActive=10
redis.pool.maxIdle=5
redis.pool.minIdle=10
redis.pool.maxWaitMillis=20000
redis.pool.maxWait=300
redis.pool.uri=redis://:123456@127.0.0.1:6379/0

### Log
logging.config=classpath:log4j2.xml



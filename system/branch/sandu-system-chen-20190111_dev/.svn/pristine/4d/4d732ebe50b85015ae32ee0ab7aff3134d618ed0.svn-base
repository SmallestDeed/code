<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 生成远程服务代理，可以像使用本地bean一样使用demoService -->
	<dubbo:reference interface="com.sandu.api.collect.service.RecordMachineInfoOperationService"
                   id="recordMachineInfoOperationService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference interface="com.sandu.queue.service.QueueService"
                     id="queueService" retries="${dubbo.retries}" version="${dubbo.version}"/>

</beans>
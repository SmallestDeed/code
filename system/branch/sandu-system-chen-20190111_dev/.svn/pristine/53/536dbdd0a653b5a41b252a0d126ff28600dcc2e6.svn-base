  <!-- 根据参数 查询 是否存在这些分类id -->
  <select id="selectCountByIdList" resultType="java.lang.Integer">
    SELECT
        COUNT(1)
    FROM
        pro_category
    WHERE id IN
          <foreach item="item" collection="idList" separator="," open="(" close=")" index="">
              #{item}
          </foreach>
      AND level = ${level}
      AND is_deleted = 0
  </select>


  <!-- 根据codeList 获 集合 -->
  <select id="selectListByCodeList" resultType="com.sandu.api.category.output.CategoryListVO">
    SELECT
    id    AS categoryId,
    name  AS categoryName
    FROM
    pro_category
    WHERE code IN
    <foreach item="item" collection="codeList" separator="," open="(" close=")" index="">

  <!-- 根据分类id 层级 查询 下级 分类列表 -->
  <select id="getCategoryListByPidLevel" resultType="com.sandu.api.category.output.ProCategoryListVO" parameterType="com.sandu.api.category.input.ProCategoryListQuery">
    SELECT
        id    AS categoryId,
        name  AS categoryName
    FROM
        pro_category
    WHERE pid = #{categoryId,jdbcType=INTEGER}
          <if test="level != null and level != '' ">
              <![CDATA[ AND level <= ${level} ]]>
          </if>
      AND is_deleted = 0
  </select>

  <!-- 根据id集合 查询 名称 字符串 -->
  <select id="selectCategoryNames" resultType="java.lang.String">
    SELECT
        GROUP_CONCAT(name)
    FROM
        pro_category
    WHERE id IN
          <foreach collection="idList" item="item" open="(" separator="," close=")">
            #{item}
          </foreach>
      AND is_deleted = 0
  </select>


  <!-- 通过子级ID查父级ID -->
  <select id="findParentId" resultType="java.lang.Integer">
    SELECT
      DISTINCT pc.pid
    FROM
      pro_category pc
    WHERE pc.id IN
          <foreach collection="idList" item="item" open="(" separator="," close=")">
              #{item}
          </foreach>
      AND is_deleted = 0
  </select>


  <!-- 根据idList 获取 上级 id 集合 -->
  <select id="selectPCategoryListByIdList" resultType="java.lang.Integer">
    SELECT
        DISTINCT pid
    FROM
        pro_category
    WHERE pid > 0
      AND id IN
          <foreach item="item" collection="idList" separator="," open="(" close=")" index="">
              #{item}
          </foreach>
      AND is_deleted = 0
  </select>

  <!-- 根据idList 获 集合 -->
  <select id="selectListByIdListPid" resultType="com.sandu.api.category.output.ProCategoryListVO">
    SELECT
        id    AS categoryId,
        name  AS categoryName
    FROM
        pro_category
    WHERE pid = #{pid}
      AND id IN
          <foreach item="item" collection="idList" separator="," open="(" close=")" index="">
              #{item}
          </foreach>
      AND is_deleted = 0
  </select>



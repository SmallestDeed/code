  <update id="batchUpdateBusinessId" parameterType="com.sandu.api.pic.model.po.ResPicBusinessUpdate">
      UPDATE res_pic
        <set>
          business_id = #{businessId,jdbcType=INTEGER},
        </set>
      where 1=1
        <choose>
          <when test="idList != null and idList.size() > 0">
            and id IN
            <foreach item="item" collection="idList" separator="," open="(" close=")" index="">
              #{item}
            </foreach>
          </when>
          <otherwise>
            and id = -1
          </otherwise>
        </choose>
  </update>


  <delete id="deleteLogicByPrimaryKey">
    UPDATE
      sys_dictionary
    SET
      is_deleted = 1,
      modifier = #{loginName,jdbcType=VARCHAR},
      gmt_modified = NOW(),
      remark = concat( remark,';delMark;')
    WHERE id = #{id,jdbcType=BIGINT}
  </delete>


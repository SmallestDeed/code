<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 生成远程服务代理，可以像使用本地bean一样使用demoService -->
    <dubbo:reference interface="com.sandu.api.servicepurchase.serivce.biz.ServicesPurchaseBizService"
                  id="servicesPurchaseBizService" protocol="dubbo" timeout="30000"
                  version="${dubbo.version}"/>
    <dubbo:reference interface="com.sandu.api.servicepurchase.serivce.ServicesPriceService"
                     id="servicesPriceService" protocol="dubbo" timeout="${dubbo.timeout}"
                     version="${dubbo.version}"/>
    <dubbo:reference interface="com.sandu.api.servicepurchase.serivce.ServicesBaseInfoService"
                     id="servicesBaseInfoService" protocol="dubbo" timeout="${dubbo.timeout}"
                     version="${dubbo.version}"/>
	<dubbo:reference interface="com.sandu.api.servicepurchase.serivce.ServicesPurchaseRecordService"
                     id="servicesPurchaseRecordService" protocol="dubbo" timeout="${dubbo.timeout}"
                     version="${dubbo.version}"/>
    <dubbo:reference interface="com.sandu.api.servicepurchase.serivce.ServicesRoleRelService"
                     id="servicesRoleRelService" protocol="dubbo" timeout="${dubbo.timeout}"
                     version="${dubbo.version}"/>
    <dubbo:reference interface="com.sandu.api.servicepurchase.serivce.ServicesAccountRelService"
                     id="servicesAccountRelService" protocol="dubbo" timeout="${dubbo.timeout}"
                     version="${dubbo.version}"/>

</beans>
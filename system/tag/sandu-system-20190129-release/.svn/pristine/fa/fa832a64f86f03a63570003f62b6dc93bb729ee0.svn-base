<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sandu.service.pointmall.dao.UsersDao">

    <resultMap id="BaseResultMap" type="com.sandu.api.pointmall.model.Users" >
        <id column="uid" property="uid" />
        <result column="nickname" property="nickname" />
        <result column="mobile" property="mobile" />
        <result column="email" property="email" />
        <result column="password" property="password" />
        <result column="gender" property="gender" />
        <result column="avatar" property="avatar" />
        <result column="status" property="status" />
        <result column="sys_code" property="sysCode" />
        <result column="creator" property="creator" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="modifier" property="modifier" />
        <result column="gmt_modified" property="gmtModified" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <sql id="Base_Column_List" >
        uid,nickname,mobile,email,password,gender,avatar,status,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted
    </sql>


    <select id="selectByPrimaryKey" resultMap="BaseResultMap" >
        select
        <include refid="Base_Column_List" />
        from users
        where uid = #{uid}
    </select>
</mapper>
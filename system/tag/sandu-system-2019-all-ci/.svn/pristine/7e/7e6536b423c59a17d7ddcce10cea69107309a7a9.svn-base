<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.imallOrderShipment.dao.ImallOrderShipmentItemDao">

    <!--保存发货单-->
    <insert id="insertImallOrderShipmentItem" parameterType="com.sandu.api.imallOrderShipment.model.ImallOrderShipmentItem" keyProperty="id" useGeneratedKeys="true">
        insert into imall_order_shipment_item(
        shipment_id,item_id,item_sku,item_name,item_description,item_original_price,item_original_point,
        item_point,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted)
        values(#{shipmentId},#{itemId},#{itemSku},#{itemName},#{itemDescription},#{itemOriginalPrice},#{itemOriginalPoint},#{itemPoint},#{sysCode},#{creator},CURRENT_TIMESTAMP,#{modifier},CURRENT_TIMESTAMP,#{isDeleted})
    </insert>

    <!--通过id获取发货单信息-->
    <select id="getImallOrderShipmentItem" resultType="com.sandu.api.imallOrderShipment.model.ImallOrderShipmentItem">
      select shipment_id,item_id,item_sku,item_name,item_description,item_original_price,item_original_point,item_point,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted
      from imall_order_shipment_item
      where id=#{id}
    </select>

    <!--通过order_id获取发货单列表-->
    <select id="getImallOrderShipmentList" resultType="com.sandu.api.imallOrderShipment.model.ImallOrderShipmentItem">
      select shipment_id,item_id,item_sku,item_name,item_description,item_original_price,item_original_point,item_point,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted
      from imall_order_shipment_item
      where shipment_id=#{shipmentId}
    </select>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.sandu.api.grouppurchase.service.biz.GroupPurchaseActivityBizService"
                   ref="groupPurchaseActivityBizService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.grouppurchase.service.biz.GroupPurchaseBizService"
                   ref="groupPurchaseBizService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.grouppurchase.service.GroupPurchaseOpenService"
                   ref="groupPurchaseOpenService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.grouppurchase.service.GroupPurchaseOpenDetailService"
                   ref="groupPurchaseOpenDetailService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <dubbo:service interface="com.sandu.api.grouppurchase.service.biz.GroupPurchaseOrderBizService"
                   ref="groupPurchaseOrderBizService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>
</beans>
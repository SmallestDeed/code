    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select gpa.*,
        bgs.product_name as spuName,
        sum(gpod.order_amount) as totalOrderAmount,
        count(gpod.order_id) as totalOrderCount,
        count(DISTINCT gpod.user_id) as totalOrderPerson,
        bgs.product_name as productText,
		rp.pic_path as productPath
        from group_purchase_activity gpa
        left join base_goods_spu bgs on gpa.spu_id = bgs.id
        left join group_purchase_open_detail gpod on gpa.id = gpod.purchase_activity_id and gpod.is_deleted = 0 and gpod.join_status =2 and gpod.user_type =1
        left join res_pic rp on rp.id = bgs.pic_id
        left join group_purchase_open gpo on gpa.id = gpo.purchase_activity_id and gpo.open_status != 3 and gpo.open_status != 4
        where gpa.is_deleted = 0
        and gpa.id = #{id,jdbcType=BIGINT}
        and gpod.purchase_open_id = gpo.id
    </select>
    <select id="findAll" resultMap="BaseResultMap">
        select
        bgs.product_name as spuName,
        t.totalOrderAmount,
        t.totalOrderCount,
        t.totalOrderPerson,
        gpa.*
        from group_purchase_activity gpa
        inner join base_goods_spu bgs on gpa.spu_id = bgs.id
        left join (
        SELECT
        gpod.purchase_activity_id,
        sum( gpod.order_amount ) AS totalOrderAmount,
        count( gpod.order_id ) AS totalOrderCount,
        count( DISTINCT gpod.user_id ) AS totalOrderPerson
        FROM
        group_purchase_open_detail gpod
        LEFT JOIN group_purchase_open gpo ON gpo.id = gpod.purchase_open_id
        WHERE
        gpo.open_status != 3
        AND gpo.open_status != 4
        AND gpod.is_deleted = 0
        AND gpod.join_status = 2
        AND gpod.user_type = 1
        GROUP BY
        gpod.purchase_activity_id
        ) t
        on gpa.id = t.purchase_activity_id
        where gpa.is_deleted = 0
        <if test="activityName != null and activityName != ''">
            and gpa.activity_name like concat ('%', #{activityName}, '%')
        </if>
        <if test="activityStatus != null">
            and gpa.activity_status = #{activityStatus}
        </if>
        <if test="companyId != null">
            and gpa.company_id = #{companyId}
        </if>
        order by gpa.gmt_create desc
    </select>
    <select id="getById" resultType="com.sandu.api.grouppurchase.model.GroupPurchaseActivity">
        select * from group_purchase_activity where id = #{id} and is_deleted = 0;
    </select>
    <select id="listExpireGroupActivity" resultType="com.sandu.api.grouppurchase.model.GroupPurchaseActivity">
        select * from group_purchase_activity where schedule_flag = 0 and (activity_end_time &lt; now() or activity_status in  (3,4));
    </select>

    <!--详情查询-->
    <select id="selectDetailById" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from sys_user_feedback
        where id = #{feedbackId}
        AND is_deleted=0
    </select>

    <!--前端列表查询-->
    <select id="selectListByUserId"
            resultType="com.sandu.api.feedback.output.SysFeedbackWebListVO">
        SELECT
          id                AS  feedbackId,
          feedback_content  AS  feedbackContent,
          gmt_create        AS  gmtCreate,
          deal_status       AS  dealStatus,
          is_read           AS  isRead
        FROM sys_user_feedback
        WHERE is_deleted = 0
        AND user_id = #{userId}
        AND platform_id = #{platformId}
        ORDER BY gmt_modified DESC
        LIMIT 10
    </select>


    <!--商家后台列表查询-->
    <select id="getByUserId"  resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM sys_user_feedback
        WHERE is_deleted = 0
        AND user_id = #{userId}
        AND platform_id = #{platformId}
        ORDER BY gmt_modified DESC
        LIMIT 10
    </select>


    <!--管理后台数量查询-->
    <select id="selectCount" resultType="java.lang.Integer"
            parameterType="com.sandu.api.feedback.input.SysFeedbackQuery">
        SELECT COUNT(suf.id)
        FROM sys_user_feedback suf
        LEFT JOIN base_company bc ON suf.company_id = bc.id
        where suf.is_deleted=0
        <if test="platformId != null ">
            AND suf.platform_id = #{platformId,jdbcType=TINYINT}
        </if>
        <if test="phone != null and phone != '' ">
            AND suf.phone  like CONCAT(CONCAT('%',#{phone,jdbcType=VARCHAR}),'%')
        </if>
        <if test="wxNickName != null and wxNickName != '' ">
            AND suf.wx_nick_name like CONCAT(CONCAT('%',#{wxNickName,jdbcType=VARCHAR}),'%')
        </if>
        <if test="estimate != null ">
            AND suf.estimate = #{estimate,jdbcType=TINYINT}
        </if>
        <if test="dealStatus != null ">
            AND suf.deal_status = #{dealStatus,jdbcType=TINYINT}
        </if>
        <if test="companyName != null and companyName != '' ">
            AND bc.company_name like CONCAT(CONCAT('%',#{companyName,jdbcType=VARCHAR}),'%')
        </if>
    </select>

    <!--管理后台列表查询-->
    <select id="selectList" resultType="com.sandu.api.feedback.output.SysFeedbackManageListVO"
            parameterType="com.sandu.api.feedback.input.SysFeedbackQuery">
        SELECT
        suf.id               AS feedbackId,
        suf.user_id          AS userId,
        suf.phone            AS phone,
        rp.pic_path          AS headPicPath,
        suf.open_id          AS openId,
        suf.user_name        AS userName,
        suf.company_id       AS companyId,
        bc.company_name      AS companyName,
        suf.wx_nick_name     AS wxNickName,
        suf.feedback_content AS  feedbackContent,
        suf.deal_status      AS dealStatus,
        suf.estimate         AS estimate,
        suf.gmt_create        AS gmtCreate
        FROM sys_user_feedback suf
        LEFT JOIN base_company bc ON suf.company_id = bc.id
        LEFT JOIN res_pic rp ON suf.head_pic_id = rp.id
        where suf.is_deleted=0
        <if test="platformId != null ">
            AND suf.platform_id = #{platformId,jdbcType=TINYINT}
        </if>
        <if test="phone != null and phone != '' ">
            AND suf.phone  like CONCAT(CONCAT('%',#{phone,jdbcType=VARCHAR}),'%')
        </if>
        <if test="wxNickName != null and wxNickName != '' ">
            AND suf.wx_nick_name like CONCAT(CONCAT('%',#{wxNickName,jdbcType=VARCHAR}),'%')
        </if>
        <if test="estimate != null ">
            AND suf.estimate = #{estimate,jdbcType=TINYINT}
        </if>
        <if test="dealStatus != null ">
            AND suf.deal_status = #{dealStatus,jdbcType=TINYINT}
        </if>
        <if test="companyName != null and companyName != '' ">
            AND bc.company_name like CONCAT(CONCAT('%',#{companyName,jdbcType=VARCHAR}),'%')
        </if>
        ORDER BY suf.gmt_create DESC
        <if test="start !=-1 and limit !=-1">
            LIMIT #{start}, #{limit}
        </if>
    </select>

    <select id="getFeedbackCompany" resultType="com.sandu.api.feedback.output.FeedbackCompany">
        select
        id            AS  companyId,
        company_name  AS  companyName
        from base_company where id IN (
              select company_id
              from  sys_user_feedback
              where is_deleted=0 and company_id is not null
              GROUP BY company_id
            )
    </select>


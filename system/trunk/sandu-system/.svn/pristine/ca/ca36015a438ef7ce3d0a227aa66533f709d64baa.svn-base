<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.service.imallOrderPayment.dao.ImallOrderPaymentLogDao">

    <!--保存付款单-->
    <insert id="insertOrderPaymentLog" parameterType="com.sandu.api.imallOrderPayment.model.ImallOrderPaymentLog" keyProperty="id" useGeneratedKeys="true">
        insert into imall_order_payment_log(payment_id,content,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted)
	    values(#{paymentId},#{content},#{sysCode},#{creator},CURRENT_TIMESTAMP,#{modifier},CURRENT_TIMESTAMP,#{isDeleted})
    </insert>

    <!--通过id获取付款单信息-->
    <select id="getImallOrderPaymentLog" resultType="com.sandu.api.imallOrderPayment.model.ImallOrderPaymentLog">
      select id,payment_id,content,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted
      from imall_order_payment_log
      where id=#{id}
    </select>

    <!--通过order_id获取付款单列表-->
    <select id="getImallOrderPaymentLogList" resultType="com.sandu.api.imallOrderPayment.model.ImallOrderPaymentLog">
      select id,payment_id,content,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted
      from imall_order_payment_log
      where payment_id=#{paymentId}
    </select>

</mapper>
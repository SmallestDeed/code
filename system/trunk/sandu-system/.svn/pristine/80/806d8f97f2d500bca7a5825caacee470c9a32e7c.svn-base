package com.sandu.api.pointmall.model.query;

import com.sandu.api.base.model.query.BaseQuery;

public class GiftQuery extends BaseQuery<GiftQuery> {
    private static final long serialVersionUID = 2018080223161809L;

    /**
     * 礼品名称
     */
    private String giftName;
    /**
     * 礼品Id
     */
    private long giftId;

    /**
     * 获取礼品名称
     * @return
     */
    public String getGiftName() {
        return giftName;
    }

    /**
     * 设置礼品名称
     * @param giftName
     */
    public void setGiftName(String giftName) {
        this.giftName = giftName;
    }

    /**
     * 获取礼品Id
     * @return
     */
    public long getGiftId() {
        return giftId;
    }

    /**
     * 设置礼品Id
     * @param giftId
     */
    public void setGiftId(long giftId) {
        this.giftId = giftId;
    }





}

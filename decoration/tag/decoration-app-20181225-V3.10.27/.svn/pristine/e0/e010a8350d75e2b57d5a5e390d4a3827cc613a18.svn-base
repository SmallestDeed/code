	<!-- 异步加载树形菜单 -->
	<resultMap id="productPropsTreeResultMap" type="com.nork.productprops.model.ProductProps">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result  column="pid" property="pid" jdbcType="INTEGER" />
		<result  column="name" property="name" jdbcType="VARCHAR" />
		<result  column="is_leaf" property="isLeaf" jdbcType="INTEGER" />
	</resultMap>
	<!-- 异步加载树形菜单 -->
	<select id="asyncLoadTree" resultMap="productPropsTreeResultMap"
			parameterType="java.lang.Integer">
		select pp.id,pp.pid,pp.name,pp.is_leaf from product_props pp
		where 1=1
		and pp.pid = #{pid,jdbcType=INTEGER}
	</select>
	
	<!-- getAttributeKeyAndValueByProductId -->
	<select id="getAttributeKeyAndValueByProductId" resultMap="AllResultMap">
		select pa.attribute_key as code,pp.prop_value from product_attribute pa
		LEFT JOIN product_props pp on pp.id = pa.attribute_value_id
		where product_id = #{productId,jdbcType=INTEGER}
	</select>
	
	<!-- getStructureProductPropByProductId -->
	<select id="getStructureProductPropCodeByProductId" resultType="java.lang.String">
		select pp.code from product_attribute pa
		left join product_props pp on pp.id = pa.attribute_value_id
		left join product_props ppp on ppp.id = pp.pid
		where pa.product_id = #{productId}
		and ppp.code = #{productAttrCode};
	</select>
	
	<!-- getStructureProductPropValueByProductId -->
	<select id="getStructureProductPropValueByProductId" resultType="java.lang.String">
		select pp.prop_value from product_attribute pa
		left join product_props pp on pp.id = pa.attribute_value_id
		left join product_props ppp on ppp.id = pp.pid
		where pa.product_id = #{productId}
		and ppp.code = #{productAttrCode};
	</select>
	

    <select id="queryBrandByBrandInfo" resultMap="AllResultMap">
        select
        <include refid="Filter_List_Field"/>
        from base_brand
        where 1 = 1
        <if test="companyId !=  null and companyId !=  ''   ">
            AND company_id = #{companyId}
        </if>
        <if test="brandCode !=  null and brandCode !=  ''   ">
            AND brand_code LIKE CONCAT(CONCAT('%',#{brandCode,jdbcType=VARCHAR}),'%')
        </if>
        <if test="brandName !=  null and brandName !=  ''   ">
            OR brand_name LIKE CONCAT(CONCAT('%',#{brandName,jdbcType=VARCHAR}),'%')
        </if>
        <if test="brandRefered !=  null and brandRefered !=  ''   ">
            AND brand_refered LIKE CONCAT(CONCAT('%',#{brandRefered,jdbcType=VARCHAR}),'%')
        </if>
        <if test="order != null and  order !=''  ">
            order by ${order}
            <if test="orderWay != null and  orderWay !='' ">
                ${orderWay}
            </if>
        </if>
        <if test="orders != null and  orders !=''  ">order by #{orders}</if>
        <if test="order == null and  orders == null ">order by id desc</if>
    </select>


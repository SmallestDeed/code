    <select id="queryProblemData" resultType="com.sandu.api.solution.model.DesignPlanBrand">
      SELECT
      plan_id,
      GROUP_CONCAT(company_id) as att1,
      GROUP_CONCAT(brand_id) as att2
      FROM design_plan_brand
      where plan_id in
      (
        SELECT id FROM `design_plan_recommended` a
        LEFT  JOIN
        (
          SELECT plan_id,COUNT(DISTINCT company_id) AS totals,company_id FROM design_plan_brand  WHERE company_id > 0 GROUP BY plan_id
        ) b ON a.id = b.plan_id
        WHERE b.totals >1 AND a.`is_deleted` = 0
      )
      group by plan_id
    </select>


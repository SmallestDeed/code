    <delete id="clearErrorPictures">
        DELETE FROM `res_render_pic` WHERE plan_recommended_id = #{targetSolutionId}
    </delete>
    <delete id="clearErrorVideos">
        DELETE FROM `res_render_video` WHERE business_id = #{targetSolutionId}
    </delete>

    <select id="queryPictureBySolutionId" resultType="com.sandu.api.solution.model.ResRenderPic">
        SELECT * FROM res_render_pic WHERE plan_recommended_id = #{solutionId} AND id IN
					(SELECT MAX(id)
					 FROM res_render_pic
					 WHERE plan_recommended_id = #{solutionId}
					 GROUP BY pic_type,rendering_type
					 ORDER BY id ASC)
    </select>

    <select id="queryVideoBySolutionId" resultType="com.sandu.api.solution.model.ResRenderVideo">
        SELECT * FROM res_render_video WHERE business_id=#{solutionId} ORDER BY id DESC LIMIT 0,1
    </select>

    <select id="getRoamByScreenShotId" resultType="com.sandu.api.solution.model.DesignRenderRoam">
        SELECT * FROM design_render_roam WHERE screen_shot_id=#{screenShotId} ORDER BY id LIMIT 0,1
    </select>
    <select id="getRoamConfigById" resultType="com.sandu.api.solution.model.RenderRoamConfig">
        SELECT * FROM res_design WHERE id=#{roamConfigId}
    </select>

    <select id="queryBugSolutionDueToCopy" resultType="java.util.Map">
        SELECT source_id AS originSolutionId, target_id AS targetSolutionId FROM design_plan_copy_log
        WHERE target_id IN (
            SELECT id FROM design_plan_recommended
            WHERE is_deleted=0 AND plan_source IN ('deliver','share') AND remark NOT IN (#{actionRemark},'copy')
            ORDER BY id DESC
            )
        ORDER BY id DESC LIMIT 0,#{limit}
    </select>

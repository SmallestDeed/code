#渲染任务服务器
taskServerHosts = 127.0.0.1

#renderServerHosts
#使用说明:renderServer(执行渲染服务器地址),storageType(文件存储方式),renderRoot(文件存储根目录),userName(ftp登陆用户),password(ftp登陆密码)
#例子:
#JSONArray格式，可配置多个：
renderServerHosts = [{"renderServer":"10.29.56.180","storageType":"local","renderRoot":"/sz/sdkj/sandu_MaxRender"}]
#renderServerHosts = [{"renderServer":"120.77.203.246","storageType":"ruiyun","renderRoot":"/sz/sdkj/sandu_MaxRender"}]

#渲染参数文件存储地址
params.upload.path = /system/sysTask/param/

#脚本模板存储地址
script.template.path = /MaxTemplet/

#脚本存储目录
script.path = /MaxScript/

#渲染参数文件存储地址
params.upload.path = /system/sysTask/param/

#脚本模板存储地址
script.template.path = /MaxTemplet/

#脚本存储目录
script.path = /MaxScript/

#3dmax文件在渲染机上的根目录
app.render.locRoot = D:\\MaxRender

#3dmax文件在渲染机上的目录
design.designPlan.maxrender.designPlan.upload.path=/MaxRender

#渲染机IPsocket
app.3dmax.socket.url = 10.29.56.180

#socket端口
app.3dmax.socket.port = 8000

#socket超时时间
app.3dmax.socket.timeout = 10000

#渲染成功后图片存放路径
app.3dmax.render.upload.path =/MaxRender/RenderPic/

#资源清理方式（可选删除：DELETE；备份：BACKUPS）
sysTask.cleanAction = DELETE

#每次清理的数据量
sysTask.cleanDataCount = 10

#脚本备份目录
script.backups.path = /Backups/

#普通渲染等待渲染最大时间（分钟）
app.render.normal.maxWaitMinutes = 20

#高清渲染等待渲染最大时间（分钟）
app.render.HD.maxWaitMinutes = 120

#暂停最大时长（天）
app.render.maxSuspendDays = 3

#暂停后，任务再次自动执行的等待时间
app.render.suspend.waitingForExecTime = 1800000

#创建渲染任务时验证所属的样板房和空间是否上架。（0:不验证；1：验证）
app.render.isShelvesValidate = 1

#渲染预解压目录
render.renderResourcesPath = /sz/sdkj/nork_resource/f_resource

#pano程序目录
app.pano.ProgramPath = /sz/sdkj/pano/krpano/

#pano程序的config文件存放目录
app.pano.configPath = templates/

#pano模板文件目录
app.pano.panoTemplatePath = /sz/sdkj/pano/scriptTemplate/

#pano程序结果输出目录
app.pano.outPutPath = /sz/sdkj/sandu_resource_src/vr720/

#pano访问url
app.pano.url = https://res3.sanduspace.com/vr720

#720渲染时长 (毫秒)
app.pano.render.time = 3600000

#渲染图二维码图片目录
system.QRCode.pic.upload.path = /e_userlogs/system/QRCode/pic/

#720背景音乐存放目录
app.pano.background.music.path = /sz/sdkj/sandu_resource_src/vr720/backgroundMusic

  <select id="getSkusBySpuId" resultMap="getSkusBySpuIdResultMap" parameterType="java.lang.Integer" >
    select
        t2.id
        ,t3.id product_id
        ,t2.inventory
        ,t2.price
        ,t2.specification_pic_id
        ,t1.pic_id
        ,t4.sale_price

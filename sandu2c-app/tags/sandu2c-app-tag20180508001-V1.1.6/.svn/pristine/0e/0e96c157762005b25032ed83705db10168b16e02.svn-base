spring.application.name=web-pc-consumer
debug=true

#Server
server.port=30013

### Dubbo
dubbo.application.name=sandu-yun-web-pc-consumer
dubbo.registry.address=127.0.0.1:2181
dubbo.consumer.check=false
dubbo.timeout=10000
dubbo.version=1.0.1
dubbo.retries=0

### Log
logging.config=classpath:log4j2.xml

app.sso.url =http\://sso.dev.sanduspace.com/

## 文件上传配置
file.storage.path:D:/temp/upload/
spring.http.multipart.maxFileSize=15MB
upload.path.format=/_platform_/_module_/_type_/[yyyy/MM/dd/HH]

### 平台编码
miniprogram.latform.code=miniProgram
mobile2c.platform.code=mobile2c
brand.website.platform.code=brand2c
  <select id="getGoodsInfoToOrder" resultMap="getGoodsInfoToOrderResultMap" parameterType="java.lang.Integer" >
    SELECT
        t2.product_id,
        t1.spu_name,
        t1.fix_transport_expense,
        t2.price,
        group_concat( t4.attribute_value_name ),
        t3.pic_path
    FROM
        base_goods_spu t1,
        base_goods_sku t2,
        res_pic t3,
        product_attribute t4
    WHERE
        t1.id = 2
        AND t2.spu_id = t1.id
        AND t3.id = t2.main_pic_id
        AND t4.product_id = t2.product_id
    GROUP BY
        t2.product_id
  </select>

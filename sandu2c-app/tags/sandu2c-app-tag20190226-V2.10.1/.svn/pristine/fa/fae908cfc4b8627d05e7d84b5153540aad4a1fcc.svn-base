<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- 户型服务 -->
    <dubbo:reference id="spaceCommonService" interface="com.sandu.home.service.SpaceCommonService" version="${dubbo.version}" retries="${dubbo.retries}"/>
    <!-- 省市-->
    <dubbo:reference interface="com.sandu.base.service.BaseAreaService"
                     id="baseAreaService" version="${dubbo.version}" retries="${dubbo.retries}"/>
</beans>
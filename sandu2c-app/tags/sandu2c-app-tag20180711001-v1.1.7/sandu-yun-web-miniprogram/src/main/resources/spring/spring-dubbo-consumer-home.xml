<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <!-- 生成远程服务代理，可以像使用本地bean一样使用demoService -->
    <dubbo:reference id="spaceCommonService"
                     interface="com.sandu.home.service.SpaceCommonService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <dubbo:reference id="baseHouseService"
                     interface="com.sandu.home.service.BaseHouseService" retries="${dubbo.retries}" version="${dubbo.version}"/>
    <dubbo:reference id="houseSpaceNewService"
                     interface="com.sandu.home.service.HouseSpaceNewService" retries="${dubbo.retries}" version="${dubbo.version}"/>
    <!-- 通用空间缓存服务接口 -->
    <dubbo:reference id="spaceCommonCacheService"
                     interface="com.sandu.home.service.SpaceCommonCacheService" retries="${dubbo.retries}" version="${dubbo.version}"/>
    <!-- 户型申请表服务接口 -->
    <dubbo:reference id="baseHouseApplyService"
                     interface="com.sandu.home.service.BaseHouseApplyService" retries="${dubbo.retries}" version="${dubbo.version}"/>
</beans>
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 声明需要暴露的服务接口 -->
    <dubbo:service interface="com.sandu.home.service.SpaceCommonService" 
                   ref="spaceCommonService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>
    <dubbo:service interface="com.sandu.home.service.BaseHouseService"
                   ref="baseHouseService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>
    <dubbo:service interface="com.sandu.home.service.HouseSpaceNewService"
                   ref="houseSpaceNewService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <!-- 通用空间缓存服务接口 -->
    <dubbo:service interface="com.sandu.home.service.SpaceCommonCacheService"
                   ref="spaceCommonCacheService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

    <!-- 户型申请表服务接口 -->
    <dubbo:service interface="com.sandu.home.service.BaseHouseApplyService"
                   ref="baseHouseApplyService" protocol="dubbo" timeout="${dubbo.timeout}"
                   version="${dubbo.version}"/>

</beans>
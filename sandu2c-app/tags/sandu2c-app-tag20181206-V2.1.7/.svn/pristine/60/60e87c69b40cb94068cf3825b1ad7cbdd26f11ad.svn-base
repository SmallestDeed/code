<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- Redis缓存服务-->
    <dubbo:service interface="com.sandu.cache.service.RedisService" ref="redisService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!-- Redis登录用户缓存服务-->
    <dubbo:service interface="com.sandu.cache.service.RedisLoginService" ref="redisLoginService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!-- 设计规则缓存服务-->
    <dubbo:service interface="com.sandu.cache.service.DesignRuleCacheService" ref="designRuleCacheService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>
</beans>
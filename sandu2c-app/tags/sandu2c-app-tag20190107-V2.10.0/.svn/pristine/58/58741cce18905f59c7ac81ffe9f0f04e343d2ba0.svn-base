package com.sandu.analyze.exception;

/**
 * 搜索引擎服务异常
 *
 * @date 20171211
 * @auth pengxuangang
 */
public class ElasticSearchException extends Exception {

    private static final String CLASS_PREFIX_LOG = "[搜索引擎服务异常]:";

    public ElasticSearchException() {
    }

    public ElasticSearchException(String message) {
        super(CLASS_PREFIX_LOG + message);
    }

    public ElasticSearchException(String message, Throwable cause) {
        super(CLASS_PREFIX_LOG + message, cause);
    }

    public ElasticSearchException(Throwable cause) {
        super(cause);
    }

    public ElasticSearchException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(CLASS_PREFIX_LOG + message, cause, enableSuppression, writableStackTrace);
    }
}

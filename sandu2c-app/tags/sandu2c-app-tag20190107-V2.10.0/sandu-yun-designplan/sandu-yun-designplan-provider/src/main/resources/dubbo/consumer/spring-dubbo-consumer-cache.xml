<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- Redis缓存服务-->
    <dubbo:reference id="redisService" interface="com.sandu.cache.service.RedisService" version="${dubbo.version}" retries="${dubbo.retries}"/>

    <!--设计规则缓存服务-->
    <dubbo:reference id="designRuleCacheService" interface="com.sandu.cache.service.DesignRuleCacheService" version="${dubbo.version}" retries="${dubbo.retries}"/>

</beans>
package com.sandu.product.model;

import com.sandu.common.model.Mapper;

import java.io.Serializable;

/**
 * @version V1.0
 * @Title: BaseProduct.java
 * @Package com.sandu.product.model
 * @Description:产品模块-产品库
 * @createAuthor pandajun
 * @CreateDate 2015-06-15 17:01:37
 */
public class ProductVO extends Mapper implements Serializable {
    private static final long serialVersionUID = 1L;
    private Integer id;
    private String planProductId;
    private Integer designPlanId;
    private Integer spaceCommonId;
    private String mediaType;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getPlanProductId() {
        return planProductId;
    }

    public void setPlanProductId(String planProductId) {
        this.planProductId = planProductId;
    }

    public Integer getDesignPlanId() {
        return designPlanId;
    }

    public void setDesignPlanId(Integer designPlanId) {
        this.designPlanId = designPlanId;
    }

    public Integer getSpaceCommonId() {
        return spaceCommonId;
    }

    public void setSpaceCommonId(Integer spaceCommonId) {
        this.spaceCommonId = spaceCommonId;
    }

    public String getMediaType() {
        return mediaType;
    }

    public void setMediaType(String mediaType) {
        this.mediaType = mediaType;
    }

}

spring.application.name=yun-cache-provider
debug=true

### Dubbo
dubbo.registry.address=127.0.0.1:2181
dubbo.protocol.port.cache=21010
dubbo.timeout=10000
dubbo.version=1.0.1
dubbo.retries=0

#\u6700\u5927\u5206\u914D\u7684\u5BF9\u8C61\u6570
redis.pool.maxActive=10
#\u6700\u5927\u80FD\u591F\u4FDD\u6301idel\u72B6\u6001\u7684\u5BF9\u8C61\u6570
redis.pool.maxIdle=5
redis.pool.minIdle=10
redis.pool.maxWaitMillis=20000
#\u5F53\u6C60\u5185\u6CA1\u6709\u8FD4\u56DE\u5BF9\u8C61\u65F6\uFF0C\u6700\u5927\u7B49\u5F85\u65F6\u95F4
redis.pool.maxWait=300
#\u683C\u5F0F\uFF1Aredis://:[\u5BC6\u7801]@[\u670D\u52A1\u5668\u5730\u5740]:[\u7AEF\u53E3]/[db index]
#redis.pool.uri=redis://:123456@127.0.0.1:6379/0
redis.pool.uri=redis://:sanduspace@redis.dev.sanduspace.com:33436/0

### Log
logging.config=classpath:log4j2.xml

#redis\u8FDE\u63A5\u5730\u5740
redis.pool.login.host=127.0.0.1
#redis\u7AEF\u53E3\u53F7
redis.pool.login.port=6379
redis.pool.login.password=123456
#\u5F53\u6C60\u5185\u6CA1\u6709\u8FD4\u56DE\u5BF9\u8C61\u65F6\uFF0C\u6700\u5927\u7B49\u5F85\u65F6\u95F4
redis.pool.login.maxIdle=300
#\u6700\u5927\u80FD\u591F\u4FDD\u6301idel\u72B6\u6001\u7684\u5BF9\u8C61\u6570
redis.pool.login.maxTotal=6000
redis.pool.login.minIdle=100
redis.pool.login.maxWaitMillis=1000

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 推荐方案服务 -->

    <dubbo:service interface="com.sandu.designplan.service.DesignPlanRecommendedService" ref="designPlanRecommendedService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>



    <!--设计模块渲染场景服务-->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanRenderSceneService" ref="designPlanRenderSceneService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!--设计模块服务-->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanService" ref="designPlanService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!--设计方案产品服务-->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanProductService" ref="designPlanProductService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>


    <!-- 方案推荐产品服务 -->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanRecommendedProductService" ref="designPlanRecommendedProductService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!-- 方案推荐收藏服务 -->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanRecommendFavoriteService" ref="designPlanRecommendFavoriteService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

    <!-- 方案点赞服务 -->
    <dubbo:service interface="com.sandu.designplan.service.DesignPlanLikeService" ref="designPlanLikeService" protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}"/>

</beans>
  <select id="selectDecorateOrderById" resultType="com.sandu.decorateorderscore.model.DecorateOrder">
	SELECT 
	  a.id,
	  a.company_id AS companyId,
	  b.company_name AS companyName 
	FROM
	  decorate_order a 
	  LEFT JOIN base_company b 
	    ON a.company_id = b.id 
  	  where a.id = #{id}
  </select>
  
  
  <select id="selectDecorateOrderCount" resultType="java.lang.Integer">
  	  select count(1) from (
  	  	SELECT 
  	  		distinct 
		    c.user_name AS userName,
		    c.mobile,
		    c.house_acreage AS houseAcreage,
		    b.company_name AS companyName,
		    IFNULL(a.sign_time,a.gmt_create) AS gmtCreate,
		    CASE WHEN
		      IFNULL(a.order_status,-1) IN(5,6,7) 
		      THEN 1 
		      ELSE 2 
		    END AS orderNum,
		    a.`order_status`,
		    c.province_code,
		    c.city_code 
		  FROM
		    proprietor_info c
		    LEFT JOIN decorate_order a 
		      ON a.`proprietor_info_id` = c.id 
		    LEFT JOIN base_company b 
		      ON a.`company_id` = b.id 
		  WHERE c.business_type = 0 
		    AND c.customer_type IN (1, 2)
		    AND c.is_deleted= 0 
  	  ) as t
  </select>
  
  <select id="selectDecorateOrderList" resultType="com.sandu.decorateorderscore.model.DecorateOrderVO"
   parameterType="com.sandu.decorateorderscore.model.DecorateOrderQuery">
  		SELECT 
  		  distinct 
		  t.userName,
		  t.mobile,
		  t.houseAcreage,
		  t.companyName,
		  CONCAT(d.area_name, e.area_name) AS cityName,
		  t.orderNum as statusStr
		FROM
		  (SELECT 
		    c.user_name AS userName,
		    c.mobile,
		    c.house_acreage AS houseAcreage,
		    b.company_name AS companyName,
		    IFNULL(a.sign_time,a.gmt_create) AS gmtCreate,
		    CASE WHEN
		      IFNULL(a.order_status,-1) IN(5,6,7) 
		      THEN 1 
		      ELSE 2 
		    END AS orderNum,
		    a.`order_status`,
		    c.province_code,
		    c.city_code 
		  FROM
		    proprietor_info c
		    LEFT JOIN decorate_order a 
		      ON a.`proprietor_info_id` = c.id 
		    LEFT JOIN base_company b 
		      ON a.`company_id` = b.id 
		  WHERE c.business_type = 0 
		    AND c.customer_type IN (1, 2)
		    AND c.is_deleted= 0 ) t 
		  LEFT JOIN base_area d 
		    ON t.province_code = d.area_code 
		  LEFT JOIN base_area e 
		    ON t.city_code = e.area_code 
		   <if test="orderBy!=null and orderBy !=''">
		    	${orderBy}
		   </if>
		   <if test="pageNo != null and pageSize != null">
		        limit #{pageNo},#{pageSize}
		   </if> 
  </select>
  

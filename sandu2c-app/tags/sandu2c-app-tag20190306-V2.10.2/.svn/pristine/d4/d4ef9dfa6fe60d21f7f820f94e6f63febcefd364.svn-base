<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sandu.base.dao.ResBannerPicMapper">
    <!-- **常量定义** -->
    <sql id="All_Column_List">
        id,pic_code,pic_name,pic_file_name,pic_type,pic_size,pic_weight,pic_high,pic_suffix,pic_level,
        pic_format,pic_path,pic_desc,sys_code,creator,gmt_create,modifier,gmt_modified,is_deleted,remark
    </sql>

    <select id="selectPicById" resultType="com.sandu.banner.model.ResBannerPic"  parameterType="java.lang.Integer">
        SELECT
            <include refid="All_Column_List" />
        FROM res_banner_pic
        WHERE 1=1 and id = #{picId}
    </select>
</mapper>
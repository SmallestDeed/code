spring.application.name=web-pc-consumer
debug=true

#Server
server.port=30010

### Dubbo
dubbo.application.name=sandu-yun-web-pc-consumer
dubbo.registry.address=zookeeper01test.sanduspace.com:2181
dubbo.consumer.check=false
dubbo.timeout=10000
dubbo.version=1.0.1
dubbo.retries=0


### Log
logging.config=classpath:log4j2.xml

app.sso.url =https\://sso.test.sanduspace.com/


### 平台编码
miniprogram.platform.code=miniProgram
mobile2c.platform.code=mobile2c
brand.website.platform.code=brand2c

## 存入资源路径
file.storage.path:/data001/resource/
spring.http.multipart.maxFileSize=15MB
upload.path.format=/_platform_/_module_/_type_/[yyyy/MM/dd/HH]
server.url=http://show.test.sanduspace.com


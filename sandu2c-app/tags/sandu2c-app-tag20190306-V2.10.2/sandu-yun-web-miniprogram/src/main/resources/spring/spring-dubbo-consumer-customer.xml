<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

	<dubbo:reference interface="com.sandu.customer.service.CustomerBaseInfoService"
		id="customerBaseInfoService" protocol="dubbo" timeout="${dubbo.timeout}"
		version="${dubbo.version}" />

	<dubbo:reference
		interface="com.sandu.customer.service.CustomerOperateLogService" id="customerOperateLogService"
		protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}" />

	<dubbo:reference
		interface="com.sandu.customer.service.CustomerScoreDetailService" id="customerScoreDetailService"
		protocol="dubbo" timeout="${dubbo.timeout}" version="${dubbo.version}" />
</beans>
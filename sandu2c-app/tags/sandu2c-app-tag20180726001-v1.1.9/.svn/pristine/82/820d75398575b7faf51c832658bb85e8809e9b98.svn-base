    <!-- getProductValueByBaimoValue -->
    <!-- eg:baimo_mengk获得mengk的value -->
    <select id="getProductValueByBaimoValue" resultType="java.lang.Integer">
        select
        sdneed.value from sys_dictionary sdbig
        left join sys_dictionary sdsmall
        on sdsmall.type = sdbig.valuekey and sdsmall.value =
        #{smallTypeValue,jdbcType=INTEGER}
        left join sys_dictionary sdneed on
        sdneed.type = sdbig.valuekey and sdneed.valuekey =
        replace(sdsmall.valuekey,"basic_","")
        where sdbig.type = "productType"
        and sdbig.value = #{typeValue,jdbcType=INTEGER}
    </select>

    <!-- findOneByTypeAndValueAndValue -->
    <select id="findOneByTypeAndValueAndValue" resultMap="AllResultMap">
        select
        sdsmall.valuekey,sdsmall.att1,sdsmall.type from sys_dictionary sdbig
        left join sys_dictionary sdsmall on sdsmall.type = sdbig.valuekey and
        sdsmall.value = #{smallTypeValue,jdbcType=INTEGER}
        where sdbig.type =
        #{type,jdbcType=VARCHAR} and sdbig.value =
        #{typeValue,jdbcType=INTEGER}
    </select>


    <!-- 通过ValueKeys 和 type 查列表 -->
    <select id="getListByValueKeys" resultMap="AllResultMap">
        select
        <include refid="All_Column_List"/>
        from sys_dictionary
        where 1 = 1
        and type = #{type,jdbcType=VARCHAR}
        and
        valueKey in
        <foreach collection="ValueKeys" index="index" item="item"
                 open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
    <!-- 通过空间类型查询面积范围列表 -->
    <select id="selectAreaList" resultMap="AllResultMap"
            parameterType="java.lang.String">
        select
        <include refid="All_Column_List"/>
        from sys_dictionary
        <where>
            is_deleted = 0
            AND type IN (
            SELECT
            valueKey
            FROM
            sys_dictionary
            WHERE
            type = #{type,jdbcType=VARCHAR}
            )
        </where>
    </select>

    <!-- 通过空间类型查询面积范围总记录 -->
    <select id="selectAreaCount" resultType="int" parameterType="java.lang.String">
        select count(1)
        <include refid="All_Column_List"/>
        from sys_dictionary
        <where>
            is_deleted = 0
            AND type IN (
            SELECT
            valueKey
            FROM
            sys_dictionary
            WHERE
            type = #{type,jdbcType=VARCHAR}
            )
        </where>
    </select>

    <resultMap id="sysDictionaryBoMap" type="com.sandu.system.model.SysDictionaryBo">
        <result  column="big_type_value" property="bigTypeValue" jdbcType="INTEGER" />
        <result  column="small_type_value" property="smallTypeValue" jdbcType="INTEGER" />
    </resultMap>

    <select id="getTypeValueByValueKeys" resultMap="sysDictionaryBoMap" >
        select sd2.value AS big_type_value ,sd1.value AS small_type_value
        FROM sys_dictionary sd1 ,sys_dictionary sd2
        where sd2.`type` = 'productType'
        AND sd2.`valuekey` = sd1.`type`
        AND sd1.is_deleted = 0
        AND	sd1.valueKey in
        <foreach collection="valueKeys" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
</mapper>

    <select id="selectSmallTypeObj" resultType="com.sandu.system.model.SysDictionary"
            parameterType="java.util.Map">
        SELECT
        s2.`id`,s2.name,s2.type,s2.`value`,s2.`valuekey`,s2.`att1`,s2.`att2`,s2.`att3`,s2.`att4`,s2.`att5`,s2.`att6`
        FROM sys_dictionary s1, sys_dictionary s2 WHERE s1.type =
        'productType'
        AND s1.value = #{typeValue,jdbcType=INTEGER} AND s2.type
        = s1.valuekey
        AND s2.value = #{smallTypeValue,jdbcType=INTEGER}
        AND
        s1.is_deleted = 0 AND s2.is_deleted = 0
    </select>


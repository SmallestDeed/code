    <select id="getSpuIdList" resultType="java.lang.Integer"
            parameterType="com.sandu.product.model.UserProductsConllect">
        select
            t2.id
        from
            user_product_collect t1
            left join base_goods_spu t2 on t2.id = t1.spu_id
            left join base_product t9 on t9.goods_spu_id = t2.id
            left join product_category_rel t6 on t6.product_id = t9.id
            left join pro_category t7 on t7.id = t6.category_id
            left join platform2c_product_rel t8 on t8.product_id = t9.id
        where
            t2.is_deleted = 0
            and t9.is_deleted = 0
            and t2.is_putaway = 1
            and t8.putaway_state = 1
            <if test="userId != null and userId != '' ">
                AND t1.user_id = #{userId,jdbcType=INTEGER}
            </if>
            <if test="collectCatalogId != null and collectCatalogId != 0 ">
                and t1.collect_catalog_id = #{collectCatalogId,jdbcType=INTEGER}
            </if>
            <if test="productCategoryCode != null and productCategoryCode != '' ">
                and t7.third_stage_code = #{productCategoryCode,jdbcType=VARCHAR}
            </if>
        group by
            t2.id
        ORDER BY t1.gmt_create
            <if test="isSort== 0 or isSort== null">DESC</if>
            <if test="isSort== 1">ASC</if>
            <if test="start !=-1 and limit !=-1">LIMIT #{start}, #{limit}</if>
    </select>

    <select id="getCollectCount" resultType="java.lang.Integer"
            parameterType="com.sandu.product.model.UserProductsConllect">
        select count(1) from (
            select
                1
            from
                user_product_collect t1
                left join base_goods_spu t2 on t2.id = t1.spu_id
                left join base_product t9 on t9.goods_spu_id = t2.id
                left join product_category_rel t6 on t6.product_id = t9.id
                left join pro_category t7 on t7.id = t6.category_id
                left join platform2c_product_rel t8 on t8.product_id = t9.id
            where
                t2.is_deleted = 0
                and t9.is_deleted = 0
                and t2.is_putaway = 1
                and t8.putaway_state = 1
                <if test="userId != null and userId != '' ">

                </if>
                <if test="collectCatalogId != null and collectCatalogId != 0 ">
                    and t1.collect_catalog_id = #{collectCatalogId,jdbcType=INTEGER}
                </if>
                <if test="productCategoryCode != null and productCategoryCode != '' ">
                    and t7.third_stage_code = #{productCategoryCode,jdbcType=VARCHAR}
                </if>
            group by
                t2.id
        )t
    </select>


    <select id="getAllListAll" resultMap="userProductsConllectList"
            parameterType="com.sandu.product.model.UserProductsConllect">
        SELECT
        DISTINCT c.id AS id,
        c.product_id AS product_id,
        c.user_id AS user_id, u.user_name AS user_name,
        p.product_code AS product_code,
        p.product_name AS product_name,
        ppr.sale_price AS sale_price,
        p.product_desc AS product_desc ,
        b.brand_name AS brand_name,
        b.company_id AS companyId,
        p.brand_id AS brand_id,
        p.product_type_value AS product_type_value,
        s.valuekey AS product_type,
        s.name AS product_type_name,
        r.pic_path AS product_path
        FROM user_product_collect c
        LEFT JOIN sys_user u ON c.user_id = u.id
        LEFT JOIN base_product p ON c.product_id =p.id
        LEFT JOIN base_brand b ON p.brand_id = b.id
        LEFT JOIN res_pic r ON p.pic_id = r.id
        LEFT JOIN sys_dictionary s ON s.value = CAST(p.product_type_value AS signed)
        LEFT JOIN collect_catalog cc ON cc.id = c.collect_catalog_id
        left join product_category_rel pcr on pcr.product_id = p.id
        left join pro_category pc on pc.id = pcr.category_id
        LEFT JOIN platform2c_product_rel ppr ON ppr.product_id = c.product_id AND ppr.is_deleted = 0
        WHERE s.type='productType'
        <!-- 以前已经上架显示，现在是已发布的显示   -->
        and p.putaway_state = 3
        AND ppr.platform_id =  14
        AND ppr.putaway_state = 1
        AND ppr.allot_state = 1
        <if test="userId != null and userId != '' ">
            AND u.id = #{userId,jdbcType=INTEGER}
        </if>
        <if test="collectCatalogId != null and collectCatalogId != 0 ">
            and c.collect_catalog_id = #{collectCatalogId,jdbcType=INTEGER}
        </if>
        <if test="productCategoryCode != null and productCategoryCode != '' ">
            and pc.third_stage_code = #{productCategoryCode,jdbcType=VARCHAR}
        </if>
        ORDER BY c.gmt_create 
        <if test="isSort== 0 or isSort== null">DESC</if>
        <if test="isSort== 1">ASC</if>
        <if test="start !=-1 and limit !=-1">LIMIT #{start}, #{limit}</if>
	
    </select>


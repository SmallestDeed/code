<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sandu.order.dao.CouponOperateLogMapper">

    <insert id="insert" parameterType="com.sandu.order.ActivityUserCouponOperateLog" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO (user_id,company_id,shop_id,coupon_id,coupon_no,
                    operate_type,operate_time,order_id,order_no,creator,gmt_create)
        VALUES (#{userId},#{companyId},#{shopId},#{couponId},#{couponNo},
                #{operateType},#{operateTime},#{orderId},#{orderNo},#{creator},#{gmtCreate}
        )
    </insert>


</mapper>
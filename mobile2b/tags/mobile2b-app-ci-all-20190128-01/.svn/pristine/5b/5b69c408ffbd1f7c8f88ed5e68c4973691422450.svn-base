}
#content .NewDesign .top {
	float: left;
	margin-right: 0;
	height: 785px;
	background: #fff;
	margin:90px 0;
}
#content .disiger .list {
	width: 100%;
	margin: 0 auto;
}
#content .disiger .list li {
	float: left;
}
#content .lay_top{
	margin-bottom:15px;
	padding: 20px 0 0 370px;
}
#content .lay_top li a {
	display: block;
}
.vline{
	width: 100%;
	height: 81px;
	background: url(../image/vLine.png) no-repeat;
	margin-top: -65px;
}
#footer p {
	display: block;
	width: 420px;	
	margin:0 auto;

    <select id="getMyDrawHouseList" resultMap="getMyDrawHouseListByUserIdResultMap"
            parameterType="com.nork.home.model.search.MyHouseSearch">
        SELECT
        t1.id,
        t1.house_common_code,
        t1.total_area,
        t5.pic_path,
        t3.living_name,
        group_concat(t2.space_function_id) house_spe
        FROM
        base_house t1
        LEFT JOIN house_space_new t4 ON t4.house_id = t1.id
        LEFT JOIN space_common t2 ON t2.id = t4.space_id AND t2.is_deleted = 0
        LEFT JOIN base_living t3 ON t3.id = t1.living_id
        LEFT JOIN res_house_pic t5 ON t5.id = (
        CASE
        WHEN t1.snap_pic_id IS NULL
        OR t1.snap_pic_id = '' THEN
        t1.pic_res1_id
        ELSE
        t1.snap_pic_id
        END
        )
        WHERE
        t1.is_deleted = 0
        AND t1.user_id = #{userId}
        AND t1.is_public = 1
        group by t1.id
        ORDER BY t1.gmt_modified DESC
        <if test="start != null and limit != null">
            LIMIT #{start},#{limit}
        </if>
    </select>

    <select id="getMyDrawHouseCount" resultType="java.lang.Integer"
            parameterType="com.nork.home.model.search.MyHouseSearch">
    SELECT
        count(1)
    FROM
        (SELECT
            1
        FROM
            draw_base_house t1
            left join draw_space_common t2 on t2.draw_base_house_id = t1.id and t2.is_deleted = 0
            left join base_living t3 on t3.id = t1.living_id
            left join house_space_new t4 on t4.house_id = t1.base_house_id
        WHERE
            t1.is_deleted = 0
            AND t1.user_id = #{userId}
            AND t1.house_status = 6
            AND t1.is_public = 1
        GROUP BY
            t1.base_house_id)t
  </select>


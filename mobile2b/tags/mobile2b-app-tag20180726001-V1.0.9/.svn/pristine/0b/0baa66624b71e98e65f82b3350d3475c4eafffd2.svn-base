package com.nork.common.async;

import javax.servlet.http.HttpServletRequest;

/***
 * 更新设计方案任务参数
 * @author qiu.jun
 * @date 2016.06.16
 *
 */
public class UnitUpdateParameter {
	private Integer designPlanId;
	private String context;
	private Integer planProductId;
	private Integer productId;
	private String materialPicId;
	private String msgId;
	private HttpServletRequest request;
	private Integer userId;
	private String splitTexturesChoose;
	public UnitUpdateParameter(Integer designPlanId, String context, Integer planProductId, Integer productId,
			String materialPicId, String msgId, HttpServletRequest request, String splitTexturesChoose) {
		this.designPlanId = designPlanId;
		this.context = context;
		this.planProductId = planProductId;
		this.productId = productId;
		this.materialPicId = materialPicId;
		this.msgId = msgId;
		this.request = request;
		this.splitTexturesChoose=splitTexturesChoose;
	}

	public String getSplitTexturesChoose() {
		return splitTexturesChoose;
	}

	public void setSplitTexturesChoose(String splitTexturesChoose) {
		this.splitTexturesChoose = splitTexturesChoose;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Integer getDesignPlanId() {
		return designPlanId;
	}

	public void setDesignPlanId(Integer designPlanId) {
		this.designPlanId = designPlanId;
	}

	public String getContext() {
		return context;
	}

	public void setContext(String context) {
		this.context = context;
	}

	public Integer getPlanProductId() {
		return planProductId;
	}

	public void setPlanProductId(Integer planProductId) {
		this.planProductId = planProductId;
	}

	public Integer getProductId() {
		return productId;
	}

	public void setProductId(Integer productId) {
		this.productId = productId;
	}

	public String getMaterialPicId() {
		return materialPicId;
	}

	public void setMaterialPicId(String materialPicId) {
		this.materialPicId = materialPicId;
	}

	public String getMsgId() {
		return msgId;
	}

	public void setMsgId(String msgId) {
		this.msgId = msgId;
	}

	public HttpServletRequest getRequest() {
		return request;
	}

	public void setRequest(HttpServletRequest request) {
		this.request = request;
	}

}

    <dubbo:reference interface="com.sandu.pay.order.service.PayAccountService"
                     id="payAccountService" version="1.0.1" retries="0" timeout="60000"/>

    <!--Mini program pay-->
    <dubbo:reference interface="com.sandu.pay.minipro.service.WXMiniProPayService"
                   id="wxMiniProPayService" protocol="dubbo" timeout="10000" version="1.0.1"/>

    <dubbo:reference interface="com.sandu.pay2.service.Pay2Service"
                     id="miniPay"  protocol="dubbo" timeout="10000" version="1.0.1" group="miniPay"/>

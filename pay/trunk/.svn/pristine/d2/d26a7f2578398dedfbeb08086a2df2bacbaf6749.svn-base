<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.gateway.pay.trade.dao.PayTradeTransfersRecordLogMapper">
	<insert id="insertSelective" parameterType="com.sandu.gateway.pay.trade.model.PayTradeTransfersRecordLog" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO pay_trade_transfers_record_log 
			(
				trade_transfers_id, 
				external_request, 
				external_response, 
				sys_code, 
				operator, 
				creator, 
				gmt_create, 
				modifier, 
				gmt_modified, 
				is_deleted, 
				remark
			) 
		VALUES 
			(
				#{tradeTransfersId},
				#{externalRequest},
				#{externalResponse},
				#{sysCode},
				#{operator},
				#{creator},
				#{gmtCreate},
				#{modifier},
				#{gmtModified},
				#{isDeleted},
				#{remark}
			);
	</insert>   
    
</mapper>

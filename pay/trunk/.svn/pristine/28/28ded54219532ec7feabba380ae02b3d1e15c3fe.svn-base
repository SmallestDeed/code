public class RechargeIntegralVo {
    private static final long serialVersionUID = -452216881102141767L;

    //充值Id
    private Integer rechargeId;

    //充值金额
    private Integer rechargeMoney;

    //充值度币名称
    private String rechargeIntegral;

    //赠送度币
    private String presentIntegral;
    
    //显示折扣
    private String discountName;

    public Integer getRechargeId() {
        return rechargeId;
    }

    public void setRechargeId(Integer rechargeId) {
        this.rechargeId = rechargeId;
    }

    public Integer getRechargeMoney() {
        return rechargeMoney;
    }

    public void setRechargeMoney(Integer rechargeMoney) {
        this.rechargeMoney = rechargeMoney;
    }

    public String getRechargeIntegral() {
        return rechargeIntegral;
    }

    public void setRechargeIntegral(String rechargeIntegral) {
        this.rechargeIntegral = rechargeIntegral;
    }

    public String getPresentIntegral() {
        return presentIntegral;
    }

    public void setPresentIntegral(String presentIntegral) {
        this.presentIntegral = presentIntegral;
    }

	public String getDiscountName() {
		return discountName;
	}

	public void setDiscountName(String discountName) {
		this.discountName = discountName;
	}
    
}

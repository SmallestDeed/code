<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 声明需要暴露的服务接口 -->
    <!--阿里扫码支付-->
    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="aliScanCodePayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="aliScanCodePay"/>
                   
    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="wxScanCodePayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="wxScanCodePay"/>

    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="miniPayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="miniProPay"/>

    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="wxH5PayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="wxH5Pay"/>

    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="wxAppPayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="wxAppPay"/>

    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="aliH5PayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="aliH5Pay"/>

    <dubbo:service interface="com.sandu.gateway.pay.forward.service.PayService"
                   ref="aliAppPayService" protocol="dubbo" timeout="30000"  version="2.0.0" group="aliAppPay"/>

    <dubbo:service interface="com.sandu.gateway.pay.callback.service.PayCallbackService"
                   ref="wxPayCallbackService" protocol="dubbo" timeout="30000"  version="2.0.0" group="wxPayCallback"/>
    <dubbo:service interface="com.sandu.gateway.pay.callback.service.RefundCallbackService"
                   ref="wxRefundCallbackService" protocol="dubbo" timeout="30000"  version="2.0.0" group="wxRefundCallback"/>
                   
    <dubbo:service interface="com.sandu.gateway.pay.callback.service.PayCallbackService"
                   ref="aliPayCallbackService" protocol="dubbo" timeout="30000"  version="2.0.0" group="aliPayCallback"/>

</beans>
 

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sandu.gateway.pay.config.dao.BaseCompanyMiniProgramConfigMapper">

    <!--通过AppId查询企业微信信息 -->
    <select id="selectByAppId" parameterType="java.lang.String" resultType="com.sandu.gateway.pay.config.model.BaseCompanyMiniProgramConfig">
        SELECT * FROM
        base_company_mini_program_config
        WHERE is_deleted = 0
        AND  app_id = #{appId}
    </select>

</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sandu.gateway.pay.trade.dao.PayTradeTransfersRecordMapper">
	<insert id="insertSelective" parameterType="com.sandu.gateway.pay.trade.model.PayTradeTransfersRecord" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO pay_trade_transfers_record 
			(
				pay_trade_no,
				intenal_trade_no,
				extenal_trade_no,
				pay_method,
				amount,
				trade_date,
				trade_desc,
				open_id,
				sign, 
				status, 
				payment_time,
				ip,
				platform_code,
				source, 
				operator, 
				sys_code, 
				creator, 
				gmt_create, 
				modifier, 
				gmt_modified, 
				is_deleted, 
				remark
			) 
		VALUES 
			(
				#{payTradeNo},
				#{intenalTradeNo},
				#{extenalTradeNo},
				#{payMethod},
				#{amount},
				#{tradeDate},
				#{tradeDesc},
				#{openId},
				#{sign},
				#{status},
				#{paymentTime},
				#{ip},
				#{platformCode},
				#{source},
				#{operator},
				#{sysCode},
				#{creator},
				#{gmtCreate},
				#{modifier},
				#{gmtModified},
				#{isDeleted},
				#{remark}
			);
	</insert>
	
	<update id="updateById" parameterType="com.sandu.gateway.pay.trade.model.PayTradeTransfersRecord">
		update pay_trade_Transfers_record
		<set>
			<if test="payTradeNo!= null">pay_trade_no=#{payTradeNo},</if>
			<if test="intenalTradeNo!= null">intenal_trade_no=#{intenalTradeNo},</if>
			<if test="extenalTradeNo!= null">extenal_trade_no=#{extenalTradeNo},</if>
			<if test="payMethod!= null">pay_method=#{payMethod},</if>
			<if test="amount!= null">amount=#{amount},</if> 
			<if test="tradeDate!= null">trade_date=#{tradeDate},</if>
			<if test="tradeDesc!= null">trade_desc=#{tradeDesc},</if>
			<if test="openId!= null">open_id=#{openId},</if>
			<if test="sign!= null">sign=#{sign},</if>
			<if test="status!= null">status=#{status},</if>
			<if test="paymentTime!= null">payment_time=#{paymentTime},</if>
			<if test="ip!= null">ip=#{ip},</if> 
			<if test="platformCode!= null">platform_code=#{platformCode},</if> 
			<if test="source!= null">source=#{source},</if>
			<if test="operator!= null">operator=#{operator},</if>
			<if test="sysCode!= null">sys_code=#{sysCode},</if> 
			<if test="creator!= null">creator=#{creator},</if> 
			<if test="gmtCreate!= null">gmt_create=#{gmtCreate},</if> 
			<if test="modifier!= null">modifier=#{modifier},</if> 
			<if test="gmtModified!= null">gmt_modified=#{gmtModified},</if>
			<if test="isDeleted!= null">is_deleted=#{isDeleted},</if> 
			<if test="remark!= null">remark=#{remark},</if>
		</set>
		where id = #{id,jdbcType=INTEGER}
	</update>
	
	
	<select id="selectList" resultType="com.sandu.gateway.pay.trade.model.PayTradeTransfersRecord">
		SELECT
		    *
		FROM
		    pay_trade_Transfers_record
		WHERE is_deleted=0
		<if test="payTradeNo!= null">  and pay_trade_no = #{payTradeNo,jdbcType=VARCHAR} </if>
		<if test="intenalTradeNo != null">  and intenal_trade_no = #{intenalTradeNo,jdbcType=VARCHAR} </if>
		
	</select>
	

	
</mapper>

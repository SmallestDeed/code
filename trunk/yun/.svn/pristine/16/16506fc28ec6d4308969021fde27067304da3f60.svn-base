    <select id="getPayModelGroupRefList" resultMap="BaseResultMap" parameterType="java.util.Map">
        select
        t1.id, t1.business_id, t1.pay_model_config_id, t1.effective_time, t1.expiry_time, t1.is_deleted, t1.creator, t1.gmt_create,
        t1.modifier, t1.gmt_modified
        from pay_model_group_ref AS t1
        LEFT OUTER JOIN pay_model_config AS t2
        ON t1.pay_model_config_id = t2.id
        where t1.is_deleted = '0'
        AND t2.is_deleted = '0'
        AND t1.expiry_time > NOW()
        <if test="timeType != null and timeType != ''">
            AND t2.time_type = #{timeType}
        </if>
        <if test="rangeType != null">
            AND t2.range_type = #{rangeType}
        </if>
        <if test="businessId != null">
            AND t1.business_id = #{businessId,jdbcType=INTEGER}
        </if>
        <if test="list != null and list.size() > 0">
            AND t1.pay_model_config_id IN
            <foreach collection="list" item="c" open="(" close=")" separator=",">
                '${c.id}'
            </foreach>
        </if>
        ORDER BY t1.expiry_time DESC
    </select>
    <select id="getPayModelGroupRef" resultMap="BaseResultMap" parameterType="java.util.Map">
        select
        <include refid="Base_Column_List"/>
        from pay_model_group_ref
        where is_deleted = '0'
        <if test="businessId != null">
            AND business_id = #{businessId,jdbcType=INTEGER}
        </if>
        <if test="payModelConfigId != null">
            AND  pay_model_config_id = #{payModelConfigId,jdbcType=INTEGER}
        </if>
    </select>
    <select id="getUserRefInfoList" resultType="java.util.Map" parameterType="java.util.Map">
        SELECT t2.package_name  AS packageName,t2.time_type AS timeType,t1.expiry_time AS expiryTime,t2.biz_type AS bizType
        FROM pay_model_group_ref AS t1
        LEFT OUTER JOIN pay_model_config AS t2
        ON t2.id = t1.pay_model_config_id
        WHERE 1 = 1
        AND t1.is_deleted = '0'
        AND t2.is_deleted = '0'
        AND t1.expiry_time >= NOW()
        <if test="businessId != null and businessId != ''">
            AND t1.business_id = #{businessId}
        </if>
        <if test="rangeType != null and rangeType != ''">
            AND t2.range_type = #{rangeType}
        </if>
        <if test="bizType != null and bizType != ''">
            AND (t2.biz_type = #{bizType,jdbcType=VARCHAR}
            OR  t2.biz_type = 'renderTypeGive')
        </if>
        ORDER BY t1.expiry_time DESC
    </select>

    <!--获取用户可免费渲染的关联信息-->
    <select id="getPayModelVoRenderList" resultType="com.sandu.pay.order.PayModelVo" parameterType="java.util.Map">
        SELECT
            t2.pay_model_config_id AS payModelConfigId,
            t2.id AS payModelGroupRefId,
            t1.package_user_type AS packageUserType,
            t2.expiry_time AS expiryTime
        FROM
            pay_model_config AS t1
        LEFT OUTER JOIN pay_model_group_ref AS t2 ON t1.id = t2.pay_model_config_id
        WHERE
            1 = 1
        AND t1.is_deleted = '0'
        AND t2.is_deleted = '0'
        AND t1.biz_type = 'companyFreeRenderPlan'
        AND t1.business_type = '0'
        AND (NOW() BETWEEN t2.effective_time AND t2.expiry_time)
        AND t1.range_type = '2'
        AND (t1.package_user_type = #{packageUserType} OR t1.package_user_type = #{userType})
        <if test="platformId != null">
            AND t1.platform_id = #{platformId}
        </if>
        <if test="companyVoList != null and companyVoList.size() > 0">
            AND t2.business_id IN
            <foreach collection="companyVoList" item="c" open="(" close=")" separator=",">
                '${c.companyId}'
            </foreach>
        </if>
        ORDER BY
            t2.expiry_time DESC
        LIMIT 1
    </select>

    <!--获取用户可免费购买户型的关联信息-->
    <select id="getPayModelVoHouseList" resultType="com.sandu.pay.order.PayModelVo" parameterType="java.util.Map">
        SELECT
        t2.pay_model_config_id AS payModelConfigId,
        t2.id AS payModelGroupRefId,
        t1.package_user_type AS packageUserType,
        t2.expiry_time AS expiryTime
        FROM
        pay_model_config AS t1
        LEFT OUTER JOIN pay_model_group_ref AS t2 ON t1.id = t2.pay_model_config_id
        WHERE
        1 = 1
        AND t1.is_deleted = '0'
        AND t2.is_deleted = '0'
        AND t1.biz_type = 'companyFreeRenderPlan'
        AND t1.business_type = '1'
        AND (NOW() BETWEEN t2.effective_time AND t2.expiry_time)
        AND t1.range_type = '2'
        AND (t1.package_user_type = #{packageUserType} OR t1.package_user_type = #{userType})
        <if test="platformId != null">
            AND t1.platform_id = #{platformId}
        </if>
        AND t2.business_id = #{companyId}
        ORDER BY
        t2.expiry_time DESC
        LIMIT 1
    </select>

    <!--获取公司id-->
    <select id="getCompanyNameByDoMianName" resultType="java.lang.Integer" parameterType="java.lang.String">
        SELECT
        t1.id AS companyId
        FROM
        base_company AS t1
        WHERE
        t1.company_domain_name = #{companyDomainName}
        LIMIT 1
    </select>

</mapper>
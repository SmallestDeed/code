			and t1.gmt_modified between #{gmtModifiedStart,jdbcType=TIMESTAMP} and #{gmtModifiedEnd,jdbcType=TIMESTAMP}
		</if>
		<if test="isDeleted !=  null  ">			and t1.is_deleted  = #{isDeleted,jdbcType=INTEGER} </if>
		<if test="att1 !=  null and att1 !=  ''   "> and t1.att1  = #{att1,jdbcType=VARCHAR} </if>
		<if test="sch_Att1_ !=  null and sch_Att1_ !=  ''   ">
			and t1.att1  like CONCAT(CONCAT('%',#{sch_Att1_,jdbcType=VARCHAR}),'%')
		</if>
		<if test="sch_Att1 !=  null and sch_Att1 !=  ''   ">
			and t1.att1  like CONCAT('%',#{sch_Att1,jdbcType=VARCHAR})
		</if>
		<if test="schAtt1_ !=  null and schAtt1_ !=  ''   ">
			and t1.att1  like CONCAT(#{schAtt1_,jdbcType=VARCHAR},'%')
		</if>
		<if test="att1Start !=  null and att1End!= null  ">
			and t1.att1 between #{att1Start,jdbcType=VARCHAR} and #{att1End,jdbcType=VARCHAR}
		</if>
		<if test="att2 !=  null and att2 !=  ''   "> and t1.att2  = #{att2,jdbcType=VARCHAR} </if>
		<if test="sch_Att2_ !=  null and sch_Att2_ !=  ''   ">
			and t1.att2  like CONCAT(CONCAT('%',#{sch_Att2_,jdbcType=VARCHAR}),'%')
		</if>
		<if test="sch_Att2 !=  null and sch_Att2 !=  ''   ">
			and t1.att2  like CONCAT('%',#{sch_Att2,jdbcType=VARCHAR})
		</if>
		<if test="schAtt2_ !=  null and schAtt2_ !=  ''   ">
			and t1.att2  like CONCAT(#{schAtt2_,jdbcType=VARCHAR},'%')
		</if>
		<if test="att2Start !=  null and att2End!= null  ">
			and t1.att2 between #{att2Start,jdbcType=VARCHAR} and #{att2End,jdbcType=VARCHAR}
		</if>
		<if test="numa1 !=  null  ">			and t1.numa1  = #{numa1,jdbcType=INTEGER} </if>
		<if test="numa2 !=  null  ">			and t1.numa2  = #{numa2,jdbcType=INTEGER} </if>
		<if test="remark !=  null and remark !=  ''   "> and t1.remark  = #{remark,jdbcType=VARCHAR} </if>
		<if test="sch_Remark_ !=  null and sch_Remark_ !=  ''   ">
			and t1.remark  like CONCAT(CONCAT('%',#{sch_Remark_,jdbcType=VARCHAR}),'%')
		</if>		<if test="sch_Remark !=  null and sch_Remark !=  ''   ">
		and t1.remark  like CONCAT('%',#{sch_Remark,jdbcType=VARCHAR})
	</if>		<if test="schRemark_ !=  null and schRemark_ !=  ''   ">
		and t1.remark  like CONCAT(#{schRemark_,jdbcType=VARCHAR},'%')
	</if>		<if test="remarkStart !=  null and remarkEnd!= null  ">
		and t1.remark between #{remarkStart,jdbcType=VARCHAR} and #{remarkEnd,jdbcType=VARCHAR}
	</if>
	</select>

	<select id="getSharePaginatedListPc" resultMap="AllResultMap"
			parameterType="com.sandu.pay.order.model.search.PayOrderSearch">
		select
		t1.product_name,t1.total_fee,t1.`order_date`,t1.`current_integral`,t1.finance_type, dpr.plan_name,
		t1.product_type,t1.pay_type,t1.adjust_fee
		from pay_order  AS t1
		LEFT OUTER JOIN sys_user AS t2
		ON t1.user_id = t2.id
		LEFT OUTER JOIN base_platform AS t3
		ON t1.platform_id = t3.id
		LEFT JOIN design_plan_recommended dpr ON t1.plan_id = dpr.id
		WHERE 1 = 1
		AND  t1.is_deleted=0
		AND t1.product_type != 'authcode'
		AND t2.user_type = '3'
		AND t1.pay_state = 'SUCCESS'
		AND (t2.franchiser_account_type = '1' OR t2.franchiser_account_type = '2')
		<if test="platformBussinessType != null and platformBussinessType != ''">
			AND t3.platform_bussiness_type = #{platformBussinessType}
		</if>
		<if test="franchiserGroupId != null and franchiserGroupId != ''">
			AND t2.franchiser_group_id = #{franchiserGroupId}
		</if>
		ORDER BY t1.`id` DESC
		<if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
	</select>

	<select id="getRefundProductName" resultType="java.lang.String"
			parameterType="java.lang.String">
		SELECT CONCAT(IFNULL(t1.design_name,''),t2.product_name,'退还度币成功') AS productName
		FROM auto_render_task_state AS t1
		LEFT OUTER JOIN pay_order AS t2
		ON t1.order_number = t2.order_no
		WHERE  1 = 1
		AND t2.order_no = #{orderNo} limit 1
	</select>

	<select id="getRefundProductNamePc" resultType="java.lang.String"
			parameterType="java.lang.String">
		SELECT CONCAT(IFNULL(t3.plan_name,''),t1.product_name,'退还度币成功') AS productName
		FROM pay_order AS t1
		LEFT OUTER JOIN sys_task AS t2 ON t1.task_id = t2.id
		LEFT OUTER JOIN design_plan AS t3 ON t2.business_id = t3.id
		WHERE 1 = 1
		AND t1.order_no = #{orderNo}
	</select>

	<select id="selectPayModelConfig2C" resultType="com.sandu.pay.order.model.PayModelConfig">
		SELECT
		    *
		FROM
		    pay_model_config
		 WHERE
		    biz_type = 'renderType2C'
		    and is_deleted = 0;
	</select>

	<select id="countHouseByPayOrder" resultType="int">
		SELECT
			COUNT(DISTINCT house_id)
		FROM
			pay_order
		WHERE
			biz_type = "Buy"
		AND
		 	is_deleted = 0
		AND
		  	house_id != 0
		AND
			user_id = #{userId}
	</select>


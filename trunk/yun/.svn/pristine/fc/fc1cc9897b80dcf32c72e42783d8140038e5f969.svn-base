	<select id="getExpenseRecordCountV2" resultType="int">
		SELECT
		   COUNT(*)
		FROM
		   pay_order
		WHERE
		   is_deleted=0 AND
		   user_id = #{userId} AND
		   pay_state in ('SUCCESS','GIVES_UCCESS')
	</select>

	<select id="selectExpenseRecordListV2" resultMap="AllResultMap" parameterType="com.sandu.pay.order.model.search.PayOrderSearch">
		SELECT
		   *
		FROM
			pay_order
		WHERE
			is_deleted=0 AND
			user_id = #{userId} AND
			pay_state in ('SUCCESS','GIVES_UCCESS')
			ORDER BY gmt_modified DESC
		 <if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
	</select>

	<select id="selectPlanNameByPlanId" resultType="string">
          SELECT
              plan_name
          FORM
              design_plan_recommended
          WHERE
              id = #{planId} AND
              is_deleted=0
	</select>

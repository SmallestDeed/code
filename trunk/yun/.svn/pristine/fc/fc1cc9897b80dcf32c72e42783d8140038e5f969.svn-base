	<select id="getOrderByOrderNo" resultMap="AllResultMap" parameterType="java.lang.String">
		SELECT
			po.id,po.`user_id`,po.`product_type`,po.`product_name`,po.`order_no`,po.`prepay_id`,
			po.`total_fee`,po.`pay_type`,po.`pay_state`,po.`code_url`,po.`order_date`,po.`trade_type`,
			po.`finance_type`,po.`biz_type`,po.`task_id`,po.plan_id,po.`house_id`,po.`adjust_fee`,
			po.current_integral,su.`balance_amount`,po.product_id,po.platform_id,po.att1,po.att2
 		FROM pay_order po
		LEFT JOIN sys_user su ON su.id = po.`user_id`
		WHERE po.is_deleted = '0'
		AND su.is_deleted = '0'
		AND order_no = #{orderNo,jdbcType=VARCHAR}
	</select>
	

	<select id="selectExpenseRecordCount" resultType="int"
			parameterType="com.sandu.pay.order.model.search.PayOrderSearch">
		select
			count(po.id)
		from pay_order po
		LEFT JOIN design_plan_recommended dpr ON po.plan_id = dpr.id
        LEFT OUTER JOIN base_platform AS t2 ON po.platform_id = t2.id
        LEFT OUTER JOIN base_platform AS t3 ON t2.platform_bussiness_type = t3.platform_bussiness_type
		WHERE po.user_id = #{userId,jdbcType=INTEGER}
        <if test="platformCode != null and platformCode != ''">
            AND t3.platform_code = #{platformCode}
        </if>
		AND  po.is_deleted=0
		AND po.`pay_state` = #{payState,jdbcType=VARCHAR}
	</select>

	<select id="selectExpenseRecordList" resultMap="AllResultMap"
			parameterType="com.sandu.pay.order.model.search.PayOrderSearch">
		select
			po.product_name,po.total_fee,po.`order_date`,po.`current_integral`,po.finance_type, dpr.plan_name,
			po.product_type,po.pay_type,po.adjust_fee
		from pay_order po LEFT JOIN design_plan_recommended dpr ON po.plan_id = dpr.id
		LEFT OUTER JOIN base_platform AS t2
		ON po.platform_id = t2.id
		LEFT OUTER JOIN base_platform AS t3
		ON t2.platform_bussiness_type = t3.platform_bussiness_type
		WHERE po.user_id = #{userId,jdbcType=INTEGER}
		AND  po.is_deleted=0
		AND po.`pay_state` = #{payState,jdbcType=VARCHAR}
		<if test="platformCode != null and platformCode != ''">
			AND t3.platform_code = #{platformCode}
		</if>
		<if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
		ORDER BY po.`id` DESC
	</select>
	
	<select id="getConsumeSumByOrderNum" resultType="com.sandu.pay.order.model.PayOrder">
		SELECT
		SUM(total_fee) as totalFee,user_id as userId
		FROM
			pay_order
		WHERE
		is_deleted = 0
		AND pay_state = 'SUCCESS'
		AND finance_type='Out'
		AND
			user_id = (
				SELECT
					user_id AS A
				FROM
					`pay_order`
				WHERE
					order_no = #{orderNum}
			)
	</select>
	
	<select id="getCountByUserIdAndStatus" resultType="int">
		select count(id) from pay_order 
		where user_id = #{userId,jdbcType=INTEGER} 
		and pay_state = #{status,jdbcType=VARCHAR} 
		and is_deleted=0 and gmt_create>DATE_SUB(SYSDATE(),INTERVAL 5 MINUTE)
	</select>
	<update id="deleteByIdList">
		update pay_order
		<set>
			is_deleted = 1,
			<if test="remark!= null">  remark = #{remark,jdbcType=VARCHAR},</if>
			<if test="numa1!= null">  numa1 = #{numa1,jdbcType=INTEGER}</if>
		</set>

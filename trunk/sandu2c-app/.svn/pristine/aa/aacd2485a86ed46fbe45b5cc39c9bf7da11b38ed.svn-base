    <select id="selectList" resultMap="ResultMapList" parameterType="com.sandu.supplydemand.input.BaseSupplyDemandAdd">
        select
        bsd.id,bsd.type,bsd.creator_id,bsd.creator_type_value,bsd.supply_demand_category_id,bsd.province,bsd.city,bsd.district,bsd.street,
        bsd.address,bsd.cover_pic_id,bsd.title,bsd.description,bsd.description_pic_id,bsd.decoration_company,bsd.designer,bsd.material_shop,bsd.phone,bsd.contact,
        bsd.proprietor,bsd.builder,bsd.push_status,bsd.business_status,bsd.gmt_publish,bsd.sys_code,bsd.modifier,bsd.gmt_modified,bsd.is_deleted,bsd.remark,
        sdp.pic_path,ba1.area_name as provinceName ,ba2.area_name as cityName,ba3.area_name as districtName,ba4.area_name as streetName,bsd.view_num,u.nick_name,u.sex,
        dir.plan_type as planType,dir.plan_id AS planId ,dir.house_id houseId
        from base_supply_demand bsd
        left join supply_demand_pic sdp on bsd.cover_pic_id = sdp.id and sdp.is_deleted =0
        left join base_area ba1 on bsd.province = ba1.area_code
        left join base_area ba2 on bsd.city = ba2.area_code
        left join base_area ba3 on bsd.district = ba3.area_code
        left join base_area ba4 on bsd.street = ba4.area_code
        left join sys_user u on u.id = bsd.creator_id
        left join demand_info_rel dir on dir.supply_demand_id = bsd.id
        <where>
            <if test="supplyDemandIds != null and supplyDemandIds.size > 0">
                and bsd.id in
                <foreach collection="supplyDemandIds" index="index" item="supplyDemandId" open="(" separator="," close=")">

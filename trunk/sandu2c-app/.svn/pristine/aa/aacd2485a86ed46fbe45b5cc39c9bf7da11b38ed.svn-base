                </foreach>
            </if>
            <if test="supplyDemandId != null">and bsd.id = #{supplyDemandId,jdbcType=INTEGER}</if>
            <if test="mark!=null and creatorId != null ">and bsd.creator_id = #{creatorId,jdbcType=INTEGER}</if>
            <if test=" creatorTypes !=null and creatorTypes.size > 0">

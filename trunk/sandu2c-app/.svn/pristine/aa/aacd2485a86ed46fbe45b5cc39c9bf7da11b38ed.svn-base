            <if test="pushStatus != null"> and bsd.push_status = #{pushStatus,jdbcType=INTEGER} </if>

            and bsd.is_deleted = 0
        </where>
        <if test="isSortByGmtModified!=null and isSortByViewNum == null  ">order by bsd.recommended_time desc,bsd.gmt_publish ${isSortByGmtModified}</if>

        <if test="isSortByViewNum!=null  and isSortByGmtModified==null ">order by  bsd.recommended_time desc,bsd.view_num ${isSortByViewNum}</if>


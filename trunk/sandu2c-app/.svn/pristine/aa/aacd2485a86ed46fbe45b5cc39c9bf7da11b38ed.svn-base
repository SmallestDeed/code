            <if test = "streets != null and streets.size >0 ">
                AND bsd.street in
                <foreach collection="streets" index="index" item="item" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>


            <if test="supplyDemandCategoryIdList == null  and supplyDemandCategoryId!=null">AND supply_demand_category_id = #{supplyDemandCategoryId,jdbcType=VARCHAR} </if>
            <if test = "supplyDemandCategoryIdList != null and supplyDemandCategoryIdList.size >0 ">
                AND supply_demand_category_id in
                <foreach collection="supplyDemandCategoryIdList" index="index" item="item" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
            <if test="city !=null">AND bsd.city = #{city,jdbcType=VARCHAR}</if>
            <if test="district !=null ">AND bsd.district = #{district,jdbcType=VARCHAR}</if>
            <if test="title !=null">AND bsd.title like CONCAT(CONCAT('%',#{title,jdbcType=VARCHAR}),'%')</if>
            <if test="type !=null and type!=0">AND bsd.type = #{type,jdbcType=INTEGER}</if>
            <if test="mediation == null">
                <if test="decorationCompany != null">AND decoration_company = #{decorationCompany,jdbcType=INTEGER} </if>
                <if test="designer != null">AND designer = #{designer,jdbcType=INTEGER} </if>
                <if test="materialShop != null">AND material_shop = #{materialShop,jdbcType=INTEGER} </if>
                <if test="proprietor != null">AND proprietor = #{proprietor,jdbcType=INTEGER} </if>

    <select id="selectUserInfo" parameterType="java.lang.Integer" resultType="com.sandu.user.model.UserVo">
    SELECT
	s.id AS userId,
	s.user_name AS userName,
	s.nick_name AS nickName,
	s.user_type AS userType,
	s.mobile AS mobile,
	r.pic_path AS picPath,
	s.area_long_code as areaLongCode,
	s.uuid
    FROM
        sys_user s
    LEFT JOIN res_pic r ON r.id = s.pic_id
    WHERE
        s.id = #{id,jdbcType=INTEGER}
    AND s.is_deleted = 0
    </select>




    <select id="selectCompanyInfo" parameterType="java.lang.Integer" resultType="com.sandu.product.model.BaseCompany">
    SELECT
	s.id AS userId,
	b.company_name AS companyName,
	b.company_desc AS companyDesc,
    r.pic_path AS companyLogoPicPath,
    b1.area_name as provinceName,
    b2.area_name as cityName,
    b3.area_name as areaName,
    b4.area_name as streetName
    FROM
        sys_user s
    LEFT JOIN base_company b ON b.id = s.business_administration_id
    left join res_pic r ON r.id = b.company_logo
    left join base_area b1 on b1.area_code = b.province_code
    left join base_area b2 on b2.area_code = b.city_code
    left join base_area b3 on b3.area_code = b.area_code
    left join base_area b4 on b4.area_code = b.street_code
    WHERE
        s.id = #{id,jdbcType=INTEGER}
    AND s.is_deleted = 0
    </select>




    <select id="selectAllCompanyInfo" parameterType="list" resultType="com.sandu.product.model.BaseCompany">
        SELECT
        s.id AS userId,
        b.company_name AS companyName,
        r.pic_path AS companyLogoPicPath,
        b1.area_name cityName,
        b2.area_name areaName
        FROM
        sys_user s
        LEFT JOIN base_company b ON b.id = s.business_administration_id
        left join res_pic r ON r.id = b.company_logo
        left join base_area b1 on b1.area_code = b.city_code
        left join base_area b2 on b2.area_code = b.area_code
        WHERE
        s.id in
        <foreach collection="list" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>
        AND s.is_deleted = 0
    </select>



    <select id="selectCompanyShopInfo" parameterType="java.lang.Integer" resultType="com.sandu.user.model.CompanyShopVo">
        SELECT
            cs.id AS companyShopId,
            r.pic_path AS companyShopPicPath,
            cs.shop_name AS companyShopName,
            cs.shop_introduced AS companyShopDesc,
            b1.area_name AS provinceName,
            b2.area_name AS cityName,
            b3.area_name AS areaName,
            b4.area_name AS streetName
        FROM
            company_shop cs
        LEFT JOIN res_pic r ON r.id = cs.logo_pic_id
        LEFT JOIN base_area b1 ON b1.area_code = cs.province_code
        LEFT JOIN base_area b2 ON b2.area_code = cs.city_code
        LEFT JOIN base_area b3 ON b3.area_code = cs.area_code
        LEFT JOIN base_area b4 ON b4.area_code = cs.street_code
        WHERE
            cs.user_id = #{id,jdbcType=INTEGER}
        AND cs.is_deleted = 0
    </select>



    <select id="selectAreaInfo" parameterType="list" resultType="com.sandu.user.model.AreaInfoVo">
        SELECT
            area_name,
            id,
            area_code,
            level_id
        FROM
        base_area
        WHERE
        area_code IN
        <foreach collection="list" item="code" separator="," open="(" close=")">

     <select id="getLevelInfo" parameterType="com.sandu.user.model.SysUserLevelConfig" resultType="com.sandu.user.model.SysUserLevelBo">
            SELECT
                C.id as id,
                P.price as price,
                P.user_pay_type as userPayType
            FROM
                sys_user_level_config C
            INNER JOIN sys_user_level_price P ON C.business_id = P.id
            WHERE
                C.business_type_id = #{businessTypeId}
            AND C.user_id = #{userId}
     </select>
     
     <select id="getListByUserId" resultType="com.sandu.user.model.SysUserLevelPrice">
        SELECT
            P.id AS id,
            P.end_user_group_name AS endUserGroupName,
            P.user_pay_type AS userPayType,
            P.price AS price
        FROM
            sys_user U
        INNER JOIN sys_user_register_info R ON U.id = R.user_id
        INNER JOIN sys_user_level_price P ON P.end_user_group_name = R.user_type
        WHERE
            U.id = #{userId}  and U.is_deleted = 0
        ORDER BY P.user_pay_type desc;
    </select>
     <update id="updateById">
      UPDATE sys_user_level_config SET business_id = #{levelId} WHERE id=#{id}
    </update>


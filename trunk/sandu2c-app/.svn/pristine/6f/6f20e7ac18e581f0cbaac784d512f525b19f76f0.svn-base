  <select id="selectAllInviteList" resultType="com.sandu.user.model.view.UserInviteTopListVo" parameterType="com.sandu.user.model.input.UserInviteAdd" >
    select
    count(u.id) AS inviteCount,
		u.invite_id AS inviteId,
		s.nick_name userName,
		r.pic_path userIcon,
		d.name userTypeStr
    from user_invite u
    LEFT JOIN sys_user s on s.id = u.invite_id
    left join res_pic r on r.id = s.pic_id
    left join sys_dictionary d on d.type = 'userType' and d.value = s.user_type
        where u.is_deleted = 0
        and u.status = 1
    GROUP BY u.invite_id
    ORDER BY count(u.id) DESC
    <if test="start != null and start != -1"> LIMIT #{start}, #{limit}</if>
  </select>


  <select id="selectAllInviteCount" resultType="java.lang.Integer"  >

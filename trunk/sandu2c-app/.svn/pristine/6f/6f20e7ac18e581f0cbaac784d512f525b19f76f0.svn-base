  <select id="selectMyUserInviteList" resultType="com.sandu.user.model.view.UserInviteListVo" parameterType="object" >
    select
    u.gmt_create AS registertime,
    u.gmt_modified AS signTime,
    u.status AS status,
    s.user_type AS userType,
    s.nick_name AS userName,
    CONVERT(IF(ISNULL(s.sex),0,s.sex),SIGNED) AS sex,
    r.pic_path AS userPicPath
    from user_invite_track u
    left join sys_user s on s.id = u.fid and    s.is_deleted = 0
    left join res_pic r on r.id = s.pic_id and r.is_deleted = 0
    where u.is_deleted = 0
    <if test="status != null and status != -1">
      and u.status = #{status}
    </if>
    and u.invite_id = #{inviteId,jdbcType=INTEGER}
    order by u.gmt_modified DESC
    limit #{start}, #{limit}
  </select>


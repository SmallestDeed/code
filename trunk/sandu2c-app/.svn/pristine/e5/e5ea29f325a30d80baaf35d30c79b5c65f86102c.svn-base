            <if test="orderNum != null and  orderNum !='' ">
                ${orderNum}
            </if>
        </if>
        <!--  <if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>  -->
        <if test="currentPage != -1 and pageSize != -1  ">
            LIMIT #{currentPage}, #{pageSize}
        </if>
    </select>

    <!-- 查询最大户型编码 -->
    <select id="codeString" resultType="java.lang.String" parameterType="java.lang.String">
        select MAX(Right(house_door_code, 6)) from base_house where house_door_code like
        CONCAT(#{code,jdbcType=VARCHAR},'%')
    </select>

    <!-- 一次性取出要处理的数据 -->
    <select id="addHouseCode" resultMap="AllResultMap" parameterType="java.lang.String">
        select
        <include refid="All_Column_List"/>
        from base_house
        where is_deleted = 0
        <if test="houseKind !=  null and houseKind !=  ''   ">and house_kind = #{houseKind,jdbcType=VARCHAR}</if>
        <if test="houseKind ==  null or houseKind ==  ''   ">and (house_kind is null or house_kind = '')</if>
        and area_long_code like CONCAT(#{areaLongCode,jdbcType=VARCHAR},'%')
    </select>

    <update id="updateByPrimaryKey">
        update base_house
        <set>
            house_door_code = #{houseDoorCode,jdbcType=VARCHAR}
        </set>
        where id = #{id,jdbcType=INTEGER}
    </update>
    
    <select id="getHaveSpaceCount" resultType="int"
		parameterType="com.sandu.home.model.search.BaseHouseSearch">
		select count(DISTINCT h.id)
		from base_house as h
		LEFT JOIN house_space_new   s on (s.house_id = h.id)
        <!--JOIN base_house_guide_pic_info bhgpi ON s.house_id = bhgpi.house_id-->
        LEFT JOIN space_common c on(s.standard_space_id = c.id)
		LEFT JOIN design_templet t ON (t.space_common_id = c.id)
		where 1=1
        <!--为户型绘制的户型-->
        AND c.origin = 1
		AND c.id is not null
		AND t.id is not null
		AND h.is_deleted = 0
		AND c.is_deleted = 0
		AND t.is_deleted = 0
		AND s.is_deleted = 0
		<if test="spaceCommonStatusList !=  null">
			and c.status  in 
			<foreach collection="spaceCommonStatusList" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
		<if test="designTempletPutawayStateList !=  null">
			and t.putaway_state  in 
			<foreach collection="designTempletPutawayStateList" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
		<if test="livingId !=  null and livingId!='' ">
		 	and h.living_id = #{livingId,jdbcType=INTEGER}
		</if>	
	</select>
	
	<select id="getHaveSpaceList" resultMap="AllResultMap"
		parameterType="com.sandu.home.model.search.BaseHouseSearch">
		  SELECT DISTINCT
			h.id,
			h.sys_code,
			h.creator,
			h.gmt_create,
			h.is_public,
			h.modifier,
			h.gmt_modified,
			h.is_deleted,
			h.house_code,
			h.house_short_name,
			h.house_name,
			h.pic_res1_id,
			h.pic_res2_id,
			h.pic_res3_id,
			h.pic_res4_id,
			h.house_desc,
			h.living_id,
			h.total_area,
			h.total_floors,
			h.current_period,
			h.house_high,
			h.apply_floors,
			h.house_layout,
			h.room_layout,
			h.office_layout,
			h.way_layout,
			h.is_exist_symmetry,
			h.symmetry_ids,
			h.is_merge,
			h.merge_ids,
			h.merge_new_id,
			h.pic_res_id,
			h.is_all,
			h.house_common_code,
			h.house_type_code,
			h.house_tag,
			h.house_status,
			h.deal_status,
			h.is_review,
			h.area_code,
			h.att1,
			h.area_long_code,
			h.house_kind,
			h.att4,
			h.att5,
			h.att6,
			h.date_att1,
			h.date_att2,
			h.reset_state,
			h.num_att2,
			h.num_att3,
			h.num_att4,
			h.remark,
            h.snap_pic_id
		FROM
			base_house AS h
        LEFT JOIN base_living bl on bl.id = h.living_id
		LEFT JOIN house_space_new s ON (s.house_id = h.id)
        <!--JOIN base_house_guide_pic_info bhgpi ON s.house_id = bhgpi.house_id-->
        LEFT JOIN space_common c ON (s.standard_space_id = c.id)
		LEFT JOIN design_templet t ON (t.space_common_id = c.id)
		where 1=1
        <!--为户型绘制的户型-->
        AND c.origin = 1
		AND c.id is not null
		AND t.id is not null
		AND h.is_deleted = 0
		AND c.is_deleted = 0
		AND t.is_deleted = 0
		AND s.is_deleted = 0
		<if test="spaceCommonStatusList !=  null">
			and c.status  in 
			<foreach collection="spaceCommonStatusList" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
		<if test="designTempletPutawayStateList !=  null">
			and t.putaway_state  in 
			<foreach collection="designTempletPutawayStateList" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
		<if test="livingId !=  null and livingId!='' ">
		 	and h.living_id = #{livingId,jdbcType=INTEGER}
		</if>	
		<if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
	</select>

    <select id="listBaseHouseByDesignPLanReanderSceneId" resultMap="AllResultMap">
        SELECT DISTINCT bh.id, bh.house_common_code
        FROM base_house bh
        LEFT JOIN pay_order po ON bh.id = po.house_id
        LEFT JOIN auto_render_task_state arts ON arts.order_number = po.order_no
        WHERE arts.business_id in
          <foreach collection="list" separator="," index="index" item="sceneId" open="(" close=")">
              #{sceneId}
          </foreach>
    </select>
    <select id="getListByIds" resultMap="AllResultMap">
        select
            t1.id,
            t1.house_common_code,
            t1.living_id,
            t3.living_name,
            group_concat(t2.space_type) space_type_strs,
            t1.snap_pic_id,
            t1.pic_res1_id,
            t1.total_area
            <if test="houseIdsStr != null and houseIdsStr != ''">
                ,locate(concat(concat(',',t1.id),','),#{houseIdsStr}) sort
            </if>
        from
            base_house t1
            inner join house_space_new t2 on t2.house_id = t1.id and t2.is_deleted = 0
            inner join base_living t3 on t3.id = t1.living_id and t3.is_deleted = 0
        where
            1 = 1
            <if test="houseIds != null and houseIds.size > 0">
                and t1.id in
                <foreach collection="houseIds" index="index" item="houseId" open="(" separator="," close=")">
                    #{houseId}
                </foreach>
            </if>
        group by t1.id
        <if test="houseIdsStr != null and houseIdsStr != ''">
            order by sort
        </if>
        <if test="start != null and limit != null">
            limit #{start},#{limit}
        </if>
    </select>
</mapper>

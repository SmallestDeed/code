	   <select id="houseSearchList" resultMap="baseHouseResultMap"
			parameterType="com.sandu.home.model.BaseHouseResult">
    SELECT l.living_name,l.area_long_code,l.id as living_id,COUNT(DISTINCT h.id) AS house_count FROM base_living l
	LEFT JOIN base_house h ON l.id = h.living_id and h.is_deleted=0 
	LEFT JOIN house_space_new   s on (s.house_id = h.id)
    <!--JOIN base_house_guide_pic_info bhgpi ON s.house_id = bhgpi.house_id-->
    LEFT JOIN space_common c on(s.standard_space_id = c.id)
	LEFT JOIN design_templet t ON (t.space_common_id = c.id)
	where 1=1
           <!--为户型绘制的户型-->
           AND c.origin = 1
	AND l.is_deleted = 0
	AND h.is_deleted = 0
	AND s.is_deleted = 0
	AND c.is_deleted = 0
	AND t.is_deleted = 0
	AND t.id is not null
	AND c.id is not null
	<if test="spaceCommonStatusList !=  null">
		and c.status  in 
		<foreach collection="spaceCommonStatusList" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</if>
	<if test="designTempletPutawayStateList !=  null">
		and t.putaway_state  in 
		<foreach collection="designTempletPutawayStateList" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</if>
	<if test="areaLongCode !=  null and areaLongCode !=  ''   ">
			and l.area_long_code  like CONCAT(CONCAT('%',#{areaLongCode,jdbcType=VARCHAR}),'%')
	</if>
	<if test="livingName != null and livingName != ''">
		AND l.living_name LIKE CONCAT(CONCAT('%',#{livingName,jdbcType=VARCHAR}),'%')
	</if>
    GROUP BY l.id
	 <if test="start !=-1 and limit !=-1">LIMIT #{start}, #{limit}</if>
	</select>
	
     <select id="getHouseCount" resultType="int"
		parameterType="com.sandu.home.model.BaseHouseResult">
		SELECT count(p.id) from
    (SELECT l.id FROM base_living l
	LEFT JOIN base_house h ON l.id = h.living_id and h.is_deleted=0 
	LEFT JOIN house_space_new   s on (s.house_id = h.id)
	LEFT JOIN space_common c on(s.standard_space_id = c.id)
	LEFT JOIN design_templet t ON (t.space_common_id = c.id)
	where 1=1
         <!--为户型绘制的户型-->
         AND c.origin = 1
	AND l.is_deleted = 0
	AND h.is_deleted = 0
	AND s.is_deleted = 0
	AND c.is_deleted = 0
	AND t.is_deleted = 0
	AND t.id is not null
	AND c.id is not null
	<if test="spaceCommonStatusList !=  null">
		and c.status  in 
		<foreach collection="spaceCommonStatusList" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</if>
	<if test="designTempletPutawayStateList !=  null">
		and t.putaway_state  in 
		<foreach collection="designTempletPutawayStateList" index="index" item="item" open="(" separator="," close=")">
			#{item}
		</foreach>
	</if>
	<if test="areaLongCode !=  null and areaLongCode !=  ''   ">

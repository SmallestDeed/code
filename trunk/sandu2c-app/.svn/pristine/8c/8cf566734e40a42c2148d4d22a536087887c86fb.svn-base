    <!-- 通过户型ID获取该户型下所有的空间类型 -->
    <select id="getSpaceTypeListByHouseId" resultType="String"
            parameterType="java.lang.Integer">
        select
        h.space_type
        from house_space_new h
        where 1=1 and h.house_id = #{houseId}
    </select>

    <select id="selectHouseIdNextHouseSpaceNew" resultType="int" parameterType="java.lang.Integer">
        select count(b.id) from house_space_new as h
        LEFT JOIN base_house as b on(b.id=h.house_id)
        where h.house_id = #{houseId,jdbcType=INTEGER}
    </select>
    
    <!-- 通过户型ID获取该户型下有哪些空间类型 -->
    <select id="getSpaceTypeByHouseId" resultType="String"
            parameterType="java.lang.Integer">
        SELECT 
		  DISTINCT c.space_function_id 
		FROM
		  space_common c 
		  JOIN house_space_new s 
		    ON s.standard_space_id = c.id 
		  JOIN base_house h 
		    ON h.id = s.house_id 
		  JOIN base_living l 
		    ON l.id = h.living_id
		  JOIN design_templet dt
		    ON dt.space_common_id = c.id
		WHERE 1 = 1 
		  AND s.house_id = #{houseId,jdbcType=INTEGER}
		  AND c.status = 3
		  AND c.is_standard_space = 1
		  AND c.`is_deleted` = 0
		  AND s.`is_deleted` = 0 
		  AND h.`is_deleted` = 0
		  AND dt.is_deleted = 0
		  AND dt.putaway_state = 3
    </select>

    <select id="getFullHouseSpaceFunctionId" resultType="integer">
        select value from sys_dictionary where type = 'houseType' and valuekey = 'fullroom'
    </select>

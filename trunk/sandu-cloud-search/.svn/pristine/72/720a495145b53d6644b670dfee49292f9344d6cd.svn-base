public class ProductFullTextQueryException extends Exception {

    public ProductFullTextQueryException() {
    }

    public ProductFullTextQueryException(String message) {
        super(message);
    }

    public ProductFullTextQueryException(String message, Throwable cause) {
        super(message, cause);
    }

    public ProductFullTextQueryException(Throwable cause) {
        super(cause);
    }

    public ProductFullTextQueryException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
}

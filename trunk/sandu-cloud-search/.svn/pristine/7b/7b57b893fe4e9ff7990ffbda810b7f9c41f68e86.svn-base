package com.sandu.search.exception;

/**
 * 分类产品索引异常
 *
 * @date 20171213
 * @auth pengxuangang
 */
public class CategoryProductIndexException extends Exception {
    public CategoryProductIndexException() {
    }

    public CategoryProductIndexException(String message) {
        super(message);
    }

    public CategoryProductIndexException(String message, Throwable cause) {
        super(message, cause);
    }

    public CategoryProductIndexException(Throwable cause) {
        super(cause);
    }

    public CategoryProductIndexException(String message, Throwable cause, boolean enableSuppression, boolean writableStackTrace) {
        super(message, cause, enableSuppression, writableStackTrace);
    }
}

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">


    <!-- 方案 -->
    <dubbo:reference id="designPlanService"
                     interface="com.sandu.design.service.DesignPlanService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 推荐方案 -->
    <dubbo:reference id="designPlanRecommendedService"
                     interface="com.sandu.design.service.DesignPlanRecommendedService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 推荐方案产品 -->
    <dubbo:reference id="designPlanRecommendedProductService"
                     interface="com.sandu.design.service.DesignPlanRecommendedProductService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 自动渲染 -->
    <dubbo:reference id="optimizePlanService"
                     interface="com.sandu.design.service.OptimizePlanService" retries="${dubbo.retries}" version="${dubbo.version}"/>


</beans>
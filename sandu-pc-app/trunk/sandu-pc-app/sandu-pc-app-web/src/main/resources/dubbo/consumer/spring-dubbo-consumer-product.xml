<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- 方案产品 -->
    <dubbo:reference id="designPlanProductService"
                     interface="com.sandu.product.service.DesignPlanProductService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 企业信息 -->
    <dubbo:reference id="baseCompanyService"
                     interface="com.sandu.product.service.BaseCompanyService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 企业店铺 -->
    <dubbo:reference id="companyShopService"
                     interface="com.sandu.product.service.CompanyShopService" retries="${dubbo.retries}" version="${dubbo.version}"/>

</beans>
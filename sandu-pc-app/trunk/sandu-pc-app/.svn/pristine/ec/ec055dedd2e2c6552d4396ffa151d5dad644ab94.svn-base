debug=true
server.port=36001

# logging
logging.level.root=info
logging.level.com.sandu=debug


### Dubbo
dubbo.application.name=${spring.application.name}
dubbo.registry.address=127.0.0.1:2181
dubbo.consumer.check=false
dubbo.timeout=30000
dubbo.version=1.0.1
dubbo.retries=0

### Log
#logging.config=classpath\:log4j2.xml

app.sso.url =http\://sso.dev.sanduspace.com/

##上传文件限制大小
spring.http.multipart.max-file-size = 10MB

#设定HttpHeader请求头大小
server.maxHttpHeaderSize=102400000   

#resource url
app.resources.url =http://show.dev.sanduspace.com

#web server upload
#app.upload.root = /data001/mfs
app.upload.root = E:\\\\
#app.upload.root = /data001/resource

# 说明:配置不同模块文件存放的根节点(未加密文件)
app.resources.upload.root.distribute={"cfg":[{"modelName":"d_userdesign,e_userlogs,b_base,c_basedesign,a_common,f_resource,g_other,f_version","uploadRoot":"/data001/resource/src"}]}

# 配置不同模块文件加密后存放的根节点(加密文件)
app.resources.encrypt.upload.root.distribute={"cfg":[{"modelName":"d_userdesign,e_userlogs,b_base,c_basedesign,a_common,f_resource,g_other,f_version","uploadRoot":"/data001/resource"}]}

# 域名配置
app.resources.url.distribute={"cfg":[{"modelName":"d_userdesign,e_userlogs,b_base,c_basedesign,a_common,f_resource,g_other,f_version","domain":"http://show.dev.sanduspace.com"}]}


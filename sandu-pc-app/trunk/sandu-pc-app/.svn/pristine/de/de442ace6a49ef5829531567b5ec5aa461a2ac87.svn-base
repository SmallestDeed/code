        <if test="sch_SysCode !=  null and sch_SysCode !=  ''   ">
            and sys_code  like CONCAT('%',#{sch_SysCode,jdbcType=VARCHAR})
        </if>
        <if test="schSysCode_ !=  null and schSysCode_ !=  ''   ">

        select
        <include refid="All_Column_List" />
        from sys_dictionary
        where 1=1
        <if test="showU3dModel!= null">  and show_u3d_model = #{showU3dModel,jdbcType=INTEGER} </if>
        <if test="att7!= null">  and att7 = #{att7,jdbcType=VARCHAR} </if>
        <if test="att7Info!= null">  and att7_info = #{att7Info,jdbcType=VARCHAR} </if>
        <if test="sysCode !=  null and sysCode !=  ''   "> and sys_code  = #{sysCode,jdbcType=VARCHAR} </if>

package com.sandu.common.cache.service.impl;

import java.util.Map;

import com.sandu.common.cache.CommonCacher;
import com.sandu.common.cache.service.CommonCacheService;
import com.sandu.common.metadata.ModuleType;
import com.sandu.common.model.ResponseEnvelope;

public class CommonCacheServiceImpl implements CommonCacheService {

	@Override
	public void removeAll(ModuleType designplan, String method, Map<Object, Object> paramsMap) {
		CommonCacher.removeAll(designplan, method, paramsMap);
	}

	@SuppressWarnings("rawtypes")
	@Override
	public ResponseEnvelope getAll(ModuleType moduleType, String method, Map<Object, Object> paramsMap) {
		return CommonCacher.getAll(moduleType, method, paramsMap);
	}

	@SuppressWarnings("rawtypes")
	@Override
	public void addAll(ModuleType moduleType, String method, Map<Object, Object> paramsMap,
			ResponseEnvelope responseEnvelope) {
		CommonCacher.addAll(moduleType, method, paramsMap, responseEnvelope);
	}

}

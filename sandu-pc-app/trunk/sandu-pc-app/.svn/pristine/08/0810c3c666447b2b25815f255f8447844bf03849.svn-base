<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!-- 720制作 -->
    <dubbo:reference id="designPlanStoreReleaseService"
                     interface="com.sandu.panorama.service.DesignPlanStoreReleaseService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 720制作，联系人 -->
    <dubbo:reference id="unionContactService"
                    interface="com.sandu.panorama.service.UnionContactService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 720制作，效果图方案 TEMP -->
    <dubbo:reference id="designPlanRenderSceneService"
                     interface="com.sandu.design.service.DesignPlanRenderSceneService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!-- 方案切换 TEMP -->
    <dubbo:reference id="designPlanRecommendedServiceV2"
                    interface="com.sandu.design.service.DesignPlanRecommendedServiceV2" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!--720制作，联盟门店-->
    <dubbo:reference id="unionGroupService"
                     interface="com.sandu.panorama.service.UnionGroupService" retries="${dubbo.retries}" version="${dubbo.version}"/>

    <!--720制作，优惠活动-->
    <dubbo:reference id="unionSpecialOfferService"
                     interface="com.sandu.panorama.service.UnionSpecialOfferService" retries="${dubbo.retries}" version="${dubbo.version}"/>

</beans>
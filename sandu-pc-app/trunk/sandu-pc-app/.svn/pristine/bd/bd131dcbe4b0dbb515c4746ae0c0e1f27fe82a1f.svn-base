package com.sandu.panorama.model.input;

import io.swagger.annotations.ApiModelProperty;
import lombok.Data;
import org.springframework.web.multipart.MultipartFile;

import java.io.Serializable;

@Data
public class UnionContactUpdate implements Serializable{

    @ApiModelProperty(value = "联系人ID", dataType = "int", required = true)
    private Integer id;
    @ApiModelProperty(value = "联系人名称", dataType = "string")
    private String name;
    @ApiModelProperty(value = "联系人电话", dataType = "string")
    private String phone;
    @ApiModelProperty(value = "联系人头像", dataType = "file")
    private MultipartFile pic;

}

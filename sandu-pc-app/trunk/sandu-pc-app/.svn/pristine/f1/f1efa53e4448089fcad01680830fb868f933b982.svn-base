<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- demo -->
    <!-- <dubbo:service interface="com.sandu.demo.service.DecorateDemoService"
                   ref="decorateDemoService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/> -->

	<!-- design module ->start -->
	<dubbo:service interface="com.sandu.design.service.DesignTempletProductService"
                   ref="designTempletProductService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

	<dubbo:service interface="com.sandu.design.service.DesignTempletService"
                   ref="designTempletService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

	<dubbo:service interface="com.sandu.home.service.SpaceCommonService"
                   ref="spaceCommonService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	<!-- design module ->end -->

	<!-- product module ->start -->
	<!-- BaseBrandService -->
	<dubbo:service interface="com.sandu.home.service.SpaceCommonService"
                   ref="spaceCommonService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
                   
	<!-- BaseProductService -->
	<dubbo:service interface="com.sandu.product.service.BaseProductService"
                   ref="baseProductService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
                   
	<!-- GroupProductDetailsService -->
	<dubbo:service interface="com.sandu.product.service.GroupProductDetailsService"
                   ref="groupProductDetailsService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- GroupProductService -->
	<dubbo:service interface="com.sandu.product.service.GroupProductService"
                   ref="groupProductService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- ProductAttributeService -->
	<dubbo:service interface="com.sandu.product.service.ProductAttributeService"
                   ref="productAttributeService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- ProductPropsService -->
	<dubbo:service interface="com.sandu.product.service.ProductPropsService"
                   ref="productPropsService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- StructureProductDetailsService -->
	<dubbo:service interface="com.sandu.product.service.StructureProductDetailsService"
                   ref="structureProductDetailsService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- StructureProductService -->
	<dubbo:service interface="com.sandu.product.service.StructureProductService"
                   ref="structureProductService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	<!-- product module ->end -->
	
	<!-- system module ->start -->
	<!-- ResDesignService -->
	<dubbo:service interface="com.sandu.system.service.ResDesignService"
                   ref="resDesignService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
                   
	<!-- ResFileService -->
	<dubbo:service interface="com.sandu.system.service.ResFileService"
                   ref="resFileService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- ResModelService -->
	<dubbo:service interface="com.sandu.system.service.ResModelService"
                   ref="resModelService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- ResPicService -->
	<dubbo:service interface="com.sandu.system.service.ResPicService"
                   ref="resPicService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- ResTextureService -->
	<dubbo:service interface="com.sandu.system.service.ResTextureService"
                   ref="resTextureService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- SysDictionaryService -->
	<dubbo:service interface="com.sandu.system.service.SysDictionaryService"
                   ref="sysDictionaryService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- SysRoleService -->
	<dubbo:service interface="com.sandu.system.service.SysRoleService"
                   ref="sysRoleService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- SysUserRoleService -->
	<dubbo:service interface="com.sandu.system.service.SysUserRoleService"
                   ref="sysUserRoleService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>
	
	<!-- system module ->end -->

</beans>
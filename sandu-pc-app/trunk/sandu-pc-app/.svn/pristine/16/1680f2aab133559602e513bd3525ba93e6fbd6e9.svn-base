    <!-- 根据uuid查询720分享 -->
    <select id="getDesignPlanStoreRelease" resultMap="getPanoramaResultMap"
            parameterType="java.lang.String">
        select r.id,r.uuid,r.user_id,r.share_title,r.share_type,r.pv,r.union_group_id,r.union_special_offer_id,r.union_contact_id,r.house_id,r.house_pic_id from design_plan_store_release r
        where r.is_deleted = 0 and r.uuid = #{uuid,jdbcType=VARCHAR};
    </select>

    <!-- 通过用户ID获取分享总数 -->
    <select id="getCountByUserId" resultType="java.lang.Integer"
            parameterType="com.sandu.panorama.model.input.DesignPlanStoreReleaseSearch">
        select count(r.id) from design_plan_store_release r
        left join design_plan_store_release_details rd on rd.store_release_id = r.id
        left join design_plan_render_scene dprs on dprs.id = rd.design_plan_id
        left join res_render_pic rrp on rrp.id = dprs.cover_pic_id
        where r.is_deleted = 0 and rd.is_deleted = 0 and rd.is_main = 1
        <if test="userId != null">
            and r.user_id = #{userId,jdbcType=INTEGER}
        </if>
        <if test="shareTitle != null and shareTitle != ''">
            and r.share_title like concat("%", #{shareTitle,jdbcType=VARCHAR}, "%")
        </if>
        order by r.gmt_create desc
    </select>

    <!-- 通过用户ID获取分享列表 -->
    <select id="getListByUserId" resultMap="getPanoramaResultMap"
            parameterType="com.sandu.panorama.model.input.DesignPlanStoreReleaseSearch">
        select r.id,r.share_title,r.gmt_create,r.share_type,rd.design_plan_id main_design_plan_id,rrp.pic_path coverPicPath,r.pv from design_plan_store_release r
        left join design_plan_store_release_details rd on rd.store_release_id = r.id
        left join design_plan_render_scene dprs on dprs.id = rd.design_plan_id
        left join res_render_pic rrp on rrp.id = dprs.cover_pic_id
        where r.is_deleted = 0 and rd.is_deleted = 0 and rd.is_main = 1
        <if test="userId != null">
            and r.user_id = #{userId,jdbcType=INTEGER}
        </if>
        <if test="shareTitle != null and shareTitle != ''">
            and r.share_title like concat("%", #{shareTitle,jdbcType=VARCHAR}, "%")
        </if>
        order by r.gmt_create desc
        <if test="start != null and limit != null">
            LIMIT #{start}, #{limit}
        </if>
    </select>

    <!-- 获取方案封面图  -->
    <select id="getCoverPicPath" resultType="java.lang.String"
            parameterType="java.lang.Integer">
        select rrp.pic_path from design_plan_render_scene dprs
        left join res_render_pic rrp on rrp.design_scene_id = dprs.id
        and (rrp.file_key = 'design.designPlan.render.small.pic' or rrp.file_key = 'design.designPlan.render.video.cover')
        and rrp.is_deleted = 0
        where dprs.is_deleted = 0 and dprs.id = #{mainDesignPlanId,jdbcType=INTEGER}
        order by rrp.id desc limit 1;
    </select>
    

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 720制作 -->
    <dubbo:service interface="com.sandu.panorama.service.DesignPlanStoreReleaseService"
                   ref="designPlanStoreReleaseService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

    <!-- 720制作，联系人 -->
    <dubbo:service interface="com.sandu.panorama.service.UnionContactService"
                   ref="unionContactService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

    <!-- 720制作，效果图方案 TEMP -->
    <dubbo:service interface="com.sandu.design.service.DesignPlanRenderSceneService"
                   ref="designPlanRenderSceneService" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

    <!-- 推荐方案 -->
    <dubbo:service interface="com.sandu.design.service.DesignPlanRecommendedServiceV2"
                   ref="designPlanRecommendedServiceV2" protocol="dubbo" version="${dubbo.version}" timeout="${dubbo.timeout}"/>

</beans>
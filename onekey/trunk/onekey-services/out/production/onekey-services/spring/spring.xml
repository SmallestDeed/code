<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd
">
	<import resource="classpath:spring/spring-common.xml"/>
	<import resource="classpath:spring/spring-mybatis.xml"/>
	<import resource="classpath:spring/spring-context-jedis.xml"/>
	<import resource="classpath:spring/spring-exception.xml"/>
	<!-- Dubbo配置 -->
	<import resource="classpath:dubbo/spring-dubbo.xml"/>
	<!-- Dubbo服务提供者配置 -->
	<import resource="classpath:dubbo/provider/spring-dubbo-provider-**.xml"/>
	
	<import resource="classpath:dubbo/consumer/spring-dubbo-consumer-**.xml"/>

	<bean id="springContextHolder" class="com.nork.common.util.SpringContextHolder"/>

	<!-- Init thread pool -->
	<bean id="uploadFileThreadPoolManager" class="com.nork.threadpool.ThreadPoolManager" init-method="init" destroy-method="destroy"/>  
</beans>
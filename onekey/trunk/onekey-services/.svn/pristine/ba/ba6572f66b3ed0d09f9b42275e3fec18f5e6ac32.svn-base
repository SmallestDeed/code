<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd
                    http://code.alibabatech.com/schema/dubbo
                    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 智能装修（一键装修） -->
    <dubbo:service interface="com.nork.onekeydesign.service.IntelligenceDecorationService" ref="intelligenceDecorationService" protocol="dubbo" timeout="300000" version="1.0"/>
    <!-- 样板房服务 -->
    <dubbo:service interface="com.nork.onekeydesign.service.DesignTempletService" ref="designTempletService" protocol="dubbo" timeout="10000" version="1.0"/>
    <!-- 推荐方案服务 -->
    <dubbo:service interface="com.nork.onekeydesign.service.DesignPlanRecommendedService" ref="designPlanRecommendedService" protocol="dubbo" timeout="10000" version="1.0"/>
    <!-- 记录已使用推荐方案服务 -->
    <dubbo:service interface="com.nork.onekeydesign.service.TempletPlanOperationHistoryService" ref="templetPlanOperationHistoryService" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 一键自动装修服务 -->
	<dubbo:service interface="com.nork.onekeydesign.service.OptimizePlanService" ref="optimizePlanService" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 样板房产品服务 -->
	<dubbo:service interface="com.nork.onekeydesign.service.DesignTempletProductService" ref="designTempletProductService" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 设计方案服务 -->
	<dubbo:service interface="com.nork.onekeydesign.service.DesignPlanService" ref="designPlanService" protocol="dubbo" timeout="300000" version="1.0"/>
	<!-- 配置文件资源服务 -->
	<dubbo:service interface="com.nork.system.service.ResFileService" ref="resFileService" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 方案资源服务 -->
	<dubbo:service interface="com.nork.system.service.ResDesignService" ref="resDesignService" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 推荐方案产品服务 -->
	<dubbo:service interface="com.nork.onekeydesign.service.DesignPlanRecommendedProductServiceV2" ref="designPlanRecommendedProductServiceV2" protocol="dubbo" timeout="10000" version="1.0"/>
	<!-- 一键装修生成方案副本服务 -->
	<dubbo:service interface="com.nork.onekeydesign.service.DesignPlanTemplateService" ref="designPlanTemplateService" protocol="dubbo" timeout="300000" version="1.0"/>
</beans>
 

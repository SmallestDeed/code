package com.nork.common.async;
import javax.servlet.http.HttpServletRequest;

import com.nork.onekeydesign.model.DesignPlan;

public class AssembleDesignPlanDeletedParameter {
	
	private DesignPlan plan;
	private HttpServletRequest request;
	
	public AssembleDesignPlanDeletedParameter(DesignPlan plan,HttpServletRequest request){
		this.plan = plan;
		this.request = request;
	}

	public DesignPlan getPlan() {
		return plan;
	}

	public void setPlan(DesignPlan plan) {
		this.plan = plan;
	}

	public HttpServletRequest getRequest() {
		return request;
	}

	public void setRequest(HttpServletRequest request) {
		this.request = request;
	}
	
	
}

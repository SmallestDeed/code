package com.sandu.search.entity.elasticsearch.po.metadate;

import java.io.Serializable;

/**
 * 产品平台关联持久化对象
 *
 * @date 20180227
 * @auth pengxuangang
 */
public class ProductPlatformRelPo implements Serializable {

    private static final long serialVersionUID = 4166250950405187516L;

    //产品ID
    private int productId;
    //平台编码
    private String platformCode;
    //平台产品售卖价格
    private Integer platformProductSalePrice;
    //平台产品建议价格
    private Integer platformProductAdvicePrice;
    //平台产品描述
    private String platformProductDesc;
    //平台产品发布状态
    private int platformPutawatStatus;
    //平台产品状态
    private int platformStatus;
    //平台封面图片
    private int platformCoverPicId;
    //平台产品图片列表
    private String platformPicIds;
    
	public int getProductId() {
		return productId;
	}
	
	public void setProductId(int productId) {
		this.productId = productId;
	}
	
	public String getPlatformCode() {
		return platformCode;
	}
	
	public void setPlatformCode(String platformCode) {
		this.platformCode = platformCode;
	}
	
	public Integer getPlatformProductSalePrice() {
		return platformProductSalePrice;
	}
	
	public void setPlatformProductSalePrice(Integer platformProductSalePrice) {
		this.platformProductSalePrice = platformProductSalePrice;
	}
	
	public Integer getPlatformProductAdvicePrice() {
		return platformProductAdvicePrice;
	}
	
	public void setPlatformProductAdvicePrice(Integer platformProductAdvicePrice) {
		this.platformProductAdvicePrice = platformProductAdvicePrice;
	}
	
	public String getPlatformProductDesc() {
		return platformProductDesc;
	}
	
	public void setPlatformProductDesc(String platformProductDesc) {
		this.platformProductDesc = platformProductDesc;
	}
	
	public int getPlatformPutawatStatus() {
		return platformPutawatStatus;
	}
	
	public void setPlatformPutawatStatus(int platformPutawatStatus) {
		this.platformPutawatStatus = platformPutawatStatus;
	}
	
	public int getPlatformStatus() {
		return platformStatus;
	}
	
	public void setPlatformStatus(int platformStatus) {
		this.platformStatus = platformStatus;
	}
	
	public int getPlatformCoverPicId() {
		return platformCoverPicId;
	}
	
	public void setPlatformCoverPicId(int platformCoverPicId) {
		this.platformCoverPicId = platformCoverPicId;
	}
	
	public String getPlatformPicIds() {
		return platformPicIds;
	}
	
	public void setPlatformPicIds(String platformPicIds) {
		this.platformPicIds = platformPicIds;
	}
    
}

package com.nork.onekeydesign.model;

/**
 * 支付方式(渲染方式,价格)
 * @author huangsongbo
 *
 */
public class PlanMode {

	/**数据字典value*/
	private String mode;
	
	private Double price;

	private String name;
	
	/**数据字典type*/
	private String type;
	
	public PlanMode() {
		super();
	}

	public PlanMode(String mode, Double price, String name, String type) {
		super();
		this.mode = mode;
		this.price = price;
		this.name = name;
		this.type = type;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getMode() {
		return mode;
	}

	public void setMode(String mode) {
		this.mode = mode;
	}

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}
	
}

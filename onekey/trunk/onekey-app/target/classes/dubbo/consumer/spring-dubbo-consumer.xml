<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
    <!-- 智能装修（一键装修） -->
    <dubbo:reference interface="com.nork.onekeydesign.service.IntelligenceDecorationService" id="intelligenceDecorationService" version="1.0" retries="0" timeout="300000"/>
    <!-- 样板房服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignTempletService" id="designTempletService" version="1.0"/>
    <!-- 推荐方案服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignPlanRecommendedService" id="designPlanRecommendedService" version="1.0"/>
    <!-- 记录已使用推荐方案服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.TempletPlanOperationHistoryService" id="templetPlanOperationHistoryService" version="1.0"/>
    <!-- 一键自动装修服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.OptimizePlanService" id="optimizePlanService" version="1.0"/>
    <!-- 样板房产品服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignTempletProductService" id="designTempletProductService" version="1.0"/>
    <!-- 设计方案服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignPlanService" id="designPlanService" version="1.0" retries="0" timeout="300000"/>
    <!-- 配置文件资源服务 -->
    <dubbo:reference interface="com.nork.system.service.ResFileService" id="resFileService"  version="1.0"/>
    <!-- 方案资源服务 -->
    <dubbo:reference interface="com.nork.system.service.ResDesignService" id="resDesignService"  version="1.0"/>
    <!-- 推荐方案产品服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignPlanRecommendedProductServiceV2" id="designPlanRecommendedProductServiceV2" version="1.0"/>
    <!-- 一键装修生成方案副本服务 -->
    <dubbo:reference interface="com.nork.onekeydesign.service.DesignPlanTemplateService" id="designPlanTemplateService"  retries="0" version="1.0" timeout="300000"/>
</beans>
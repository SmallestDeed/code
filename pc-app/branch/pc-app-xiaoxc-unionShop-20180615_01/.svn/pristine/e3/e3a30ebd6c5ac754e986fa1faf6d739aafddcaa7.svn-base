package com.nork.design.model.input;

import com.nork.common.model.Mapper;

import java.io.Serializable;

/**
 * Created by kono on 2018/6/5 0005.
 */
public class FavoriteQuery extends Mapper implements Serializable {
    private static final long serialVersionUID = 3253838463231336929L;

    //删除收藏文件夹code
    private String bid;

    //新增收藏夹名称
    private String nm;

    //收藏方案文件夹code
    private String fid;

    //推荐方案ID
    private Integer redId;

    //如果等于1说明 是下拉
    private String select;

    public String getBid() {
        return bid;
    }

    public void setBid(String bid) {
        this.bid = bid;
    }

    public String getNm() {
        return nm;
    }

    public void setNm(String nm) {
        this.nm = nm;
    }

    public String getFid() {
        return fid;
    }

    public void setFid(String fid) {
        this.fid = fid;
    }

    public Integer getRedId() {
        return redId;
    }

    public void setRedId(Integer redId) {
        this.redId = redId;
    }

    public String getSelect() {
        return select;
    }

    public void setSelect(String select) {
        this.select = select;
    }
}

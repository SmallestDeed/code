	   <select id="getUserMenus" resultMap="AllResultMap"
			parameterType="java.lang.Integer">
			select *
			from sys_func
			where id in 
			(
			select DISTINCT d.funcid
			from  sys_user a,sys_user_role b,sys_role c,sys_role_func d
			where a.is_deleted = 0
			and   a.id  = #{userId}
			and   a.id  = b.user_id
			and   b.role_id =  c.id
			and   c.is_deleted = 0
			and   c.id = d.roleid
			)
			and is_menu    = 1
			and is_deleted = 0
			order by longcode 
		</select>	
    
    <!-- 获取登录用户的U3D权限菜单 -->
    <select id="getUserU3DMenus" resultMap="AllResultMap"
			parameterType="java.lang.Integer">
			select *
			from sys_func
			where id in 
			(
			select DISTINCT d.funcid
			from  sys_user a,sys_user_role b,sys_role c,sys_role_func d
			where a.is_deleted = 0
			and   a.id  = #{userId}
			and   a.id  = b.user_id
			and   b.role_id =  c.id
			and   c.is_deleted = 0
			and   c.id = d.roleid
			)
			and is_deleted = 0
			and longcode like '%.0.%'
			and levelid > 0
			ORDER BY parentid ASC,sequence ASC
		</select>	

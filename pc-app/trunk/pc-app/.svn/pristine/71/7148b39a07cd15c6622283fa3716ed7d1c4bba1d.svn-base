	<select id="selectWebMessageList" resultMap="MessageResultMap"	parameterType="com.nork.system.model.UserMessageDesignPlan">
			SELECT
			br.id AS id,
			bm.gmt_create AS gmtCreate,
			bm.business_obj_id AS businessObjId,
			bm.business_type_id AS businessTypeId,
			bm.user_id AS userId,
			su.user_name AS sender,
			bm.content AS content
			FROM base_message bm 
			LEFT JOIN sys_user su ON bm.user_id = su.id and su.is_deleted = 0
			LEFT JOIN base_message_recieve br ON  bm.id = br.message_id 
			LEFT JOIN sys_user u ON br.user_id = u.id and u.is_deleted = 0
			WHERE 1=1 
			<if test="id !=  null">
				and br.id  =#{id,jdbcType=INTEGER}
	    	</if>
			<if test="receiverId !=  null">
				and br.user_id  =#{receiverId,jdbcType=INTEGER}
	    	</if>
			<if test="userId !=  null">
				and bm.user_id  =#{userId,jdbcType=INTEGER}
	    	</if>
			<if test="messageId !=  null  ">
				 and br.message_id  = #{messageId,jdbcType=INTEGER}
			</if>
			<if test="messageType ==  null  ">
				and bm.message_type  = 0
			</if>
			<if test="businessTypeId !=  null  ">
				and bm.business_type_id  = #{businessTypeId,jdbcType=INTEGER}
			</if>
			order by br.gmt_create desc
	</select>

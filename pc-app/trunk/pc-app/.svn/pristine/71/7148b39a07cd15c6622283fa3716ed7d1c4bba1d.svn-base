			order by ${order} <if test="orderNum != null and  orderNum !='' ">${orderNum}</if>
		</if>
		<if test="orders != null and  orders !=''  ">  order by #{orders}</if>
		<if test="order == null and  orders == null ">  order by id desc</if>
		<if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
	</select>
	
	<!-- 查询用户消息所有记录-->
    <select id="selectAllMessage" resultMap="AllResultMap"
		parameterType="com.nork.system.model.search.BaseMessageSearch">
				
		SELECT
			t.*
		FROM
			(
				SELECT
					m.id,
					mr.id as recieve_id,
					mr.is_readed,
					m.message_type,
					m.business_type_id,
					m.business_obj_id,
					m.business_obj_type,
					concat('PC端', m.content) AS content,
					m.gmt_create,
					m.gmt_modified,
					m.numa1
				FROM
					base_message m
					LEFT JOIN base_message_recieve mr on mr.message_id = m.id
				WHERE
					1 = 1
				<if test="userId !=  null  ">
					and m.user_id = #{userId,jdbcType=INTEGER}
				</if>
				AND m.is_deleted = 0
				UNION ALL
					SELECT
						um.id AS id,
						r.id AS recieve_id,
						r.is_readed,
						'' message_type,
						'' business_type_id,
						'' business_obj_id,
						'' business_obj_type,
						concat(
							'移动端&lt;','color','="#01017A"&gt;',
							content,
							'&lt;','/color','&gt;|',
							title
						) AS content,
						um.gmt_create AS gmt_create,
						um.gmt_modified AS gmt_modified,
						um.pic_id AS  numa1
					FROM
						sys_user_message um
						LEFT JOIN base_message_recieve r ON um.id = r.message_id and r.att1 = 1
					WHERE
						1 = 1
					<if test="userId !=  null  ">
						and um.user_id = #{userId,jdbcType=INTEGER}
					</if>
					AND um.platform_id = 1
					AND um.is_deleted = 0
			) t
			ORDER BY
					t.gmt_create desc
		<if test="start !=-1 and limit !=-1"> LIMIT #{start}, #{limit}</if>
	</select>
	
	<!-- 查询用户消息所有条数 -->
	<select id="selectCountAllMessage" resultType="int"
		parameterType="com.nork.system.model.search.BaseMessageSearch">
		SELECT
			count(1)
		FROM
			(
				SELECT
					m.id
				FROM
					base_message m
				WHERE
					1 = 1
				<if test="userId !=  null  ">
					and  m.user_id  = #{userId,jdbcType=INTEGER}
				</if>
				AND m.is_deleted = 0
				UNION ALL
					SELECT
						um.id
					FROM
						sys_user_message um
					WHERE
						1 = 1
					<if test="userId !=  null">
						and  um.user_id  = #{userId,jdbcType=INTEGER}
					</if>
					AND um.platform_id = 1
					AND um.is_deleted = 0
			) t
	</select>
	<!-- 查询用户消息所有条数 -->
	<select id="selectCountUnreaded" resultType="int"
		parameterType="com.nork.system.model.search.BaseMessageSearch">
		
				SELECT
			count(*)
		FROM
			(
				SELECT
					r.id,
					m.user_id,
					m.message_type,
					u.user_name AS sender,
					su.user_name AS receiver,
					m.business_type_id,
					m.business_obj_id,
					m.business_obj_type,
					concat('PC端', m.content) AS content,
					m. STATUS,
					m.sys_code,
					m.creator,
					m.gmt_create,
					m.modifier,
					m.gmt_modified,
					m.is_deleted,
					m.numa1
				FROM
					base_message m
				LEFT JOIN sys_user u ON m.user_id = u.id
				AND u.is_deleted = 0
				LEFT JOIN base_message_recieve r ON m.id = r.message_id
				LEFT JOIN sys_user su ON r.user_id = su.id
				AND su.is_deleted = 0 
				WHERE
					1 = 1
				<if test="userId !=  null  ">			and  m.user_id  = #{userId,jdbcType=INTEGER} </if>	
				AND m.is_deleted = 0
				and r.is_readed = 0
				UNION ALL
					SELECT
						r.id AS id,
						sum.user_id AS user_id,
						'' message_type,
						u.user_name AS sender,
						u.user_name AS receiver,
						'' business_type_id,
						'' business_obj_id,
						'' business_obj_type,
						concat(
							'移动端&lt;','color','="#01017A"&gt;',
							content,
							'&lt;','/color','&gt;|',
							title
						) AS content,
						sum. STATUS AS STATUS,
						sum.sys_code AS sys_code,
						sum.creator AS creator,
						sum.gmt_create AS gmt_create,
						sum.modifier AS modifier,
						sum.gmt_modified AS gmt_modified,
						sum.is_deleted AS is_deleted,
						'' numa1
					FROM
						sys_user_message sum
					LEFT JOIN sys_user u ON sum.user_id = u.id
					AND u.is_deleted = 0
					LEFT JOIN base_message_recieve r ON sum.id = r.message_id
					AND u.is_deleted = 0
					LEFT JOIN sys_user su ON r.user_id = su.id
					AND su.is_deleted = 0 
					WHERE
						1 = 1
					<if test="userId !=  null  ">			and  sum.user_id  = #{userId,jdbcType=INTEGER} </if>
					AND sum.platform_id = 1
					AND sum.is_deleted = 0
					and r.is_readed = 0
			) t
	</select>
    	<!-- **结果定义** -->
	<resultMap id="MessageResultMap" type="com.nork.system.model.UserMessageDesignPlan">
		<id column="id" property="id" jdbcType="INTEGER" />
		<id column="message_id" property="messageId" jdbcType="INTEGER" />
		<id column="is_readed" property="isReaded" jdbcType="INTEGER" />
		<result  column="user_id" property="userId" jdbcType="INTEGER" />
		<result  column="user_id" property="receiverId" jdbcType="INTEGER" />
		<result  column="user_name" property="sender" jdbcType="VARCHAR" />
		<result  column="message_type" property="messageType" jdbcType="INTEGER" />
		<result  column="business_type_id" property="businessTypeId" jdbcType="INTEGER" />
		<result  column="business_obj_id" property="businessObjId" jdbcType="INTEGER" />
		<result  column="content" property="content" jdbcType="VARCHAR" />
		<result  column="user_name" property="receiver" jdbcType="VARCHAR" />
		<result  column="gmt_create" property="gmtCreate" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<!-- 查询后台站内消息列表 -->
	<select id="selectMessageList" resultMap="MessageResultMap"
		parameterType="com.nork.system.model.UserMessageDesignPlan">
		SELECT
		bm.id as id ,
		mr.user_id as userId,
		bm.gmt_create AS gmtCreate,
		su.user_name AS sender,
		u.user_name AS receiver,
		bm.message_type AS messageType,
		bm.business_type_id AS businessTypeId,
		bm.content AS content
		FROM base_message bm
		LEFT JOIN sys_user su ON bm.user_id = su.id and su.is_deleted = 0
		LEFT JOIN base_message_recieve mr ON bm.id = mr.message_id
		LEFT JOIN sys_user u ON mr.user_id = u.id and u.is_deleted = 0
		WHERE 1=1 
		<if test="id !=  null  ">
			and mr.id = #{id,jdbcType=INTEGER}
		</if>
		<if test="messageId !=  null  ">
			and mr.message_id = #{messageId,jdbcType=INTEGER}
		</if>
		<if test="gmtCreate !=  null and tomorrow != null  ">
			and bm.gmt_create between #{gmtCreate,jdbcType=TIMESTAMP} and #{tomorrow,jdbcType=TIMESTAMP}
		 </if>
		<if test="sender !=  null and sender !=  ''   ">
			and su.user_name   like CONCAT(CONCAT('%',#{sender,jdbcType=VARCHAR}),'%')
		</if>
		<if test="receiver !=  null and receiver !=  ''   ">
			and u.user_name  like CONCAT(CONCAT('%',#{receiver,jdbcType=VARCHAR}),'%')
		</if>
		<if test="messageType != null  ">
			and bm.message_type = #{messageType,jdbcType=INTEGER}
	    </if>
		<if test="messageType == null  ">
			and bm.message_type = 0
	    </if>
		<if test="businessTypeId != null  ">
			and bm.business_type_id = #{businessTypeId,jdbcType=INTEGER} 
	    </if>
	</select>

package com.nork.common.exception;

/**
 * 异常状态码
 * 
 * @author huangsongbo
 *
 */
public enum ExceptionCode {

	// design模块 ->start
	
	/**
	 * 参数错误/异常
	 */
	/*public static final Integer Code_10010001 = 10010001;*/
	Code_10010001(10010001, "方法参数错误/异常"),
	
	Code_10010002(10010002, "未找到组合信息")
	;
	
	// design模块 ->end
	
	private Integer code;
	
	private String msg;

	public Integer getCode() {
		return code;
	}

	public void setCode(Integer code) {
		this.code = code;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	private ExceptionCode(Integer code, String msg) {
		this.code = code;
		this.msg = msg;
	}
	
}

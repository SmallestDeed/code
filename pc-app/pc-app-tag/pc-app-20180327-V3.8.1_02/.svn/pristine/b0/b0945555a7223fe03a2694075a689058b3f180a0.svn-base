package com.nork.system.model.po;


import java.io.Serializable;

/**
 * 方案渲染数据持久化
 *
 * @auth xiaoxc
 * @data 2018/7/18
 */
public class ResRenderPicPo implements Serializable {

    private static final long serialVersionUID = -2351366876289688417L;
    public static final Integer EXCEPTION_CODE = 333333;

    //方案主键ID
    private Integer planId;
    //方案渲染图片主键ID
    private Integer resRenderPicId;
    //渲染类型
    private Integer renderingType;
    //渲染图片地址
    private String picPath;
    // 处理下标
    private String index;

    public Integer getPlanId() {
        return planId;
    }

    public void setPlanId(Integer planId) {
        this.planId = planId;
    }

    public Integer getResRenderPicId() {
        return resRenderPicId;
    }

    public void setResRenderPicId(Integer resRenderPicId) {
        this.resRenderPicId = resRenderPicId;
    }

    public Integer getRenderingType() {
        return renderingType;
    }

    public void setRenderingType(Integer renderingType) {
        this.renderingType = renderingType;
    }

    public String getPicPath() {
        return picPath;
    }

    public void setPicPath(String picPath) {
        this.picPath = picPath;
    }

    public String getIndex() {
        return index;
    }

    public void setIndex(String index) {
        this.index = index;
    }
}
